<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">


    <display-name>MARIO OUTLET</display-name>
  
  <servlet>
  		<servlet-name>FilterDataLoader</servlet-name>
		<servlet-class>com.gauce.servlet.FilterDataLoader</servlet-class>
		<init-param>
			<param-name>charset</param-name>
			<param-value>euc-kr</param-value>
		</init-param>
  	</servlet>
 <servlet>
  		<servlet-name>FilterChecker</servlet-name>
		<servlet-class>com.gauce.servlet.FilterChecker</servlet-class>
  	</servlet>
 <servlet>
		<servlet-name>ServiceManager</servlet-name>
		<servlet-class>com.shift.utils.servlet.ServiceManager</servlet-class>
		<init-param>
			<param-name>charset</param-name>
			<param-value>euc-kr</param-value>
		</init-param>
	</servlet>
  <servlet>
        <servlet-name>StartUpServlet</servlet-name>
        <servlet-class>kr.fujitsu.ffw.base.StartUpServlet</servlet-class>
	    <init-param>
		    <param-name>relative-path</param-name>
		    <param-value>true</param-value>
	    </init-param>
	    <init-param>
		    <param-name>global.config</param-name>
		    <param-value>WEB-INF/global.config.xml</param-value>
	    </init-param>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>DAServlet</servlet-name>
        <servlet-class>common.web.ExtendedActionServlet</servlet-class>
        <init-param>
            <param-name>relative-path</param-name>
            <param-value>true</param-value>
        </init-param>
        <init-param>
            <param-name>config</param-name>
            <param-value>WEB-INF/xml/dacc_config.xml</param-value>
        </init-param>
        <init-param>
            <param-name>container-name</param-name>
            <param-value>engine1</param-value>
        </init-param>
        <init-param>
            <param-name>encoding</param-name>
            <param-value>UTF-8</param-value>
        </init-param>
        <init-param>
            <param-name>process-debug</param-name>
            <param-value>true</param-value>
        </init-param>
    </servlet>    

