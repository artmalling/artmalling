  <servlet-mapping>
		<servlet-name>FilterDataLoader</servlet-name>
		<url-pattern>/monitor/FilterDataLoader</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
		<servlet-name>FilterChecker</servlet-name>
		<url-pattern>/monitor/FilterChecker</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
		<servlet-name>ServiceManager</servlet-name>
		<url-pattern>/ServiceManager</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
        <servlet-name>StartUpServlet</servlet-name>
        <url-pattern>*.start</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DMServlet</servlet-name>
        <url-pattern>*.dm</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DOServlet</servlet-name>
        <url-pattern>*.do</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DCServlet</servlet-name>
        <url-pattern>*.dc</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DBServlet</servlet-name>
        <url-pattern>*.db</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CCServlet</servlet-name>
        <url-pattern>*.cc</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>DAServlet</servlet-name>
        <url-pattern>*.da</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>DVServlet</servlet-name>
        <url-pattern>*.dv</url-pattern>
    </servlet-mapping>
    
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
  
  <filter>
		<filter-name>GauceFilter</filter-name>
		<filter-class>com.gauce.filter.GauceFilter</filter-class>
		<init-param>
			<param-name>conf-reload</param-name>
			<param-value>5000</param-value>
		</init-param>
	</filter>
  
  <filter-mapping>
		<filter-name>GauceFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
  
  <mime-mapping>
		<extension>gfm</extension>
		<mime-type>application/x-gforms-xml</mime-type>
	</mime-mapping>
  </web-app>