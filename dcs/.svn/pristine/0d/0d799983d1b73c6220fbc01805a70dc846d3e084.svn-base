                                        , MOBILE_PH2
                                        , MOBILE_PH3  
                                        , RECV_MSG
                                        , REG_DT
                                        , REG_ID
                                         )
                                  VALUES (
                                          ?
                                        , ?
                                        , ?    
                                        , ?
                                        , ?  
                                        , DCS.F_ENCRYPTEDSTR_M(?) 
                                        , DCS.F_ENCRYPTEDSTR_M(?)
                                        , DCS.F_ENCRYPTEDSTR_M(?)  
                                        , ?   
                                        , TO_CHAR(SYSDATE, 'YYYYMMDDHH24MISS')
                                        ,?
                                        )
        ]]>
    </query>
      
