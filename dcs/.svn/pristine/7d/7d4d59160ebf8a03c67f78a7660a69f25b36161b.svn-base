    <!--  회원정보 조회.  -->
    <query name="SEL_CUST">
        <![CDATA[
                        
            SELECT A.CUST_ID
                 , A.HHOLD_ID
                 , A.COMP_PERS_FLAG
                 , DCS.SC_Crypto_FUN('DEC', A.SS_NO) AS SS_NO
                 , A.CUST_NAME
                 , DCS.SC_CRYPTO_FUN('DEC',A.CARD_PWD_NO) AS CARD_PWD_NO
                 --, DECODE(A.HOME_NEW_YN, 'Y',A.HNEW_ZIP_CD1,A.HOME_ZIP_CD1)  HOME_ZIP_CD1
                 --, DECODE(A.HOME_NEW_YN, 'Y',A.HNEW_ZIP_CD2,A.HOME_ZIP_CD2)  HOME_ZIP_CD2
                 --, DECODE(A.HOME_NEW_YN, 'Y',A.HNEW_ADDR1,  A.HOME_ADDR1)    HOME_ADDR1
                 --, DECODE(A.HOME_NEW_YN, 'Y',A.HNEW_ADDR2,  A.HOME_ADDR2)    HOME_ADDR2
                 , A.HNEW_ZIP_CD1
                 , A.HNEW_ZIP_CD2
                 , A.HNEW_ADDR1
                 , A.HNEW_ADDR2
                 , A.HOME_ZIP_CD1
                 , A.HOME_ZIP_CD2
                 , A.HOME_ADDR1
                 , A.HOME_ADDR2
                 , A.NOCLS_REQ_YN
                 , A.HOME_CLS_YN
                 , DCS.SC_Crypto_FUN('DEC', A.HOME_PH1) AS HOME_PH1
                 , DCS.SC_Crypto_FUN('DEC', A.HOME_PH2) AS HOME_PH2
                 , DCS.SC_Crypto_FUN('DEC', A.HOME_PH3) AS HOME_PH3
                 , DCS.SC_Crypto_FUN('DEC', A.MOBILE_PH1) AS MOBILE_PH1
                 , DCS.SC_Crypto_FUN('DEC', A.MOBILE_PH2) AS MOBILE_PH2
                 , DCS.SC_Crypto_FUN('DEC', A.MOBILE_PH3) AS MOBILE_PH3
                 , DCS.SC_Crypto_FUN('DEC', A.EMAIL1) AS EMAIL1
                 , DCS.SC_Crypto_FUN('DEC', A.EMAIL2) AS EMAIL2
                 , DCS.SC_Crypto_FUN('DEC', A.EMAIL2) AS EMAIL3
                 , A.EMAIL_YN
                 , A.EMAIL_AGREE_DT
                 , A.WED_YN
                 , A.WED_DT
                 , A.BIRTH_DT
                 , A.LUNAR_FLAG
                 , A.SMS_YN
                 , A.SMS_AGREE_DT
                 , A.POST_RCV_CD
                 , A.OFFI_NM
                 , A.OFFI_PH1
                 , A.OFFI_PH2
                 , A.OFFI_PH3
                 , A.OFFI_FAX1
                 , A.OFFI_FAX2
                 , A.OFFI_FAX3
                 , A.OFFI_INTER_PH
                 , DECODE(A.OFFI_NEW_YN, 'Y',A.ONEW_ZIP_CD1,A.OFFI_ZIP_CD1)  OFFI_ZIP_CD1
                 , DECODE(A.OFFI_NEW_YN, 'Y',A.ONEW_ZIP_CD2,A.OFFI_ZIP_CD2)  OFFI_ZIP_CD2
                 , DECODE(A.OFFI_NEW_YN, 'Y',A.ONEW_ADDR1,  A.OFFI_ADDR1)    OFFI_ADDR1
                 , DECODE(A.OFFI_NEW_YN, 'Y',A.ONEW_ADDR2,  A.OFFI_ADDR2)    OFFI_ADDR2
                 , A.OFFI_CLS_YN
                 , A.SCOMP_EMP_YN
                 , A.CHILD_CNT
                 , A.WEB_USER_ID
                 , A.WEB_PASSWD
                 , A.NAME_CONF_YN
                 , A.I_PIN
                 , '' ENTR_PATH
                 , A.ENTR_DT
                 , A.SCED_REQ_DT
                 , A.SCED_TAKE_FLAG
                 , A.SCED_PROC_DT
                 , A.ALIEN_YN
                 , A.CUST_STAT_FLAG
                 --, '' CARD_NO
                 , (SELECT DCS.SC_Crypto_FUN('DEC', B.CARD_NO)
                      FROM DCS.DM_CARD B 
                     WHERE A.CUST_ID = B.CUST_ID 
                     AND B.ISSUE_DT = ( SELECT MAX(C.ISSUE_DT)
                                          FROM DCS.DM_CARD C
                                         WHERE C.CUST_ID = A.CUST_ID 
                                        GROUP BY C.CUST_ID )) AS CARD_NO 
                 , '' HORI_ADDR1
                 , '' HORI_ADDR2
                 , '' OORI_ADDR1
                 , '' OORI_ADDR2
                 , '' CARD_TYPE_CD
                 , 'U' CHKSAVE
                 , A.HOUSE_TYPE
                 , A.HOUSE_FLAG
                 , A.HOLD_CAR_YN
                 , A.FAVOR_DEPT1_YN
                 , A.FAVOR_DEPT2_YN
                 , A.FAVOR_DEPT3_YN
                 , A.FAVOR_DEPT4_YN
                 , A.FAVOR_DEPT5_YN
                 , A.INCOME_AMT
                 , A.FAMILY_CNT
                 , A.DEPT_NAME
                 , A.POSITION
                 , A.POSITION_ETC
                 , A.AGENT_NAME
                 , A.AGENT_SSNO
                 , A.AGENT_RELATION
                 , A.AGENT_PH1
                 , A.AGENT_PH2
                 , A.AGENT_PH3
                 , A.LEGAL_AGENT
                 , A.OFF_TERMS1_YN
                 , A.OFF_TERMS1_DT 
                 , A.OFF_TERMS1_NAME 
                 , A.OFF_TERMS2_YN
                 , A.OFF_TERMS2_DT   
                 , A.OFF_TERMS2_NAME  
                 , A.OFF_TERMS3_YN
                 , A.OFF_TERMS3_DT 
                 , A.OFF_TERMS3_NAME 
                 , A.OFF_TERMS4_YN
                 , A.OFF_TERMS4_DT   
                 , A.OFF_TERMS4_NAME       
                 , A.ID_CHECK
                 , A.ISSUE_ORG
                 , A.ISSUE_DT
                 , A.HOME_NEW_YN
                 , A.OFFI_NEW_YN
                 , DPS.FN_GET_PUMBUN_NAME(A.ENTR_PBN) AS ENTR_PBN_NAME                                  
                 --, (
                 --    SELECT DECODE(COUNT(B.CUST_GRADE),0,'N', MAX(B.CUST_GRADE)) CUST_GRADE
                 --      FROM DCS.DO_CUST_GRADE B
                 --     WHERE B.BRCH_ID  = X
                 --       AND B.APP_YEAR = TO_CHAR(SYSDATE,'YYYY')
                 --       AND B.CUST_ID  = D.CUST_ID
                 --  ) CUST_GRADE        
                 , (
                     SELECT DECODE(COUNT(B.CUST_GRADE),0,'N', MAX(B.CUST_GRADE)) CUST_GRADE
                       FROM DCS.V_DO_CUST_GRADE_CURR B
                      WHERE B.BRCH_ID  = ?
                        AND B.CUST_ID  = D.CUST_ID
                   ) AS CUST_GRADE                  
                 , (
                     SELECT DECODE(COUNT(C.VIP_FLAG),0,'', MAX(C.VIP_FLAG)) VIP_FLAG
                       FROM DCS.DO_VIP_REASON C
                      WHERE C.CUST_ID  = D.CUST_ID
                   ) VIP_FLAG
                   , '' AS POCARD_PREFIX
                 , A.ENTR_PBN                  
                 --, SUBSTR( (SELECT MAX(B.APP_DT||B.CUST_TYPE)
                 --             FROM DCS.DO_CUST_TYPE B
                 --            WHERE B.CUST_ID = D.CUST_ID
                 --              AND B.BRCH_ID = X
                 --           )
                 --         ,9,2) AS CUST_TYPE
                 , (SELECT MAX(B.CUST_TYPE)
                              FROM DCS.V_DO_CUST_TYPE_CURR B
                             WHERE B.CUST_ID = D.CUST_ID
                               AND B.BRCH_ID = ?
                            ) AS CUST_TYPE
                 , SEX_CD           
                 , (
                 	 SELECT SUM(ISSUE_CNT)
                       FROM DCS.DM_CARD
                      WHERE CUST_ID = D.CUST_ID
                        AND CARD_NO LIKE DCS.SC_Crypto_FUN('ENC', ?) ||'%'
                   ) AS ISSUE_CNT  
				   , A.TM_YN
				   , A.MOBILE_COMP
				   , A.DI
				   , A.CI
				   , '' AS ENTR_CH
                   , TO_CHAR(NVL(A.REG_DATE,'19000101'),'YYYY-MM-DD HH24:MI:SS')||'-'||NVL((SELECT USER_NAME FROM COM.TC_USRMST WHERE USER_ID = A.REG_ID),'알수없음') REG_COL
                   , TO_CHAR(NVL(A.MOD_DATE,'19000101'),'YYYY-MM-DD HH24:MI:SS')||'-'||NVL((SELECT USER_NAME FROM COM.TC_USRMST WHERE USER_ID = A.MOD_ID),'알수없음') MOD_COL      
                   , ON_TERMS5_DATE
                   , A.STR_CD            
                   , A.ETC_MEMO       
                   , B.CARD_GRADE AS CARD_GRADE
              FROM DCS.DM_CUSTOMER A
                 , DCS.DM_CARD B 
                 , (
                     SELECT max(A.CUST_ID) CUST_ID
                       FROM DCS.DM_CARD A
                          , DCS.DM_CUSTOMER B
                      WHERE A.CUST_ID  =  B.CUST_ID
        ]]>        
    </query> 
    <query name="SEL_CUST_CARD_NO">         
        <![CDATA[    
            AND A.CARD_NO  = DCS.F_GET_POCARD_NO(DCS.SC_Crypto_FUN('ENC', ?))    
        ]]>                               
    </query>  
    
    <query name="SEL_CUST_SS_NO">         
        <![CDATA[    
            AND (B.SS_NO = DCS.SC_Crypto_FUN('ENC', ?)
                AND DCS.SC_Crypto_FUN('ENC', LENGTHB(DCS.F_DECRYPTEDSTR_M(?))) = 13)
             OR (A.CUST_ID  =  B.CUST_ID
                AND DCS.SC_Crypto_FUN('ENC', LENGTHB(DCS.F_DECRYPTEDSTR_M(?))) <> 13
		    	AND B.SS_NO = DCS.SC_Crypto_FUN('ENC', ?) 
            	AND B.MOBILE_PH1 = DCS.SC_Crypto_FUN('ENC', ?)     
            	AND B.MOBILE_PH2 = DCS.SC_Crypto_FUN('ENC', ?)    
            	AND B.MOBILE_PH3 = DCS.SC_Crypto_FUN('ENC', ?))             	            
        ]]>                               
    </query>
    
    <query name="SEL_CUST_END">         
        <![CDATA[    
            ) D
             WHERE A.CUST_ID = D.CUST_ID
               AND A.CUST_ID = B.CUST_ID
        ]]>                               
    </query>
    
       
