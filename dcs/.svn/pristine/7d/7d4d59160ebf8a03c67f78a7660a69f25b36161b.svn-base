    <!-- 카드 정보 등록 -->
    <query name="INS_CARDNO_UPD">         
        <![CDATA[    
			UPDATE DCS.DM_CARD
			   SET (CUST_ID            
			      , PWD_NO             
			      , CARD_STAT_CD       
			      , VALID_YN           
			      , REG_INFORM_DT      
			      , PWD_NO_ERR_CNT     
			      , ISSUE_DT           
			      , FIRST_TRADE_DT     
			      , LOSS_NOTI_DT       
			      , LOSS_NOTI_NM       
			      , ENTR_PATH_CD       
			      , SCED_REQ_DT        
			      , SCED_PROC_DT       
			      , ONLINE_ISSUE_YN    
			      , ONLINE_ISSUE_DT    
			      , MOD_DATE           
			      , MOD_ID             
			      , POCARD_PREFIX      
			      , CCOMP_CD           
			      , ENTR_PBN           
			      , ISSUE_CNT) =  (SELECT CUST_ID
			                            , PWD_NO 
			                            , '0' AS CARD_STAT_CD 
			                            , 'Y' AS VALID_YN 
			                            , NULL AS REG_INFORM_DT
			                            , 0 AS PWD_NO_ERR_CNT
			                            , TO_CHAR(SYSDATE, 'YYYYMMDD') AS ISSUE_DT
			                            , NULL AS FIRST_TRADE_DT
			                            , NULL AS LOSS_NOTI_DT
			                            , NULL AS LOSS_NOTI_NM
			                            , ENTR_PATH_CD
			                            , NULL AS SCED_REQ_DT 
			                            , NULL AS SCED_PROC_DT
			                            , ONLINE_ISSUE_YN
			                            , NULL AS ONLINE_ISSUE_DT
			                            , SYSDATE AS MOD_DATE 
			                            , ?  AS MOD_ID  
			                            , POCARD_PREFIX 
			                            , CCOMP_CD 
			                            , ENTR_PBN
			                            , ISSUE_CNT 
			                         FROM DCS.DM_CARD
			                        WHERE CARD_NO = DCS.SC_Crypto_FUN('ENC', ?)) 
			    WHERE CARD_NO = DCS.SC_Crypto_FUN('ENC', ?)
			    
        ]]>        
    </query>  
     
