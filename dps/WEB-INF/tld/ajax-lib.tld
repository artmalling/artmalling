<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE taglib
  PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
  "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
    <tlib-version>1.0</tlib-version>
    <jsp-version>1.1</jsp-version>
    <short-name>ajax-lib</short-name>
    <uri/>
    <display-name/>
    <small-icon/>
    <large-icon/>    
    <description>
        Fujitsu Pattern-Java Framework Ajax Supports CustomTag Library.
        written by oshirou Takahashi(http://jsgt.org/mt/01/)
    </description>
    <tag>
        <name>library</name>
        <tag-class>kr.fujitsu.ffw.view.ajax.tag.AjaxLib</tag-class>
        <body-content>empty</body-content>
    </tag>
    <tag>
        <name>callback</name>
        <tag-class>kr.fujitsu.ffw.view.ajax.tag.AjaxCallBack</tag-class>
        <body-content>JSP</body-content>
        <attribute>
        	<name>function</name>
        	<required>true</required>
        	<rtexprvalue>false</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>open</name>
        <tag-class>kr.fujitsu.ffw.view.ajax.tag.AjaxResponse</tag-class>
        <body-content>empty</body-content>
        <attribute>
        	<name>callback</name>
        	<required>true</required>
        	<rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
        	<name>param</name>
        	<required>false</required>
        	<rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
        	<name>paramvalue</name>
        	<required>false</required>
        	<rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
        	<name>method</name>
        	<required>true</required>
        	<rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
        	<name>url</name>
        	<required>false</required>
        	<rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
        	<name>urlvalue</name>
        	<required>false</required>
        	<rtexprvalue>false</rtexprvalue>
        </attribute>

        <attribute>
        	<name>async</name>
        	<required>false</required>
        	<rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
        	<name>sload</name>
        	<required>false</required>
        	<rtexprvalue>false</rtexprvalue>
        </attribute>
    </tag>
</taglib>