<?xml version="1.0" encoding="euc-kr"?>
<!-- XML 버전 1.0을 사용하며 encoding 타입은 ‘euc-kr’로 한다.-->
<!--
 * 작 성 자 : 박종은
 * 회 사 명 : 한국후지쯔
 * 작 성 일 : 2010.04.08
 * 변 경 일 : 
 * 변경내용 : 
 * 개    요 :  
 -->
<service name="/psal214.ps">

	<!-- 점별 파트별 헤더 -->
    <gauce name="H_SEL_MASTER"> 
		<column colName="STR_CD"               colType="TB_STRING"   size="2"   dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="TEAM_CD"              colType="TB_STRING"   size="2"   dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="PART_CD"              colType="TB_STRING"   size="2"   dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="PART_NM"              colType="TB_STRING"   size="50"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="PC_CD"                colType="TB_STRING"   size="2"   dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="PC_NM"                colType="TB_STRING"   size="50"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="PUMBUN_CD"            colType="TB_STRING"   size="6"   dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="PUMBUN_NAME"          colType="TB_STRING"   size="50"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="TOT_SALE_AMT_ONLINE"  colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="SALE_QTY_ONLINE"      colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="VAT_AMT_ONLINE"       colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="REDU_AMT_ONLINE"      colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="NORM_SALE_AMT_ONLINE" colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="DC_AMT_ONLINE"        colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="NET_SALE_AMT_ONLINE"  colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="SALE_PROF_AMT_ONLINE" colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="CUST_CNT_ONLINE"      colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="CUST_DANGA_ONLINE"    colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="PROF_RATE_ONLINE"     colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="TOT_SALE_AMT_NORMAL"  colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="SALE_QTY_NORMAL"      colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="VAT_AMT_NORMAL"       colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="REDU_AMT_NORMAL"      colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="NORM_SALE_AMT_NORMAL" colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="DC_AMT_NORMAL"        colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="NET_SALE_AMT_NORMAL"  colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="SALE_PROF_AMT_NORMAL" colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="CUST_CNT_NORMAL"      colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="CUST_DANGA_NORMAL"    colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="PROF_RATE_NORMAL"     colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="TOT_SALE_AMT"         colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="SALE_QTY"             colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="VAT_AMT"              colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="REDU_AMT"             colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="NORM_SALE_AMT"        colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="DC_AMT"               colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="NET_SALE_AMT"         colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="SALE_PROF_AMT"        colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="CUST_CNT"             colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="CUST_DANGA"           colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
		<column colName="PROF_RATE"            colType="TB_DECIMAL"  size="13"  dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
    </gauce>
    
    <!-- 점별 파트별 -->
    <query name="SEL_MASTER">        
        <![CDATA[
			SELECT A.STR_CD
			     , A.TEAM_CD                    /* 팀코드            */     
			     , A.PART_CD                    /* 파트코드          */
			     , A.PART_NM                    /* 파트명            */     
			     , A.PC_CD                      /* PC코드            */
			     , A.PC_NM                      /* PC명              */
			     , A.PUMBUN_CD
			     , A.PUMBUN_NAME
			     , SUM(A.TOT_SALE_AMT_ONLINE)   / ? AS TOT_SALE_AMT_ONLINE     /* 온라인 총매출금액 */
			     , SUM(A.SALE_QTY_ONLINE)       / ? AS SALE_QTY_ONLINE         /* 온라인 판매수량   */
			     , SUM(A.VAT_AMT_ONLINE)        / ? AS VAT_AMT_ONLINE          /* 온라인 부가세     */
			     , SUM(A.REDU_AMT_ONLINE)       / ? AS REDU_AMT_ONLINE         /* 온라인 할인금액   */
			     , SUM(A.NORM_SALE_AMT_ONLINE)  / ? AS NORM_SALE_AMT_ONLINE    /* 온라인 매출금액   */
			     , SUM(A.DC_AMT_ONLINE)         / ? AS DC_AMT_ONLINE           /* 온라인 에누리금액 */
			     , SUM(A.NET_SALE_AMT_ONLINE)   / ? AS NET_SALE_AMT_ONLINE     /* 온라인 순매출금액 */
			     , SUM(A.SALE_PROF_AMT_ONLINE)  / ? AS SALE_PROF_AMT_ONLINE    /* 온라인 매출이익액 */
			     , SUM(A.CUST_CNT_ONLINE)           AS CUST_CNT_ONLINE         /* 온라인 객수       */
			     , DECODE(SUM(A.CUST_CNT_ONLINE), 0, 0
			            , ROUND(SUM(A.TOT_SALE_AMT_ONLINE) 
			                  / SUM(A.CUST_CNT_ONLINE)
			                  , 2))             / ? AS CUST_DANGA_ONLINE       /* 온라인 객단가     */
			     , DECODE(SUM(A.NORM_SALE_AMT_VAT_ONLINE), 0, 0
			            , ROUND(SUM(A.SALE_PROF_AMT_ONLINE) 
			                  / ROUND(SUM(A.NORM_SALE_AMT_VAT_ONLINE), 0)
			                  * 100, 2))            AS PROF_RATE_ONLINE        /* 온라인 매출이익율 */
			     , SUM(A.TOT_SALE_AMT_NORMAL)   / ? AS TOT_SALE_AMT_NORMAL     /* 정상 총매출금액   */
			     , SUM(A.SALE_QTY_NORMAL)       / ? AS SALE_QTY_NORMAL         /* 정상 판매수량     */
			     , SUM(A.VAT_AMT_NORMAL)        / ? AS VAT_AMT_NORMAL          /* 정상 부가세       */
			     , SUM(A.REDU_AMT_NORMAL)       / ? AS REDU_AMT_NORMAL         /* 정상 할인금액     */
			     , SUM(A.NORM_SALE_AMT_NORMAL)  / ? AS NORM_SALE_AMT_NORMAL    /* 정상 매출금액     */
			     , SUM(A.DC_AMT_NORMAL)         / ? AS DC_AMT_NORMAL           /* 정상 에누리금액   */
			     , SUM(A.NET_SALE_AMT_NORMAL)   / ? AS NET_SALE_AMT_NORMAL     /* 정상 순매출금액   */
			     , SUM(A.SALE_PROF_AMT_NORMAL)  / ? AS SALE_PROF_AMT_NORMAL    /* 정상 매출이익액   */
			     , SUM(A.CUST_CNT_NORMAL)           AS CUST_CNT_NORMAL         /* 정상 객수         */
			     , DECODE(SUM(A.CUST_CNT_NORMAL), 0, 0
			            , ROUND(SUM(A.TOT_SALE_AMT_NORMAL) 
			                  / SUM(A.CUST_CNT_NORMAL)
			                  , 2))             / ? AS CUST_DANGA_NORMAL       /* 정상 객단가       */
			     , DECODE(SUM(A.NORM_SALE_AMT_VAT_NORMAL), 0, 0
			            , ROUND(SUM(A.SALE_PROF_AMT_NORMAL) 
			                  / ROUND(SUM(A.NORM_SALE_AMT_VAT_NORMAL), 0)
			                  * 100, 2))            AS PROF_RATE_NORMAL        /* 정상 매출이익율   */
			     , SUM(A.TOT_SALE_AMT)          / ? AS TOT_SALE_AMT            /* 합계 총매출금액   */
			     , SUM(A.SALE_QTY)              / ? AS SALE_QTY                /* 합계 판매수량     */
			     , SUM(A.VAT_AMT)               / ? AS VAT_AMT                 /* 합계 부가세       */
			     , SUM(A.REDU_AMT)              / ? AS REDU_AMT                /* 합계 할인금액     */
			     , SUM(A.NORM_SALE_AMT)         / ? AS NORM_SALE_AMT           /* 합계 매출금액     */
			     , SUM(A.DC_AMT)                / ? AS DC_AMT                  /* 합계 에누리금액   */
			     , SUM(A.NET_SALE_AMT)          / ? AS NET_SALE_AMT            /* 합계 순매출금액   */
			     , SUM(A.SALE_PROF_AMT)         / ? AS SALE_PROF_AMT           /* 합계 매출이익액   */
			     , SUM(A.CUST_CNT)                  AS CUST_CNT                /* 합계 객수         */
			     , DECODE(SUM(A.CUST_CNT), 0, 0
			            , ROUND(SUM(A.TOT_SALE_AMT) 
			                  / SUM(A.CUST_CNT)
			                  , 2))             / ? AS CUST_DANGA              /* 합계 객단가       */
			     , DECODE(SUM(A.NORM_SALE_AMT_VAT), 0, 0
			            , ROUND(SUM(A.SALE_PROF_AMT) 
			                  / ROUND(SUM(A.NORM_SALE_AMT_VAT), 0)
			                  * 100, 2))            AS PROF_RATE               /* 합계 매출이익율   */
			  FROM (
			        SELECT A.STR_CD
			             , A.ORG_CD                                    /* 조직코드          */
			             , SUBSTR(A.ORG_CD, 3, 2) AS TEAM_CD           /* 팀코드            */
			             , SUBSTR(A.ORG_CD, 5, 2) AS PART_CD           /* 파트코드          */
			             , C.ORG_NAME             AS PART_NM           /* 파트명            */
			             , SUBSTR(A.ORG_CD, 7, 2) AS PC_CD             /* PC코드            */
			             , D.ORG_NAME             AS PC_NM             /* PC명              */
			             , SUBSTR(A.ORG_CD, 9, 2) AS CORNER_CD         /* 코너코드          */
			             , E.ORG_NAME             AS CORNER_NM         /* 코너명            */
			             , A.PUMBUN_CD                                 /* 브랜드 코드       */
			             , B.PUMBUN_NAME                               /* 브랜드 명         */
			             , A.SALE_QTY_ONLINE                           /* 온라인 판매수량   */
			             , A.TOT_SALE_AMT_ONLINE                       /* 온라인 총매출금액 */
			             , A.VAT_AMT_ONLINE                            /* 온라인 부가세     */
			             , A.REDU_AMT_ONLINE                           /* 온라인 할인금액   */
			             , A.NORM_SALE_AMT_ONLINE                      /* 온라인 매출금액   */
			             , A.DC_AMT_ONLINE                             /* 온라인 에누리금액 */
			             , A.NET_SALE_AMT_ONLINE                       /* 온라인 순매출금액 */
			             , A.SALE_PROF_AMT_ONLINE                      /* 온라인 매출이익액 */
			             , A.CUST_CNT_ONLINE                           /* 온라인 객수       */
			             , A.NORM_SALE_AMT_VAT_ONLINE                  /* 온라인            */
			             , A.SALE_QTY_NORMAL                           /* 정상 판매수량     */
			             , A.TOT_SALE_AMT_NORMAL                       /* 정상 총매출금액   */
			             , A.VAT_AMT_NORMAL                            /* 정상 부가세       */
			             , A.REDU_AMT_NORMAL                           /* 정상 할인금액     */
			             , A.NORM_SALE_AMT_NORMAL                      /* 정상 매출금액     */
			             , A.DC_AMT_NORMAL                             /* 정상 에누리금액   */
			             , A.NET_SALE_AMT_NORMAL                       /* 정상 순매출금액   */
			             , A.SALE_PROF_AMT_NORMAL                      /* 정상 매출이익액   */
			             , A.CUST_CNT_NORMAL                           /* 정상 객수         */
			             , A.NORM_SALE_AMT_VAT_NORMAL                  /* 정상              */
			             , A.SALE_QTY                                  /* 합계 판매수량     */
			             , A.TOT_SALE_AMT                              /* 합계 총매출금액   */
			             , A.VAT_AMT                                   /* 합계 부가세       */
			             , A.REDU_AMT                                  /* 합계 할인금액     */
			             , A.NORM_SALE_AMT                             /* 합계 매출금액     */
			             , A.DC_AMT                                    /* 합계 에누리금액   */
			             , A.NET_SALE_AMT                              /* 합계 순매출금액   */
			             , A.SALE_PROF_AMT                             /* 합계 매출이익액   */
			             , A.CUST_CNT                                  /* 합계 객수         */
			             , A.NORM_SALE_AMT_VAT                         /* 합계              */
			          FROM (
			                SELECT A.STR_CD
			                     , A.SALE_DT
			                     , A.PUMBUN_CD
			                     , A.PUMMOK_CD
			                     , A.EVENT_CD
			                     , A.EVENT_FLAG
			                     , A.EVENT_RATE
			                     , A.ORG_CD
			                     , CASE WHEN A.MG_FLAG =  '6' THEN A.SALE_QTY          ELSE 0 END AS SALE_QTY_ONLINE             /* 온라인 판매수량   */
			                     , CASE WHEN A.MG_FLAG =  '6' THEN A.TOT_SALE_AMT      ELSE 0 END AS TOT_SALE_AMT_ONLINE         /* 온라인 총매출금액 */
			                     , CASE WHEN A.MG_FLAG =  '6' THEN A.VAT_AMT           ELSE 0 END AS VAT_AMT_ONLINE              /* 온라인 부가세     */
			                     , CASE WHEN A.MG_FLAG =  '6' THEN A.REDU_AMT          ELSE 0 END AS REDU_AMT_ONLINE             /* 온라인 할인금액   */
			                     , CASE WHEN A.MG_FLAG =  '6' THEN A.NORM_SALE_AMT     ELSE 0 END AS NORM_SALE_AMT_ONLINE        /* 온라인 매출금액   */
			                     , CASE WHEN A.MG_FLAG =  '6' THEN A.DC_AMT            ELSE 0 END AS DC_AMT_ONLINE               /* 온라인 에누리금액 */
			                     , CASE WHEN A.MG_FLAG =  '6' THEN A.NET_SALE_AMT      ELSE 0 END AS NET_SALE_AMT_ONLINE         /* 온라인 순매출금액 */
			                     , CASE WHEN A.MG_FLAG =  '6' THEN A.SALE_PROF_AMT     ELSE 0 END AS SALE_PROF_AMT_ONLINE        /* 온라인 매출이익액 */
			                     , CASE WHEN A.MG_FLAG =  '6' THEN A.CUST_CNT          ELSE 0 END AS CUST_CNT_ONLINE             /* 온라인 객수       */
			                     , CASE WHEN A.MG_FLAG =  '6' THEN A.NORM_SALE_AMT_VAT ELSE 0 END AS NORM_SALE_AMT_VAT_ONLINE    /* 온라인            */
			                     , CASE WHEN A.MG_FLAG <> '6' THEN A.SALE_QTY          ELSE 0 END AS SALE_QTY_NORMAL             /* 정상 판매수량     */
			                     , CASE WHEN A.MG_FLAG <> '6' THEN A.TOT_SALE_AMT      ELSE 0 END AS TOT_SALE_AMT_NORMAL         /* 정상 총매출금액   */
			                     , CASE WHEN A.MG_FLAG <> '6' THEN A.VAT_AMT           ELSE 0 END AS VAT_AMT_NORMAL              /* 정상 부가세       */
			                     , CASE WHEN A.MG_FLAG <> '6' THEN A.REDU_AMT          ELSE 0 END AS REDU_AMT_NORMAL             /* 정상 할인금액     */
			                     , CASE WHEN A.MG_FLAG <> '6' THEN A.NORM_SALE_AMT     ELSE 0 END AS NORM_SALE_AMT_NORMAL        /* 정상 매출금액     */
			                     , CASE WHEN A.MG_FLAG <> '6' THEN A.DC_AMT            ELSE 0 END AS DC_AMT_NORMAL               /* 정상 에누리금액   */
			                     , CASE WHEN A.MG_FLAG <> '6' THEN A.NET_SALE_AMT      ELSE 0 END AS NET_SALE_AMT_NORMAL         /* 정상 순매출금액   */
			                     , CASE WHEN A.MG_FLAG <> '6' THEN A.SALE_PROF_AMT     ELSE 0 END AS SALE_PROF_AMT_NORMAL        /* 정상 매출이익액   */
			                     , CASE WHEN A.MG_FLAG <> '6' THEN A.CUST_CNT          ELSE 0 END AS CUST_CNT_NORMAL             /* 정상 객수         */
			                     , CASE WHEN A.MG_FLAG <> '6' THEN A.NORM_SALE_AMT_VAT ELSE 0 END AS NORM_SALE_AMT_VAT_NORMAL    /* 정상              */
			                     , A.SALE_QTY                                                     AS SALE_QTY                    /* 합계 판매수량     */
			                     , A.TOT_SALE_AMT                                                 AS TOT_SALE_AMT                /* 합계 총매출금액   */
			                     , A.VAT_AMT                                                      AS VAT_AMT                     /* 합계 부가세       */
			                     , A.REDU_AMT                                                     AS REDU_AMT                    /* 합계 할인금액     */
			                     , A.NORM_SALE_AMT                                                AS NORM_SALE_AMT               /* 합계 매출금액     */
			                     , A.DC_AMT                                                       AS DC_AMT                      /* 합계 에누리금액   */
			                     , A.NET_SALE_AMT                                                 AS NET_SALE_AMT                /* 합계 순매출금액   */
			                     , A.SALE_PROF_AMT                                                AS SALE_PROF_AMT               /* 합계 매출이익액   */
			                     , A.CUST_CNT                                                     AS CUST_CNT                    /* 합계 객수         */
			                     , A.NORM_SALE_AMT_VAT                                            AS NORM_SALE_AMT_VAT           /* 합계              */
			                  FROM (
			                        -- 매출  
			                        SELECT A.STR_CD
			                             , A.SALE_DT
			                             , A.PUMBUN_CD
			                             , A.PUMMOK_CD
			                             , A.EVENT_CD
			                             , A.EVENT_FLAG
			                             , A.EVENT_RATE
			                             , A.MG_RATE
			                             , A.ORG_CD
			                             , A.SALE_QTY                  /* 판매수량     */
			                             , A.TOT_SALE_AMT              /* 총매출금액   */
			                             , A.VAT_AMT                   /* 부가세       */
			                             , A.REDU_AMT                  /* 할인금액     */
			                             , A.NORM_SALE_AMT             /* 정상매출금액 */
			                             , A.DC_AMT                    /* 에누리금액   */
			                             , A.NET_SALE_AMT              /* 순매출금액   */
			                             , A.SALE_PROF_AMT             /* 매출이익액   */
			                             , A.CUST_CNT                  /* 객수         */
			                             , CASE WHEN A.NORM_SALE_AMT = 0 THEN 0
			                                    ELSE (CASE WHEN B.TAX_FLAG = '1' THEN A.NORM_SALE_AMT / 1.1
			                                                                     ELSE A.NORM_SALE_AMT
			                                          END)
			                               END  AS NORM_SALE_AMT_VAT
			                             , C.MG_FLAG
			                          FROM DPS.PS_DAYMGPBN  A
			                             , DPS.PC_PBNMST    B
			                             , DPS.PC_MARGINMST C
			                         WHERE A.SALE_DT    BETWEEN ? AND ?
			                           AND A.STR_CD     = ?
			                           AND A.ORG_CD     LIKE ? || ? || ? || ? || '%'
			                           AND A.PUMBUN_CD  LIKE ? || '%'   
			                           AND A.PUMBUN_CD  = B.PUMBUN_CD
			                           AND A.STR_CD     = C.STR_CD
			                           AND A.PUMBUN_CD  = C.PUMBUN_CD
			                           AND C.PUMMOK_CD  = '********'
			                           AND A.EVENT_CD   = C.EVENT_CD
			                           AND A.EVENT_FLAG = C.EVENT_FLAG
			                           AND A.EVENT_RATE = C.EVENT_RATE
			                           AND A.SALE_DT BETWEEN C.APP_S_DT AND C.APP_E_DT
			                           AND A.PUMBUN_CD IN ( 
			                                                SELECT MYORG.PUMBUN_CD
			                                                  FROM COM.V_MYORG MYORG
			                                                 WHERE MYORG.USER_ID     = ?
			                                                   AND MYORG.ORG_FLAG    = ?
			                                              )
			                       ) A
			               ) A
			             , DPS.PC_STRPBN B             
			             , DPS.PC_ORGMST C  /* 파트 */
			             , DPS.PC_ORGMST D  /* PC   */
			             , DPS.PC_ORGMST E  /* 코너 */
			         WHERE A.STR_CD    = B.STR_CD
			           AND A.PUMBUN_CD = B.PUMBUN_CD
			           AND A.ORG_CD    = DECODE(?, '1', B.SALE_ORG_CD, B.BUY_ORG_CD)
			           AND SUBSTR(A.ORG_CD, 1, 6) || '0000' = C.ORG_CD 
			           AND SUBSTR(A.ORG_CD, 1, 8) || '00'   = D.ORG_CD
			           AND A.ORG_CD    = E.ORG_CD
			       ) A
			GROUP BY A.STR_CD
			       , A.TEAM_CD
			       , A.PART_CD
			       , A.PART_NM
			       , A.PC_CD
			       , A.PC_NM
			       , A.PUMBUN_CD
			       , A.PUMBUN_NAME
			ORDER BY A.STR_CD
			       , A.TEAM_CD
			       , A.PART_CD
			       , A.PC_CD
			       , A.PUMBUN_CD
			       , A.PUMBUN_NAME
        ]]>        
    </query>
		        
</service>

