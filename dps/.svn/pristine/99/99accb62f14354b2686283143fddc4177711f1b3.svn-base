		UNION ALL
		
		SELECT ORG_CD,DIV, SUM(P_YYYY01) P_YYYY01,SUM(P_YYYY02) P_YYYY02,SUM(P_YYYY03) P_YYYY03,SUM(P_YYYY04) P_YYYY04,SUM(P_YYYY05) P_YYYY05,SUM(P_YYYY06) P_YYYY06,          --전년도객단가
		                    SUM(P_YYYY07) P_YYYY07,SUM(P_YYYY08) P_YYYY08,SUM(P_YYYY09) P_YYYY09,SUM(P_YYYY10) P_YYYY10,SUM(P_YYYY11) P_YYYY11,SUM(P_YYYY12) P_YYYY12
		FROM
		    (SELECT ORG_CD,DIV,DECODE(SALE_DT,?||'01',CUST_DANGA,0) AS P_YYYY01,DECODE(SALE_DT,?||'02',CUST_DANGA,0) AS P_YYYY02,DECODE(SALE_DT,?||'03',CUST_DANGA,0) AS P_YYYY03,
		                DECODE(SALE_DT,?||'04',CUST_DANGA,0) AS P_YYYY04,DECODE(SALE_DT,?||'05',CUST_DANGA,0) AS P_YYYY05,DECODE(SALE_DT,?||'06',CUST_DANGA,0) AS P_YYYY06,
		                DECODE(SALE_DT,?||'07',CUST_DANGA,0) AS P_YYYY07,DECODE(SALE_DT,?||'08',CUST_DANGA,0) AS P_YYYY08,DECODE(SALE_DT,?||'09',CUST_DANGA,0) AS P_YYYY09,
		                DECODE(SALE_DT,?||'10',CUST_DANGA,0) AS P_YYYY10,DECODE(SALE_DT,?||'11',CUST_DANGA,0) AS P_YYYY11,DECODE(SALE_DT,?||'12',CUST_DANGA,0) AS P_YYYY12 
		    FROM
		        (SELECT SUBSTR(ORG_CD,1,1) ORG_CD,'12' DIV , SUBSTR(SALE_DT,1,6) SALE_DT,ROUND((CASE WHEN SUM(CUST_CNT) = 0 THEN 0 ELSE (SUM(TOT_SALE_AMT)/SUM(CUST_CNT)) END)) CUST_DANGA
		        FROM DPS.PS_DAYMGPBN
		        WHERE SALE_DT >= ?||'0101' AND SALE_DT <=?||'1231'
		        AND (STR_CD = '01' OR STR_CD = '03')
		        
		        GROUP BY SUBSTR(SALE_DT,1,6),SUBSTR(ORG_CD,1,1)))
		GROUP BY DIV,ORG_CD
		
		
		UNION ALL
		
		SELECT  ORG_CD , DIV,    DECODE(P_YYYY01,0,0,ROUND((YYYY01 - P_YYYY01)/P_YYYY01*100,2)) YYYY01,     --객단가신장율
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY02 - P_YYYY02)/P_YYYY02*100,2)) YYYY02,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY03 - P_YYYY03)/P_YYYY03*100,2)) YYYY03,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY04 - P_YYYY04)/P_YYYY04*100,2)) YYYY04,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY05 - P_YYYY05)/P_YYYY05*100,2)) YYYY05,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY06 - P_YYYY06)/P_YYYY06*100,2)) YYYY06,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY07 - P_YYYY07)/P_YYYY07*100,2)) YYYY07,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY08 - P_YYYY08)/P_YYYY08*100,2)) YYYY08,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY09 - P_YYYY09)/P_YYYY09*100,2)) YYYY09,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY10 - P_YYYY10)/P_YYYY10*100,2)) YYYY10,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY11 - P_YYYY11)/P_YYYY11*100,2)) YYYY11,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY12 - P_YYYY12)/P_YYYY12*100,2)) YYYY12
		    FROM
		        (SELECT ORG_CD,DIV, SUM(YYYY01) YYYY01,SUM(YYYY02) YYYY02,SUM(YYYY03) YYYY03,SUM(YYYY04) YYYY04,SUM(YYYY05) YYYY05,SUM(YYYY06) YYYY06,          --총매출액
		                            SUM(YYYY07) YYYY07,SUM(YYYY08) YYYY08,SUM(YYYY09) YYYY09,SUM(YYYY10) YYYY10,SUM(YYYY11) YYYY11,SUM(YYYY12) YYYY12,
		                            SUM(P_YYYY01) P_YYYY01,SUM(P_YYYY02) P_YYYY02,SUM(P_YYYY03) P_YYYY03,SUM(P_YYYY04) P_YYYY04,SUM(P_YYYY05) P_YYYY05,SUM(P_YYYY06) P_YYYY06,          --전년총매출액
		                            SUM(P_YYYY07) P_YYYY07,SUM(P_YYYY08) P_YYYY08,SUM(P_YYYY09) P_YYYY09,SUM(P_YYYY10) P_YYYY10,SUM(P_YYYY11) P_YYYY11,SUM(P_YYYY12) P_YYYY12
		        FROM
		            (SELECT ORG_CD,DIV,DECODE(SALE_DT,?||'01',TOT_SALE_AMT,0) AS YYYY01,DECODE(SALE_DT,?||'02',TOT_SALE_AMT,0) AS YYYY02,DECODE(SALE_DT,?||'03',TOT_SALE_AMT,0) AS YYYY03,
		                        DECODE(SALE_DT,?||'04',TOT_SALE_AMT,0) AS YYYY04,DECODE(SALE_DT,?||'05',TOT_SALE_AMT,0) AS YYYY05,DECODE(SALE_DT,?||'06',TOT_SALE_AMT,0) AS YYYY06,
		                        DECODE(SALE_DT,?||'07',TOT_SALE_AMT,0) AS YYYY07,DECODE(SALE_DT,?||'08',TOT_SALE_AMT,0) AS YYYY08,DECODE(SALE_DT,?||'09',TOT_SALE_AMT,0) AS YYYY09,
		                        DECODE(SALE_DT,?||'10',TOT_SALE_AMT,0) AS YYYY10,DECODE(SALE_DT,?||'11',TOT_SALE_AMT,0) AS YYYY11,DECODE(SALE_DT,?||'12',TOT_SALE_AMT,0) AS YYYY12,
		                        DECODE(SALE_DT,?||'01',TOT_SALE_AMT,0) AS P_YYYY01,DECODE(SALE_DT,?||'02',TOT_SALE_AMT,0) AS P_YYYY02,DECODE(SALE_DT,?||'03',TOT_SALE_AMT,0) AS P_YYYY03,
		                        DECODE(SALE_DT,?||'04',TOT_SALE_AMT,0) AS P_YYYY04,DECODE(SALE_DT,?||'05',TOT_SALE_AMT,0) AS P_YYYY05,DECODE(SALE_DT,?||'06',TOT_SALE_AMT,0) AS P_YYYY06,
		                        DECODE(SALE_DT,?||'07',TOT_SALE_AMT,0) AS P_YYYY07,DECODE(SALE_DT,?||'08',TOT_SALE_AMT,0) AS P_YYYY08,DECODE(SALE_DT,?||'09',TOT_SALE_AMT,0) AS P_YYYY09,
		                        DECODE(SALE_DT,?||'10',TOT_SALE_AMT,0) AS P_YYYY10,DECODE(SALE_DT,?||'11',TOT_SALE_AMT,0) AS P_YYYY11,DECODE(SALE_DT,?||'12',TOT_SALE_AMT,0) AS P_YYYY12 
		            FROM
		                (SELECT SUBSTR(ORG_CD,1,1) ORG_CD,'13' DIV , SUBSTR(SALE_DT,1,6) SALE_DT,ROUND((CASE WHEN SUM(CUST_CNT) = 0 THEN 0 ELSE (SUM(TOT_SALE_AMT)/SUM(CUST_CNT)) END)) TOT_SALE_AMT
		                FROM DPS.PS_DAYMGPBN
		                WHERE SALE_DT >= ?||'0101' AND SALE_DT <=?||'1231'
		                AND (STR_CD = '01' OR STR_CD = '03')
		        
		            GROUP BY SUBSTR(SALE_DT,1,6),SUBSTR(ORG_CD,1,1)))
		        GROUP BY DIV,ORG_CD,ORG_CD)) M
         ORDER BY (CASE WHEN ORG_CD = '0107' THEN '1' 
            WHEN ORG_CD = '0108' THEN '2'
            WHEN ORG_CD = '01' THEN '3'
            WHEN ORG_CD = '0303' THEN '4'
            WHEN ORG_CD = '03' THEN '5'
            WHEN ORG_CD = '0' THEN '6'
            ELSE '7' END) , DIV 
			  
        ]]>        
    </query>
    
    
</service>