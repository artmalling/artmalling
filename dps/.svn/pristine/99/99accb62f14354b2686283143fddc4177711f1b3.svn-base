<service name="/psal320.ps">

    <gauce name="H_SEL_MASTER">
    	<column colName="ORG_CD"                  colType="TB_STRING"      size="13"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="ORG_NAME"                colType="TB_STRING"      size="40"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" /> 
        <column colName="DIV" 	                  colType="TB_STRING"      size="13"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="DIV_NAME"                colType="TB_STRING"      size="40"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="YYYY99"       		      colType="TB_STRING"      size="50"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="YYYY01"       		      colType="TB_STRING"      size="50"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="YYYY02"       		      colType="TB_STRING"      size="50"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="YYYY03"       		      colType="TB_STRING"      size="50"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="YYYY04"       		      colType="TB_STRING"      size="50"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="YYYY05"       		      colType="TB_STRING"      size="50"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="YYYY06"       		      colType="TB_STRING"      size="50"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="YYYY07"       		      colType="TB_STRING"      size="50"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="YYYY08"       		      colType="TB_STRING"      size="50"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="YYYY09"       		      colType="TB_STRING"      size="50"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="YYYY10"       		      colType="TB_STRING"      size="50"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="YYYY11"       		      colType="TB_STRING"      size="50"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="YYYY12"       		      colType="TB_STRING"      size="50"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        
    </gauce>
    
    
    
    <!-- 쿼리는 전부 대문자 -->
   
    <query name="SEL_MASTER">        
        <![CDATA[
	      SELECT  
	      		ORG_CD, CASE WHEN ORG_CD = '0107' THEN '마리오1관' WHEN ORG_CD = '0108' THEN '마리오2관' WHEN ORG_CD = '01' THEN '1관,2관합계' WHEN ORG_CD = '0303' THEN
               '마리오3관' WHEN ORG_CD = '0' THEN '총합계' END ORG_NAME, 
            DIV,(SELECT COMM_NAME1 FROM  COM.TC_COMMCODE TC WHERE TC.COMM_PART = 'P320' AND TC.SYS_PART = 'D' AND TC.COMM_CODE = M.DIV) DIV_NAME,
            0 YYYY99,
			CASE WHEN YYYY01 > 101 THEN TO_CHAR(YYYY01,'999,999,999,999,999,999') ELSE TO_CHAR(YYYY01,'990.99') END  YYYY01,
			CASE WHEN YYYY02 > 101 THEN TO_CHAR(YYYY02,'999,999,999,999,999,999') ELSE TO_CHAR(YYYY02,'990.99') END  YYYY02,
			CASE WHEN YYYY03 > 101 THEN TO_CHAR(YYYY03,'999,999,999,999,999,999') ELSE TO_CHAR(YYYY03,'990.99') END  YYYY03,
			CASE WHEN YYYY04 > 101 THEN TO_CHAR(YYYY04,'999,999,999,999,999,999') ELSE TO_CHAR(YYYY04,'990.99') END  YYYY04,
			CASE WHEN YYYY05 > 101 THEN TO_CHAR(YYYY05,'999,999,999,999,999,999') ELSE TO_CHAR(YYYY05,'990.99') END  YYYY05,
			CASE WHEN YYYY06 > 101 THEN TO_CHAR(YYYY06,'999,999,999,999,999,999') ELSE TO_CHAR(YYYY06,'990.99') END  YYYY06,
			CASE WHEN YYYY07 > 101 THEN TO_CHAR(YYYY07,'999,999,999,999,999,999') ELSE TO_CHAR(YYYY07,'990.99') END  YYYY07,
			CASE WHEN YYYY08 > 101 THEN TO_CHAR(YYYY08,'999,999,999,999,999,999') ELSE TO_CHAR(YYYY08,'990.99') END  YYYY08,
			CASE WHEN YYYY09 > 101 THEN TO_CHAR(YYYY09,'999,999,999,999,999,999') ELSE TO_CHAR(YYYY09,'990.99') END  YYYY09,
			CASE WHEN YYYY10 > 101 THEN TO_CHAR(YYYY10,'999,999,999,999,999,999') ELSE TO_CHAR(YYYY10,'990.99') END  YYYY10,
			CASE WHEN YYYY11 > 101 THEN TO_CHAR(YYYY11,'999,999,999,999,999,999') ELSE TO_CHAR(YYYY11,'990.99') END  YYYY11,
			CASE WHEN YYYY12 > 101 THEN TO_CHAR(YYYY12,'999,999,999,999,999,999') ELSE TO_CHAR(YYYY12,'990.99') END  YYYY12
		FROM
		    ------------------------------------------------------  1관   ------------------------------------------------------
		    (SELECT ORG_CD,DIV, SUM(YYYY01) YYYY01,SUM(YYYY02) YYYY02,SUM(YYYY03) YYYY03,SUM(YYYY04) YYYY04,SUM(YYYY05) YYYY05,SUM(YYYY06) YYYY06,          --총매출액
		                        SUM(YYYY07) YYYY07,SUM(YYYY08) YYYY08,SUM(YYYY09) YYYY09,SUM(YYYY10) YYYY10,SUM(YYYY11) YYYY11,SUM(YYYY12) YYYY12
		    FROM
		        (SELECT ORG_CD,DIV,DECODE(SALE_DT,?||'01',TOT_SALE_AMT,0) AS YYYY01,DECODE(SALE_DT,?||'02',TOT_SALE_AMT,0) AS YYYY02,DECODE(SALE_DT,?||'03',TOT_SALE_AMT,0) AS YYYY03,
		                    DECODE(SALE_DT,?||'04',TOT_SALE_AMT,0) AS YYYY04,DECODE(SALE_DT,?||'05',TOT_SALE_AMT,0) AS YYYY05,DECODE(SALE_DT,?||'06',TOT_SALE_AMT,0) AS YYYY06,
		                    DECODE(SALE_DT,?||'07',TOT_SALE_AMT,0) AS YYYY07,DECODE(SALE_DT,?||'08',TOT_SALE_AMT,0) AS YYYY08,DECODE(SALE_DT,?||'09',TOT_SALE_AMT,0) AS YYYY09,
		                    DECODE(SALE_DT,?||'10',TOT_SALE_AMT,0) AS YYYY10,DECODE(SALE_DT,?||'11',TOT_SALE_AMT,0) AS YYYY11,DECODE(SALE_DT,?||'12',TOT_SALE_AMT,0) AS YYYY12 
		        FROM
		            (SELECT SUBSTR(ORG_CD,1,4) ORG_CD,'01' DIV , SUBSTR(SALE_DT,1,6) SALE_DT,ROUND(SUM(TOT_SALE_AMT)/?) TOT_SALE_AMT
		            FROM DPS.PS_DAYMGPBN
		            WHERE SALE_DT >= ?||'0101' AND SALE_DT <=?||'1231'
		            AND STR_CD = '01'
		            AND ORG_CD LIKE '0107%'
		            GROUP BY SUBSTR(SALE_DT,1,6),SUBSTR(ORG_CD,1,4)))
		    GROUP BY DIV,ORG_CD,ORG_CD
		
		    UNION ALL
		
		    SELECT ORG_CD,DIV, SUM(YYYY01) YYYY01,SUM(YYYY02) YYYY02,SUM(YYYY03) YYYY03,SUM(YYYY04) YYYY04,SUM(YYYY05) YYYY05,SUM(YYYY06) YYYY06,          --이익액
		                        SUM(YYYY07) YYYY07,SUM(YYYY08) YYYY08,SUM(YYYY09) YYYY09,SUM(YYYY10) YYYY10,SUM(YYYY11) YYYY11,SUM(YYYY12) YYYY12
		    FROM
		        (SELECT ORG_CD,DIV,DECODE(SALE_DT,?||'01',SALE_PROF_AMT,0) AS YYYY01,DECODE(SALE_DT,?||'02',SALE_PROF_AMT,0) AS YYYY02,DECODE(SALE_DT,?||'03',SALE_PROF_AMT,0) AS YYYY03,
		                    DECODE(SALE_DT,?||'04',SALE_PROF_AMT,0) AS YYYY04,DECODE(SALE_DT,?||'05',SALE_PROF_AMT,0) AS YYYY05,DECODE(SALE_DT,?||'06',SALE_PROF_AMT,0) AS YYYY06,
		                    DECODE(SALE_DT,?||'07',SALE_PROF_AMT,0) AS YYYY07,DECODE(SALE_DT,?||'08',SALE_PROF_AMT,0) AS YYYY08,DECODE(SALE_DT,?||'09',SALE_PROF_AMT,0) AS YYYY09,
		                    DECODE(SALE_DT,?||'10',SALE_PROF_AMT,0) AS YYYY10,DECODE(SALE_DT,?||'11',SALE_PROF_AMT,0) AS YYYY11,DECODE(SALE_DT,?||'12',SALE_PROF_AMT,0) AS YYYY12 
		        FROM
		            (SELECT SUBSTR(ORG_CD,1,4) ORG_CD,'02' DIV , SUBSTR(SALE_DT,1,6) SALE_DT,ROUND(SUM(SALE_PROF_AMT)/?) SALE_PROF_AMT
		            FROM DPS.PS_DAYMGPBN
		            WHERE SALE_DT >= ?||'0101' AND SALE_DT <=?||'1231'
		            AND STR_CD = '01'
		            AND ORG_CD LIKE '0107%'
		            GROUP BY SUBSTR(SALE_DT,1,6),SUBSTR(ORG_CD,1,4)))
		    GROUP BY DIV,ORG_CD
		
		    UNION ALL
		
		    SELECT ORG_CD,DIV, SUM(YYYY01) YYYY01,SUM(YYYY02) YYYY02,SUM(YYYY03) YYYY03,SUM(YYYY04) YYYY04,SUM(YYYY05) YYYY05,SUM(YYYY06) YYYY06,          --이익율
		                        SUM(YYYY07) YYYY07,SUM(YYYY08) YYYY08,SUM(YYYY09) YYYY09,SUM(YYYY10) YYYY10,SUM(YYYY11) YYYY11,SUM(YYYY12) YYYY12
		    FROM
		        (SELECT ORG_CD,DIV,DECODE(SALE_DT,?||'01',SALE_PROF_AMT,0) AS YYYY01,DECODE(SALE_DT,?||'02',SALE_PROF_AMT,0) AS YYYY02,DECODE(SALE_DT,?||'03',SALE_PROF_AMT,0) AS YYYY03,
		                    DECODE(SALE_DT,?||'04',SALE_PROF_AMT,0) AS YYYY04,DECODE(SALE_DT,?||'05',SALE_PROF_AMT,0) AS YYYY05,DECODE(SALE_DT,?||'06',SALE_PROF_AMT,0) AS YYYY06,
		                    DECODE(SALE_DT,?||'07',SALE_PROF_AMT,0) AS YYYY07,DECODE(SALE_DT,?||'08',SALE_PROF_AMT,0) AS YYYY08,DECODE(SALE_DT,?||'09',SALE_PROF_AMT,0) AS YYYY09,
		                    DECODE(SALE_DT,?||'10',SALE_PROF_AMT,0) AS YYYY10,DECODE(SALE_DT,?||'11',SALE_PROF_AMT,0) AS YYYY11,DECODE(SALE_DT,?||'12',SALE_PROF_AMT,0) AS YYYY12 
		        FROM
		            (SELECT SUBSTR(ORG_CD,1,4) ORG_CD,'03' DIV , SUBSTR(SALE_DT,1,6) SALE_DT,DECODE(SUM(TOT_SALE_AMT),0,0,ROUND(SUM(SALE_PROF_AMT)/SUM(TOT_SALE_AMT)*100,2)) SALE_PROF_AMT
		            FROM DPS.PS_DAYMGPBN
		            WHERE SALE_DT >= ?||'0101' AND SALE_DT <=?||'1231'
		            AND STR_CD = '01'
		            AND ORG_CD LIKE '0107%'
		            GROUP BY SUBSTR(SALE_DT,1,6),SUBSTR(ORG_CD,1,4)))
		    GROUP BY DIV,ORG_CD
			
		    UNION ALL
		    
		    SELECT  ORG_CD , DIV,    DECODE(P_YYYY01,0,0,ROUND((YYYY01 - P_YYYY01)/P_YYYY01*100,2)) YYYY01,     --매출신장율
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY02 - P_YYYY02)/P_YYYY02*100,2)) YYYY02,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY03 - P_YYYY03)/P_YYYY03*100,2)) YYYY03,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY04 - P_YYYY04)/P_YYYY04*100,2)) YYYY04,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY05 - P_YYYY05)/P_YYYY05*100,2)) YYYY05,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY06 - P_YYYY06)/P_YYYY06*100,2)) YYYY06,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY07 - P_YYYY07)/P_YYYY07*100,2)) YYYY07,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY08 - P_YYYY08)/P_YYYY08*100,2)) YYYY08,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY09 - P_YYYY09)/P_YYYY09*100,2)) YYYY09,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY10 - P_YYYY10)/P_YYYY10*100,2)) YYYY10,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY11 - P_YYYY11)/P_YYYY11*100,2)) YYYY11,
		                                      DECODE(P_YYYY01,0,0,ROUND((YYYY12 - P_YYYY12)/P_YYYY12*100,2)) YYYY12
		    FROM
		        (SELECT ORG_CD,DIV, SUM(YYYY01) YYYY01,SUM(YYYY02) YYYY02,SUM(YYYY03) YYYY03,SUM(YYYY04) YYYY04,SUM(YYYY05) YYYY05,SUM(YYYY06) YYYY06,          
		                            SUM(YYYY07) YYYY07,SUM(YYYY08) YYYY08,SUM(YYYY09) YYYY09,SUM(YYYY10) YYYY10,SUM(YYYY11) YYYY11,SUM(YYYY12) YYYY12,
		                            SUM(P_YYYY01) P_YYYY01,SUM(P_YYYY02) P_YYYY02,SUM(P_YYYY03) P_YYYY03,SUM(P_YYYY04) P_YYYY04,SUM(P_YYYY05) P_YYYY05,SUM(P_YYYY06) P_YYYY06,          
		                            SUM(P_YYYY07) P_YYYY07,SUM(P_YYYY08) P_YYYY08,SUM(P_YYYY09) P_YYYY09,SUM(P_YYYY10) P_YYYY10,SUM(P_YYYY11) P_YYYY11,SUM(P_YYYY12) P_YYYY12
		        FROM
		            (SELECT ORG_CD,DIV,DECODE(SALE_DT,?||'01',TOT_SALE_AMT,0) AS YYYY01,DECODE(SALE_DT,?||'02',TOT_SALE_AMT,0) AS YYYY02,DECODE(SALE_DT,?||'03',TOT_SALE_AMT,0) AS YYYY03,
		                        DECODE(SALE_DT,?||'04',TOT_SALE_AMT,0) AS YYYY04,DECODE(SALE_DT,?||'05',TOT_SALE_AMT,0) AS YYYY05,DECODE(SALE_DT,?||'06',TOT_SALE_AMT,0) AS YYYY06,
		                        DECODE(SALE_DT,?||'07',TOT_SALE_AMT,0) AS YYYY07,DECODE(SALE_DT,?||'08',TOT_SALE_AMT,0) AS YYYY08,DECODE(SALE_DT,?||'09',TOT_SALE_AMT,0) AS YYYY09,
		                        DECODE(SALE_DT,?||'10',TOT_SALE_AMT,0) AS YYYY10,DECODE(SALE_DT,?||'11',TOT_SALE_AMT,0) AS YYYY11,DECODE(SALE_DT,?||'12',TOT_SALE_AMT,0) AS YYYY12,
		                        DECODE(SALE_DT,?||'01',TOT_SALE_AMT,0) AS P_YYYY01,DECODE(SALE_DT,?||'02',TOT_SALE_AMT,0) AS P_YYYY02,DECODE(SALE_DT,?||'03',TOT_SALE_AMT,0) AS P_YYYY03,
		                        DECODE(SALE_DT,?||'04',TOT_SALE_AMT,0) AS P_YYYY04,DECODE(SALE_DT,?||'05',TOT_SALE_AMT,0) AS P_YYYY05,DECODE(SALE_DT,?||'06',TOT_SALE_AMT,0) AS P_YYYY06,
		                        DECODE(SALE_DT,?||'07',TOT_SALE_AMT,0) AS P_YYYY07,DECODE(SALE_DT,?||'08',TOT_SALE_AMT,0) AS P_YYYY08,DECODE(SALE_DT,?||'09',TOT_SALE_AMT,0) AS P_YYYY09,
		                        DECODE(SALE_DT,?||'10',TOT_SALE_AMT,0) AS P_YYYY10,DECODE(SALE_DT,?||'11',TOT_SALE_AMT,0) AS P_YYYY11,DECODE(SALE_DT,?||'12',TOT_SALE_AMT,0) AS P_YYYY12 
		            FROM
		                (SELECT SUBSTR(ORG_CD,1,4) ORG_CD,'07' DIV , SUBSTR(SALE_DT,1,6) SALE_DT,ROUND(SUM(TOT_SALE_AMT)/?) TOT_SALE_AMT
		                FROM DPS.PS_DAYMGPBN
		                WHERE SALE_DT >= ?||'0101' AND SALE_DT <=?||'1231'
		                AND STR_CD = '01'
		                AND ORG_CD LIKE '0107%'
		                GROUP BY SUBSTR(SALE_DT,1,6),SUBSTR(ORG_CD,1,4)))
		        GROUP BY DIV,ORG_CD,ORG_CD)
		
		
		    UNION ALL
		
		
		    SELECT ORG_CD,DIV, SUM(YYYY01) YYYY01,SUM(YYYY02) YYYY02,SUM(YYYY03) YYYY03,SUM(YYYY04) YYYY04,SUM(YYYY05) YYYY05,SUM(YYYY06) YYYY06,          --총매출액
		                        SUM(YYYY07) YYYY07,SUM(YYYY08) YYYY08,SUM(YYYY09) YYYY09,SUM(YYYY10) YYYY10,SUM(YYYY11) YYYY11,SUM(YYYY12) YYYY12
		    FROM
		        (SELECT ORG_CD,DIV,DECODE(SALE_DT,?||'01',TOT_SALE_AMT,0) AS YYYY01,DECODE(SALE_DT,?||'02',TOT_SALE_AMT,0) AS YYYY02,DECODE(SALE_DT,?||'03',TOT_SALE_AMT,0) AS YYYY03,
		                    DECODE(SALE_DT,?||'04',TOT_SALE_AMT,0) AS YYYY04,DECODE(SALE_DT,?||'05',TOT_SALE_AMT,0) AS YYYY05,DECODE(SALE_DT,?||'06',TOT_SALE_AMT,0) AS YYYY06,
		                    DECODE(SALE_DT,?||'07',TOT_SALE_AMT,0) AS YYYY07,DECODE(SALE_DT,?||'08',TOT_SALE_AMT,0) AS YYYY08,DECODE(SALE_DT,?||'09',TOT_SALE_AMT,0) AS YYYY09,
		                    DECODE(SALE_DT,?||'10',TOT_SALE_AMT,0) AS YYYY10,DECODE(SALE_DT,?||'11',TOT_SALE_AMT,0) AS YYYY11,DECODE(SALE_DT,?||'12',TOT_SALE_AMT,0) AS YYYY12 
		        FROM
		            (SELECT SUBSTR(ORG_CD,1,4) ORG_CD,'04' DIV  , SUBSTR(SALE_DT,1,6) SALE_DT,ROUND(SUM(TOT_SALE_AMT)/?) TOT_SALE_AMT
		            FROM DPS.PS_DAYMGPBN
		            WHERE SALE_DT >= ?||'0101' AND SALE_DT <=?||'1231'
		            AND STR_CD = '01'
		            AND ORG_CD LIKE '0107%'
		            GROUP BY SUBSTR(SALE_DT,1,6),SUBSTR(ORG_CD,1,4)))
		    GROUP BY DIV,ORG_CD
		
		    UNION ALL
		
		    SELECT ORG_CD,DIV, SUM(YYYY01) YYYY01,SUM(YYYY02) YYYY02,SUM(YYYY03) YYYY03,SUM(YYYY04) YYYY04,SUM(YYYY05) YYYY05,SUM(YYYY06) YYYY06,          --이익액
		                        SUM(YYYY07) YYYY07,SUM(YYYY08) YYYY08,SUM(YYYY09) YYYY09,SUM(YYYY10) YYYY10,SUM(YYYY11) YYYY11,SUM(YYYY12) YYYY12
		    FROM
		        (SELECT ORG_CD,DIV,DECODE(SALE_DT,?||'01',SALE_PROF_AMT,0) AS YYYY01,DECODE(SALE_DT,?||'02',SALE_PROF_AMT,0) AS YYYY02,DECODE(SALE_DT,?||'03',SALE_PROF_AMT,0) AS YYYY03,
		                    DECODE(SALE_DT,?||'04',SALE_PROF_AMT,0) AS YYYY04,DECODE(SALE_DT,?||'05',SALE_PROF_AMT,0) AS YYYY05,DECODE(SALE_DT,?||'06',SALE_PROF_AMT,0) AS YYYY06,
		                    DECODE(SALE_DT,?||'07',SALE_PROF_AMT,0) AS YYYY07,DECODE(SALE_DT,?||'08',SALE_PROF_AMT,0) AS YYYY08,DECODE(SALE_DT,?||'09',SALE_PROF_AMT,0) AS YYYY09,
		                    DECODE(SALE_DT,?||'10',SALE_PROF_AMT,0) AS YYYY10,DECODE(SALE_DT,?||'11',SALE_PROF_AMT,0) AS YYYY11,DECODE(SALE_DT,?||'12',SALE_PROF_AMT,0) AS YYYY12 
		        FROM
		            (SELECT SUBSTR(ORG_CD,1,4) ORG_CD,'05' DIV , SUBSTR(SALE_DT,1,6) SALE_DT,ROUND(SUM(SALE_PROF_AMT)/?) SALE_PROF_AMT
		            FROM DPS.PS_DAYMGPBN
		            WHERE SALE_DT >= ?||'0101' AND SALE_DT <=?||'1231'
		            AND STR_CD = '01'
		            AND ORG_CD LIKE '0107%'
		            GROUP BY SUBSTR(SALE_DT,1,6),SUBSTR(ORG_CD,1,4)))
		    GROUP BY DIV,ORG_CD
		
		    UNION ALL
		
		    SELECT ORG_CD,DIV, SUM(YYYY01) YYYY01,SUM(YYYY02) YYYY02,SUM(YYYY03) YYYY03,SUM(YYYY04) YYYY04,SUM(YYYY05) YYYY05,SUM(YYYY06) YYYY06,          --이익율
		                        SUM(YYYY07) YYYY07,SUM(YYYY08) YYYY08,SUM(YYYY09) YYYY09,SUM(YYYY10) YYYY10,SUM(YYYY11) YYYY11,SUM(YYYY12) YYYY12
		    FROM
		        (SELECT ORG_CD,DIV,DECODE(SALE_DT,?||'01',SALE_PROF_AMT,0) AS YYYY01,DECODE(SALE_DT,?||'02',SALE_PROF_AMT,0) AS YYYY02,DECODE(SALE_DT,?||'03',SALE_PROF_AMT,0) AS YYYY03,
		                    DECODE(SALE_DT,?||'04',SALE_PROF_AMT,0) AS YYYY04,DECODE(SALE_DT,?||'05',SALE_PROF_AMT,0) AS YYYY05,DECODE(SALE_DT,?||'06',SALE_PROF_AMT,0) AS YYYY06,
		                    DECODE(SALE_DT,?||'07',SALE_PROF_AMT,0) AS YYYY07,DECODE(SALE_DT,?||'08',SALE_PROF_AMT,0) AS YYYY08,DECODE(SALE_DT,?||'09',SALE_PROF_AMT,0) AS YYYY09,
		                    DECODE(SALE_DT,?||'10',SALE_PROF_AMT,0) AS YYYY10,DECODE(SALE_DT,?||'11',SALE_PROF_AMT,0) AS YYYY11,DECODE(SALE_DT,?||'12',SALE_PROF_AMT,0) AS YYYY12 
		        FROM
		            (SELECT SUBSTR(ORG_CD,1,4) ORG_CD,'06' DIV , SUBSTR(SALE_DT,1,6) SALE_DT,DECODE(SUM(TOT_SALE_AMT),0,0,ROUND(SUM(SALE_PROF_AMT)/SUM(TOT_SALE_AMT)*100,2)) SALE_PROF_AMT
		            FROM DPS.PS_DAYMGPBN
		            WHERE SALE_DT >= ?||'0101' AND SALE_DT <=?||'1231'
