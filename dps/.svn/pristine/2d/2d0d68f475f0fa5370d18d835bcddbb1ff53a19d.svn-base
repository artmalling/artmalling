    <!-- 당초PC별월매출계획 조회 쿼리  -->
    <query name="SEL_PLANPC_DETAIL">        
        <![CDATA[
            SELECT A.STR_CD
                 , C.STR_NAME
                 , A.ORG_CD
                 , A.PLAN_YM
                 , A.ORG_LEVEL
                 , A.DEPT_CD
                 , D.ORG_SHORT_NAME                 AS DEPT_ORG_NAME
                 , A.TEAM_CD
                 , E.ORG_SHORT_NAME                 AS TEAM_ORG_NAME
                 , A.PC_CD
                 , B.ORG_SHORT_NAME                 AS PC_ORG_NAME
                 , A.PUMBUN_CD
                 , G.PUMBUN_NAME
                 , SUM(NVL(A.BFYY_NORM_SAMT,0))     AS BFYY_NORM_SAMT
                 , SUM(NVL(A.BFYY_EVT_SAMT,0))      AS BFYY_EVT_SAMT
                 , SUM(NVL(A.BFYY_SALE_TAMT,0))     AS BFYY_SALE_TAMT
                 , SUM(NVL(A.BFYY_PROF_TAMT,0))     AS BFYY_PROF_TAMT
                 , SUM(NVL(A.BFYY_SALE_CRATE,0))    AS BFYY_SALE_CRATE
                 , SUM(NVL(A.BFYY_PROF_CRATE,0))    AS BFYY_PROF_CRATE
                 , SUM(NVL(A.ORIGIN_NORM_SAMT,0))   AS ORIGIN_NORM_SAMT
                 , SUM(NVL(A.ORIGIN_EVT_SAMT,0))    AS ORIGIN_EVT_SAMT
                 , SUM(NVL(A.ORIGIN_SALE_TAMT,0))   AS ORIGIN_SALE_TAMT
                 , SUM(NVL(A.ORIGIN_PROF_TAMT,0))   AS ORIGIN_PROF_TAMT
                 , SUM(NVL(A.ORIGIN_SALE_CRATE,0))  AS ORIGIN_SALE_CRATE
                 , SUM(NVL(A.ORIGIN_PROF_CRATE,0))  AS ORIGIN_PROF_CRATE
                 , SUM(NVL(A.ORIGIN_SALE_IRATE,0))  AS ORIGIN_SALE_IRATE
                 , SUM(NVL(A.ORIGIN_PROF_IRATE,0))  AS ORIGIN_PROF_IRATE
                 , ''                               AS ERROR_CHECK
                 , MAX(A.CONF_FLAG)                 AS CONF_FLAG
              FROM (SELECT A.STR_CD
                         , A.ORG_CD
                         , TO_CHAR(ADD_MONTHS(A.PLAN_YM || '01', 12), 'YYYYMM') AS PLAN_YM
                         , A.ORG_LEVEL
                         , A.DEPT_CD
                         , A.TEAM_CD
                         , A.PC_CD
                         , A.PUMBUN_CD
                         , A.ORIGIN_NORM_SAMT       AS BFYY_NORM_SAMT
                         , A.ORIGIN_EVT_SAMT        AS BFYY_EVT_SAMT
                         , A.ORIGIN_SALE_TAMT       AS BFYY_SALE_TAMT
                         , A.ORIGIN_PROF_TAMT       AS BFYY_PROF_TAMT
                         , A.ORIGIN_SALE_CRATE      AS BFYY_SALE_CRATE
                         , A.ORIGIN_PROF_CRATE      AS BFYY_PROF_CRATE
                         , 0                        AS ORIGIN_NORM_SAMT
                         , 0                        AS ORIGIN_EVT_SAMT
                         , 0                        AS ORIGIN_SALE_TAMT
                         , 0                        AS ORIGIN_PROF_TAMT
                         , 0                        AS ORIGIN_SALE_CRATE
                         , 0                        AS ORIGIN_PROF_CRATE
                         , 0                        AS ORIGIN_SALE_IRATE
                         , 0                        AS ORIGIN_PROF_IRATE
                         , ''                       AS CONF_FLAG
                      FROM DPS.PS_PLANPUMBUN    A
                         , DPS.PC_STRPBN        B
                     WHERE A.STR_CD     = B.STR_CD
                       AND A.PUMBUN_CD  = B.PUMBUN_CD
                       AND B.BIZ_TYPE   != '5'
                       AND A.STR_CD     = ?
                       AND A.DEPT_CD    = ?
                       AND A.TEAM_CD    = ?
                       AND A.PC_CD      = ?
                       AND A.PLAN_YM    LIKE ?-1 || '%'
                       AND A.ORG_LEVEL  = '4'
                       
                       
                     UNION ALL
                     
                    SELECT A.STR_CD
                         , A.ORG_CD
                         , A.PLAN_YM
                         , A.ORG_LEVEL
                         , A.DEPT_CD
                         , A.TEAM_CD
                         , A.PC_CD
                         , A.PUMBUN_CD
                         , 0                        AS BFYY_NORM_SAMT
                         , 0                        AS BFYY_EVT_SAMT
                         , 0                        AS BFYY_SALE_TAMT
                         , 0                        AS BFYY_PROF_TAMT
                         , 0                        AS BFYY_SALE_CRATE
                         , 0                        AS BFYY_PROF_CRATE
                         , A.ORIGIN_NORM_SAMT
                         , A.ORIGIN_EVT_SAMT
                         , A.ORIGIN_SALE_TAMT
                         , A.ORIGIN_PROF_TAMT
                         , A.ORIGIN_SALE_CRATE
                         , A.ORIGIN_PROF_CRATE
                         , A.ORIGIN_SALE_IRATE
                         , A.ORIGIN_PROF_IRATE
                         , NVL(A.CONF_FLAG,'N')     AS CONF_FLAG
                      FROM DPS.PS_PLANPUMBUN    A
                         , DPS.PC_STRPBN        B
                     WHERE A.STR_CD     = B.STR_CD
                       AND A.PUMBUN_CD  = B.PUMBUN_CD
                       AND B.BIZ_TYPE   != '5'
                       AND A.STR_CD     = ?
                       AND A.DEPT_CD    = ?
                       AND A.TEAM_CD    = ?
                       AND A.PC_CD      = ?
                       AND A.PLAN_YM    LIKE ? || '%'
                       AND A.ORG_LEVEL  = '4'
                   )                A
                 , DPS.PC_ORGMST    B
                 , DPS.PC_STRMST    C
                 , ( SELECT STR_CD
                          , DEPT_CD
                          , ORG_SHORT_NAME
                       FROM DPS.PC_ORGMST
                      WHERE STR_CD    = ?
                        AND DEPT_CD   = ?
                        AND ORG_CD    = ? || ? || '000000') D
                 , ( SELECT STR_CD
                          , DEPT_CD
                          , TEAM_CD
                          , ORG_SHORT_NAME
                       FROM DPS.PC_ORGMST
                      WHERE STR_CD    = ?
                        AND DEPT_CD   = ?
                        AND TEAM_CD   = ?
                        AND ORG_CD    = ? || ? || ? || '0000') E
                 , DPS.PC_STRPBN    G
             WHERE A.STR_CD    = B.STR_CD
               AND A.STR_CD    = C.STR_CD
               AND A.ORG_CD    = B.ORG_CD
               AND A.STR_CD    = D.STR_CD
               AND A.DEPT_CD   = D.DEPT_CD
               AND A.STR_CD    = E.STR_CD
               AND A.DEPT_CD   = E.DEPT_CD
               AND A.TEAM_CD   = E.TEAM_CD
               AND A.STR_CD    = G.STR_CD
               AND A.PUMBUN_CD = G.PUMBUN_CD
               AND A.ORG_CD    = SUBSTR(G.SALE_ORG_CD,1,8) || '00'
               AND B.ORG_FLAG  = ?
               AND EXISTS ( SELECT MYORG.USER_ID
                              FROM COM.V_MYORG MYORG
                             WHERE MYORG.STR_CD      = A.STR_CD
                               AND MYORG.DEPT_CD     = A.DEPT_CD
                               AND MYORG.TEAM_CD     = A.TEAM_CD
                               AND MYORG.PC_CD       = A.PC_CD
                               AND MYORG.PUMBUN_CD   = A.PUMBUN_CD
                               AND MYORG.USER_ID     = ?
                             )
             GROUP BY  
                   A.STR_CD
                 , C.STR_NAME
                 , A.ORG_CD
                 , A.PLAN_YM
                 , A.ORG_LEVEL
                 , A.DEPT_CD
                 , D.ORG_SHORT_NAME
                 , A.TEAM_CD
                 , E.ORG_SHORT_NAME
                 , A.PC_CD
                 , B.ORG_SHORT_NAME
                 , A.PUMBUN_CD
                 , G.PUMBUN_NAME
             ORDER BY 
                   A.STR_CD
                 , A.ORG_CD
                 , A.ORG_LEVEL
                 , A.DEPT_CD
                 , A.TEAM_CD
                 , A.PC_CD
                 , A.PUMBUN_CD
                 , A.PLAN_YM
               
            
        ]]>        
    </query>
    
    
