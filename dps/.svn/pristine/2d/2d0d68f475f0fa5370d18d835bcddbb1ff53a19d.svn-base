    <!-- 쿼리는 전부 대문자 -->
    <!-- 당초팀별월매출계획 조회 쿼리  -->
    <query name="SEL_PLANPC">        
        <![CDATA[
          SELECT A.CONF_FLAG
               , A.STR_CD
               , C.STR_NAME
               , A.DEPT_CD
               , A.TEAM_CD
               , A.PC_CD
               , B.ORG_SHORT_NAME            AS ORG_NAME
               , ?                           AS PLAN_YEAR
               , A.ORIGIN_NORM_SAMT_TOT
               , A.ORIGIN_EVT_SAMT_TOT
               , A.ORIGIN_SALE_TAMT_TOT
               , A.ORIGIN_PROF_TAMT_TOT
               , 100                         AS ORIGIN_SALE_CRATE_TOT
               , 100                         AS ORIGIN_PROF_CRATE_TOT
             
               , A.ORIGIN_NORM_SAMT_JAN
               , A.ORIGIN_EVT_SAMT_JAN
               , A.ORIGIN_SALE_TAMT_JAN
               , A.ORIGIN_PROF_TAMT_JAN
               , A.ORIGIN_SALE_CRATE_JAN
               , A.ORIGIN_PROF_CRATE_JAN
             
               , A.ORIGIN_NORM_SAMT_FEB
               , A.ORIGIN_EVT_SAMT_FEB
               , A.ORIGIN_SALE_TAMT_FEB
               , A.ORIGIN_PROF_TAMT_FEB
               , A.ORIGIN_SALE_CRATE_FEB
               , A.ORIGIN_PROF_CRATE_FEB
               
               , A.ORIGIN_NORM_SAMT_MAR
               , A.ORIGIN_EVT_SAMT_MAR
               , A.ORIGIN_SALE_TAMT_MAR
               , A.ORIGIN_PROF_TAMT_MAR
               , A.ORIGIN_SALE_CRATE_MAR
               , A.ORIGIN_PROF_CRATE_MAR

               , A.ORIGIN_NORM_SAMT_APR
               , A.ORIGIN_EVT_SAMT_APR
               , A.ORIGIN_SALE_TAMT_APR
               , A.ORIGIN_PROF_TAMT_APR
               , A.ORIGIN_SALE_CRATE_APR
               , A.ORIGIN_PROF_CRATE_APR

               , A.ORIGIN_NORM_SAMT_MAY
               , A.ORIGIN_EVT_SAMT_MAY
               , A.ORIGIN_SALE_TAMT_MAY
               , A.ORIGIN_PROF_TAMT_MAY
               , A.ORIGIN_SALE_CRATE_MAY
               , A.ORIGIN_PROF_CRATE_MAY

               , A.ORIGIN_NORM_SAMT_JUN
               , A.ORIGIN_EVT_SAMT_JUN
               , A.ORIGIN_SALE_TAMT_JUN
               , A.ORIGIN_PROF_TAMT_JUN
               , A.ORIGIN_SALE_CRATE_JUN
               , A.ORIGIN_PROF_CRATE_JUN

               , A.ORIGIN_NORM_SAMT_JUL
               , A.ORIGIN_EVT_SAMT_JUL
               , A.ORIGIN_SALE_TAMT_JUL
               , A.ORIGIN_PROF_TAMT_JUL
               , A.ORIGIN_SALE_CRATE_JUL
               , A.ORIGIN_PROF_CRATE_JUL

               , A.ORIGIN_NORM_SAMT_AUG
               , A.ORIGIN_EVT_SAMT_AUG
               , A.ORIGIN_SALE_TAMT_AUG
               , A.ORIGIN_PROF_TAMT_AUG
               , A.ORIGIN_SALE_CRATE_AUG
               , A.ORIGIN_PROF_CRATE_AUG

               , A.ORIGIN_NORM_SAMT_SEP
               , A.ORIGIN_EVT_SAMT_SEP
               , A.ORIGIN_SALE_TAMT_SEP
               , A.ORIGIN_PROF_TAMT_SEP
               , A.ORIGIN_SALE_CRATE_SEP
               , A.ORIGIN_PROF_CRATE_SEP

               , A.ORIGIN_NORM_SAMT_OCT
               , A.ORIGIN_EVT_SAMT_OCT
               , A.ORIGIN_SALE_TAMT_OCT
               , A.ORIGIN_PROF_TAMT_OCT
               , A.ORIGIN_SALE_CRATE_OCT
               , A.ORIGIN_PROF_CRATE_OCT

               , A.ORIGIN_NORM_SAMT_NOV
               , A.ORIGIN_EVT_SAMT_NOV
               , A.ORIGIN_SALE_TAMT_NOV
               , A.ORIGIN_PROF_TAMT_NOV
               , A.ORIGIN_SALE_CRATE_NOV
               , A.ORIGIN_PROF_CRATE_NOV

               , A.ORIGIN_NORM_SAMT_DEC
               , A.ORIGIN_EVT_SAMT_DEC
               , A.ORIGIN_SALE_TAMT_DEC
               , A.ORIGIN_PROF_TAMT_DEC
               , A.ORIGIN_SALE_CRATE_DEC
               , A.ORIGIN_PROF_CRATE_DEC
            FROM (
                    SELECT A.CONF_FLAG
                         , A.STR_CD
                         , A.DEPT_CD
                         , A.TEAM_CD
                         , A.PC_CD
                         , A.ORG_CD
                         , SUM(A.ORIGIN_NORM_SAMT_TOT)                AS ORIGIN_NORM_SAMT_TOT
                         , SUM(A.ORIGIN_EVT_SAMT_TOT)                 AS ORIGIN_EVT_SAMT_TOT
                         , SUM(A.ORIGIN_SALE_TAMT_TOT)                AS ORIGIN_SALE_TAMT_TOT
                         , SUM(A.ORIGIN_PROF_TAMT_TOT)                AS ORIGIN_PROF_TAMT_TOT
                         , SUM(A.ORIGIN_SALE_CRATE_TOT)               AS ORIGIN_SALE_CRATE_TOT
                         , SUM(A.ORIGIN_PROF_CRATE_TOT)               AS ORIGIN_PROF_CRATE_TOT
                         
                         , SUM(A.ORIGIN_NORM_SAMT_JAN)                AS ORIGIN_NORM_SAMT_JAN
                         , SUM(A.ORIGIN_EVT_SAMT_JAN)                 AS ORIGIN_EVT_SAMT_JAN
                         , SUM(A.ORIGIN_SALE_TAMT_JAN)                AS ORIGIN_SALE_TAMT_JAN
                         , SUM(A.ORIGIN_PROF_TAMT_JAN)                AS ORIGIN_PROF_TAMT_JAN
                         , SUM(A.ORIGIN_SALE_CRATE_JAN)               AS ORIGIN_SALE_CRATE_JAN
                         , SUM(A.ORIGIN_PROF_CRATE_JAN)               AS ORIGIN_PROF_CRATE_JAN
                         
                         , SUM(A.ORIGIN_NORM_SAMT_FEB)                AS ORIGIN_NORM_SAMT_FEB
                         , SUM(A.ORIGIN_EVT_SAMT_FEB)                 AS ORIGIN_EVT_SAMT_FEB
                         , SUM(A.ORIGIN_SALE_TAMT_FEB)                AS ORIGIN_SALE_TAMT_FEB
                         , SUM(A.ORIGIN_PROF_TAMT_FEB)                AS ORIGIN_PROF_TAMT_FEB
                         , SUM(A.ORIGIN_SALE_CRATE_FEB)               AS ORIGIN_SALE_CRATE_FEB
                         , SUM(A.ORIGIN_PROF_CRATE_FEB)               AS ORIGIN_PROF_CRATE_FEB
                         
                         , SUM(A.ORIGIN_NORM_SAMT_MAR)                AS ORIGIN_NORM_SAMT_MAR
                         , SUM(A.ORIGIN_EVT_SAMT_MAR)                 AS ORIGIN_EVT_SAMT_MAR
                         , SUM(A.ORIGIN_SALE_TAMT_MAR)                AS ORIGIN_SALE_TAMT_MAR
                         , SUM(A.ORIGIN_PROF_TAMT_MAR)                AS ORIGIN_PROF_TAMT_MAR
                         , SUM(A.ORIGIN_SALE_CRATE_MAR)               AS ORIGIN_SALE_CRATE_MAR
                         , SUM(A.ORIGIN_PROF_CRATE_MAR)               AS ORIGIN_PROF_CRATE_MAR
                         
                         , SUM(A.ORIGIN_NORM_SAMT_APR)                AS ORIGIN_NORM_SAMT_APR
                         , SUM(A.ORIGIN_EVT_SAMT_APR)                 AS ORIGIN_EVT_SAMT_APR
                         , SUM(A.ORIGIN_SALE_TAMT_APR)                AS ORIGIN_SALE_TAMT_APR
                         , SUM(A.ORIGIN_PROF_TAMT_APR)                AS ORIGIN_PROF_TAMT_APR
                         , SUM(A.ORIGIN_SALE_CRATE_APR)               AS ORIGIN_SALE_CRATE_APR
                         , SUM(A.ORIGIN_PROF_CRATE_APR)               AS ORIGIN_PROF_CRATE_APR
                         
                         , SUM(A.ORIGIN_NORM_SAMT_MAY)                AS ORIGIN_NORM_SAMT_MAY
                         , SUM(A.ORIGIN_EVT_SAMT_MAY)                 AS ORIGIN_EVT_SAMT_MAY
                         , SUM(A.ORIGIN_SALE_TAMT_MAY)                AS ORIGIN_SALE_TAMT_MAY
                         , SUM(A.ORIGIN_PROF_TAMT_MAY)                AS ORIGIN_PROF_TAMT_MAY
                         , SUM(A.ORIGIN_SALE_CRATE_MAY)               AS ORIGIN_SALE_CRATE_MAY
                         , SUM(A.ORIGIN_PROF_CRATE_MAY)               AS ORIGIN_PROF_CRATE_MAY
                         
                         , SUM(A.ORIGIN_NORM_SAMT_JUN)                AS ORIGIN_NORM_SAMT_JUN
                         , SUM(A.ORIGIN_EVT_SAMT_JUN)                 AS ORIGIN_EVT_SAMT_JUN
                         , SUM(A.ORIGIN_SALE_TAMT_JUN)                AS ORIGIN_SALE_TAMT_JUN
                         , SUM(A.ORIGIN_PROF_TAMT_JUN)                AS ORIGIN_PROF_TAMT_JUN
                         , SUM(A.ORIGIN_SALE_CRATE_JUN)               AS ORIGIN_SALE_CRATE_JUN
                         , SUM(A.ORIGIN_PROF_CRATE_JUN)               AS ORIGIN_PROF_CRATE_JUN
                         
                         , SUM(A.ORIGIN_NORM_SAMT_JUL)                AS ORIGIN_NORM_SAMT_JUL
                         , SUM(A.ORIGIN_EVT_SAMT_JUL)                 AS ORIGIN_EVT_SAMT_JUL
                         , SUM(A.ORIGIN_SALE_TAMT_JUL)                AS ORIGIN_SALE_TAMT_JUL
                         , SUM(A.ORIGIN_PROF_TAMT_JUL)                AS ORIGIN_PROF_TAMT_JUL
                         , SUM(A.ORIGIN_SALE_CRATE_JUL)               AS ORIGIN_SALE_CRATE_JUL
                         , SUM(A.ORIGIN_PROF_CRATE_JUL)               AS ORIGIN_PROF_CRATE_JUL
                         
                         , SUM(A.ORIGIN_NORM_SAMT_AUG)                AS ORIGIN_NORM_SAMT_AUG
                         , SUM(A.ORIGIN_EVT_SAMT_AUG)                 AS ORIGIN_EVT_SAMT_AUG
                         , SUM(A.ORIGIN_SALE_TAMT_AUG)                AS ORIGIN_SALE_TAMT_AUG
                         , SUM(A.ORIGIN_PROF_TAMT_AUG)                AS ORIGIN_PROF_TAMT_AUG
                         , SUM(A.ORIGIN_SALE_CRATE_AUG)               AS ORIGIN_SALE_CRATE_AUG
                         , SUM(A.ORIGIN_PROF_CRATE_AUG)               AS ORIGIN_PROF_CRATE_AUG
                         
                         , SUM(A.ORIGIN_NORM_SAMT_SEP)                AS ORIGIN_NORM_SAMT_SEP
                         , SUM(A.ORIGIN_EVT_SAMT_SEP)                 AS ORIGIN_EVT_SAMT_SEP
                         , SUM(A.ORIGIN_SALE_TAMT_SEP)                AS ORIGIN_SALE_TAMT_SEP
                         , SUM(A.ORIGIN_PROF_TAMT_SEP)                AS ORIGIN_PROF_TAMT_SEP
                         , SUM(A.ORIGIN_SALE_CRATE_SEP)               AS ORIGIN_SALE_CRATE_SEP
                         , SUM(A.ORIGIN_PROF_CRATE_SEP)               AS ORIGIN_PROF_CRATE_SEP
                         
                         , SUM(A.ORIGIN_NORM_SAMT_OCT)                AS ORIGIN_NORM_SAMT_OCT
                         , SUM(A.ORIGIN_EVT_SAMT_OCT)                 AS ORIGIN_EVT_SAMT_OCT
                         , SUM(A.ORIGIN_SALE_TAMT_OCT)                AS ORIGIN_SALE_TAMT_OCT
                         , SUM(A.ORIGIN_PROF_TAMT_OCT)                AS ORIGIN_PROF_TAMT_OCT
                         , SUM(A.ORIGIN_SALE_CRATE_OCT)               AS ORIGIN_SALE_CRATE_OCT
                         , SUM(A.ORIGIN_PROF_CRATE_OCT)               AS ORIGIN_PROF_CRATE_OCT
                         
                         , SUM(A.ORIGIN_NORM_SAMT_NOV)                AS ORIGIN_NORM_SAMT_NOV
                         , SUM(A.ORIGIN_EVT_SAMT_NOV)                 AS ORIGIN_EVT_SAMT_NOV
                         , SUM(A.ORIGIN_SALE_TAMT_NOV)                AS ORIGIN_SALE_TAMT_NOV
                         , SUM(A.ORIGIN_PROF_TAMT_NOV)                AS ORIGIN_PROF_TAMT_NOV
                         , SUM(A.ORIGIN_SALE_CRATE_NOV)               AS ORIGIN_SALE_CRATE_NOV
                         , SUM(A.ORIGIN_PROF_CRATE_NOV)               AS ORIGIN_PROF_CRATE_NOV
                         
                         , SUM(A.ORIGIN_NORM_SAMT_DEC)                AS ORIGIN_NORM_SAMT_DEC
                         , SUM(A.ORIGIN_EVT_SAMT_DEC)                 AS ORIGIN_EVT_SAMT_DEC
                         , SUM(A.ORIGIN_SALE_TAMT_DEC)                AS ORIGIN_SALE_TAMT_DEC
                         , SUM(A.ORIGIN_PROF_TAMT_DEC)                AS ORIGIN_PROF_TAMT_DEC
                         , SUM(A.ORIGIN_SALE_CRATE_DEC)               AS ORIGIN_SALE_CRATE_DEC
                         , SUM(A.ORIGIN_PROF_CRATE_DEC)               AS ORIGIN_PROF_CRATE_DEC
                         
                      FROM (
                            SELECT (CASE WHEN NVL(A.CONF_FLAG, 'N') = 'N' THEN 'F' ELSE 'T' END)                         AS CONF_FLAG
                                 , A.STR_CD
                                 , A.DEPT_CD
                                 , A.TEAM_CD
                                 , A.PC_CD
                                 , A.ORG_CD
                                 , SUM(A.ORIGIN_NORM_SAMT)                                                      AS ORIGIN_NORM_SAMT_TOT
                                 , SUM(A.ORIGIN_EVT_SAMT)                                                       AS ORIGIN_EVT_SAMT_TOT
                                 , SUM(A.ORIGIN_SALE_TAMT)                                                      AS ORIGIN_SALE_TAMT_TOT
                                 , SUM(A.ORIGIN_PROF_TAMT)                                                      AS ORIGIN_PROF_TAMT_TOT
                                 , SUM(A.ORIGIN_SALE_CRATE)                                                     AS ORIGIN_SALE_CRATE_TOT
                                 , SUM(A.ORIGIN_PROF_CRATE)                                                     AS ORIGIN_PROF_CRATE_TOT
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '01' THEN A.ORIGIN_NORM_SAMT ELSE 0 END)  AS ORIGIN_NORM_SAMT_JAN
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '01' THEN A.ORIGIN_EVT_SAMT ELSE 0 END)   AS ORIGIN_EVT_SAMT_JAN
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '01' THEN A.ORIGIN_SALE_TAMT ELSE 0 END)  AS ORIGIN_SALE_TAMT_JAN
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '01' THEN A.ORIGIN_PROF_TAMT ELSE 0 END)  AS ORIGIN_PROF_TAMT_JAN
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '01' THEN A.ORIGIN_SALE_CRATE ELSE 0 END) AS ORIGIN_SALE_CRATE_JAN
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '01' THEN A.ORIGIN_PROF_CRATE ELSE 0 END) AS ORIGIN_PROF_CRATE_JAN
                                 
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '02' THEN A.ORIGIN_NORM_SAMT ELSE 0 END)  AS ORIGIN_NORM_SAMT_FEB
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '02' THEN A.ORIGIN_EVT_SAMT ELSE 0 END)   AS ORIGIN_EVT_SAMT_FEB
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '02' THEN A.ORIGIN_SALE_TAMT ELSE 0 END)  AS ORIGIN_SALE_TAMT_FEB
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '02' THEN A.ORIGIN_PROF_TAMT ELSE 0 END)  AS ORIGIN_PROF_TAMT_FEB
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '02' THEN A.ORIGIN_SALE_CRATE ELSE 0 END) AS ORIGIN_SALE_CRATE_FEB
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '02' THEN A.ORIGIN_PROF_CRATE ELSE 0 END) AS ORIGIN_PROF_CRATE_FEB
                                 
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '03' THEN A.ORIGIN_NORM_SAMT ELSE 0 END)  AS ORIGIN_NORM_SAMT_MAR
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '03' THEN A.ORIGIN_EVT_SAMT ELSE 0 END)   AS ORIGIN_EVT_SAMT_MAR
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '03' THEN A.ORIGIN_SALE_TAMT ELSE 0 END)  AS ORIGIN_SALE_TAMT_MAR
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '03' THEN A.ORIGIN_PROF_TAMT ELSE 0 END)  AS ORIGIN_PROF_TAMT_MAR
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '03' THEN A.ORIGIN_SALE_CRATE ELSE 0 END) AS ORIGIN_SALE_CRATE_MAR
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '03' THEN A.ORIGIN_PROF_CRATE ELSE 0 END) AS ORIGIN_PROF_CRATE_MAR
                                 
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '04' THEN A.ORIGIN_NORM_SAMT ELSE 0 END)  AS ORIGIN_NORM_SAMT_APR
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '04' THEN A.ORIGIN_EVT_SAMT ELSE 0 END)   AS ORIGIN_EVT_SAMT_APR
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '04' THEN A.ORIGIN_SALE_TAMT ELSE 0 END)  AS ORIGIN_SALE_TAMT_APR
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '04' THEN A.ORIGIN_PROF_TAMT ELSE 0 END)  AS ORIGIN_PROF_TAMT_APR
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '04' THEN A.ORIGIN_SALE_CRATE ELSE 0 END) AS ORIGIN_SALE_CRATE_APR
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '04' THEN A.ORIGIN_PROF_CRATE ELSE 0 END) AS ORIGIN_PROF_CRATE_APR
                                 
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '05' THEN A.ORIGIN_NORM_SAMT ELSE 0 END)  AS ORIGIN_NORM_SAMT_MAY
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '05' THEN A.ORIGIN_EVT_SAMT ELSE 0 END)   AS ORIGIN_EVT_SAMT_MAY
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '05' THEN A.ORIGIN_SALE_TAMT ELSE 0 END)  AS ORIGIN_SALE_TAMT_MAY
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '05' THEN A.ORIGIN_PROF_TAMT ELSE 0 END)  AS ORIGIN_PROF_TAMT_MAY
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '05' THEN A.ORIGIN_SALE_CRATE ELSE 0 END) AS ORIGIN_SALE_CRATE_MAY
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '05' THEN A.ORIGIN_PROF_CRATE ELSE 0 END) AS ORIGIN_PROF_CRATE_MAY
                                 
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '06' THEN A.ORIGIN_NORM_SAMT ELSE 0 END)  AS ORIGIN_NORM_SAMT_JUN
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '06' THEN A.ORIGIN_EVT_SAMT ELSE 0 END)   AS ORIGIN_EVT_SAMT_JUN
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '06' THEN A.ORIGIN_SALE_TAMT ELSE 0 END)  AS ORIGIN_SALE_TAMT_JUN
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '06' THEN A.ORIGIN_PROF_TAMT ELSE 0 END)  AS ORIGIN_PROF_TAMT_JUN
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '06' THEN A.ORIGIN_SALE_CRATE ELSE 0 END) AS ORIGIN_SALE_CRATE_JUN
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '06' THEN A.ORIGIN_PROF_CRATE ELSE 0 END) AS ORIGIN_PROF_CRATE_JUN
                                 
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '07' THEN A.ORIGIN_NORM_SAMT ELSE 0 END)  AS ORIGIN_NORM_SAMT_JUL
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '07' THEN A.ORIGIN_EVT_SAMT ELSE 0 END)   AS ORIGIN_EVT_SAMT_JUL
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '07' THEN A.ORIGIN_SALE_TAMT ELSE 0 END)  AS ORIGIN_SALE_TAMT_JUL
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '07' THEN A.ORIGIN_PROF_TAMT ELSE 0 END)  AS ORIGIN_PROF_TAMT_JUL
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '07' THEN A.ORIGIN_SALE_CRATE ELSE 0 END) AS ORIGIN_SALE_CRATE_JUL
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '07' THEN A.ORIGIN_PROF_CRATE ELSE 0 END) AS ORIGIN_PROF_CRATE_JUL
                                 
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '08' THEN A.ORIGIN_NORM_SAMT ELSE 0 END)  AS ORIGIN_NORM_SAMT_AUG
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '08' THEN A.ORIGIN_EVT_SAMT ELSE 0 END)   AS ORIGIN_EVT_SAMT_AUG
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '08' THEN A.ORIGIN_SALE_TAMT ELSE 0 END)  AS ORIGIN_SALE_TAMT_AUG
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '08' THEN A.ORIGIN_PROF_TAMT ELSE 0 END)  AS ORIGIN_PROF_TAMT_AUG
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '08' THEN A.ORIGIN_SALE_CRATE ELSE 0 END) AS ORIGIN_SALE_CRATE_AUG
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '08' THEN A.ORIGIN_PROF_CRATE ELSE 0 END) AS ORIGIN_PROF_CRATE_AUG
                                 
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '09' THEN A.ORIGIN_NORM_SAMT ELSE 0 END)  AS ORIGIN_NORM_SAMT_SEP
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '09' THEN A.ORIGIN_EVT_SAMT ELSE 0 END)   AS ORIGIN_EVT_SAMT_SEP
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '09' THEN A.ORIGIN_SALE_TAMT ELSE 0 END)  AS ORIGIN_SALE_TAMT_SEP
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '09' THEN A.ORIGIN_PROF_TAMT ELSE 0 END)  AS ORIGIN_PROF_TAMT_SEP
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '09' THEN A.ORIGIN_SALE_CRATE ELSE 0 END) AS ORIGIN_SALE_CRATE_SEP
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '09' THEN A.ORIGIN_PROF_CRATE ELSE 0 END) AS ORIGIN_PROF_CRATE_SEP
                                 
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '10' THEN A.ORIGIN_NORM_SAMT ELSE 0 END)  AS ORIGIN_NORM_SAMT_OCT
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '10' THEN A.ORIGIN_EVT_SAMT ELSE 0 END)   AS ORIGIN_EVT_SAMT_OCT
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '10' THEN A.ORIGIN_SALE_TAMT ELSE 0 END)  AS ORIGIN_SALE_TAMT_OCT
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '10' THEN A.ORIGIN_PROF_TAMT ELSE 0 END)  AS ORIGIN_PROF_TAMT_OCT
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '10' THEN A.ORIGIN_SALE_CRATE ELSE 0 END) AS ORIGIN_SALE_CRATE_OCT
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '10' THEN A.ORIGIN_PROF_CRATE ELSE 0 END) AS ORIGIN_PROF_CRATE_OCT
                                 
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '11' THEN A.ORIGIN_NORM_SAMT ELSE 0 END)  AS ORIGIN_NORM_SAMT_NOV
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '11' THEN A.ORIGIN_EVT_SAMT ELSE 0 END)   AS ORIGIN_EVT_SAMT_NOV
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '11' THEN A.ORIGIN_SALE_TAMT ELSE 0 END)  AS ORIGIN_SALE_TAMT_NOV
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '11' THEN A.ORIGIN_PROF_TAMT ELSE 0 END)  AS ORIGIN_PROF_TAMT_NOV
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '11' THEN A.ORIGIN_SALE_CRATE ELSE 0 END) AS ORIGIN_SALE_CRATE_NOV
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '11' THEN A.ORIGIN_PROF_CRATE ELSE 0 END) AS ORIGIN_PROF_CRATE_NOV
                                 
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '12' THEN A.ORIGIN_NORM_SAMT ELSE 0 END)  AS ORIGIN_NORM_SAMT_DEC
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '12' THEN A.ORIGIN_EVT_SAMT ELSE 0 END)   AS ORIGIN_EVT_SAMT_DEC
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '12' THEN A.ORIGIN_SALE_TAMT ELSE 0 END)  AS ORIGIN_SALE_TAMT_DEC
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '12' THEN A.ORIGIN_PROF_TAMT ELSE 0 END)  AS ORIGIN_PROF_TAMT_DEC
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '12' THEN A.ORIGIN_SALE_CRATE ELSE 0 END) AS ORIGIN_SALE_CRATE_DEC
                                 , (CASE WHEN SUBSTR(A.PLAN_YM,5,2) = '12' THEN A.ORIGIN_PROF_CRATE ELSE 0 END) AS ORIGIN_PROF_CRATE_DEC
                              FROM DPS.PS_PLANPC A
                             WHERE A.STR_CD    = ?
                               AND A.DEPT_CD   = ?
                               AND A.TEAM_CD   = ?
                               AND A.PC_CD     LIKE ? || '%'
                               AND A.PLAN_YM   LIKE ? || '%'
                             GROUP BY
                                   A.STR_CD
                                 , A.DEPT_CD
                                 , A.TEAM_CD
                                 , A.PC_CD
                                 , A.ORG_CD
                                 , A.PLAN_YM
                                 , A.CONF_FLAG
                                 , A.ORIGIN_NORM_SAMT
                                 , A.ORIGIN_EVT_SAMT
                                 , A.ORIGIN_SALE_TAMT
                                 , A.ORIGIN_PROF_TAMT
                                 , A.ORIGIN_SALE_CRATE
                                 , A.ORIGIN_PROF_CRATE
                           ) A
                     GROUP BY
                           A.STR_CD
                         , A.DEPT_CD
                         , A.TEAM_CD
                         , A.PC_CD
                         , A.ORG_CD
                         , A.CONF_FLAG
                     ORDER BY
                           A.STR_CD
                         , A.DEPT_CD
                         , A.TEAM_CD
                         , A.PC_CD
                         , A.ORG_CD
                 ) A
               , DPS.PC_ORGMST        B
               , DPS.PC_STRMST        C
           WHERE A.ORG_CD     = B.ORG_CD
             AND A.STR_CD     = B.STR_CD
             AND A.STR_CD     = C.STR_CD
             AND B.ORG_FLAG   = ?
             AND EXISTS ( SELECT MYORG.USER_ID
                            FROM COM.V_MYORG MYORG
                           WHERE MYORG.STR_CD      = A.STR_CD
                             AND MYORG.DEPT_CD     = A.DEPT_CD
                             AND MYORG.TEAM_CD     = A.TEAM_CD
                             AND MYORG.PC_CD       = A.PC_CD
                             AND MYORG.USER_ID     = ?
                             )
           ORDER BY
                 A.STR_CD
               , A.DEPT_CD
               , A.TEAM_CD
               , A.PC_CD
            
               
            
        ]]>        
    </query>
    
    
    
    <!-- 당초PC별월매출계획 조회 쿼리  -->
    <query name="SEL_PLANPUMBUN_SELORIGIN">        
        <![CDATA[
            SELECT A.STR_CD
                 , C.STR_NAME
                 , A.ORG_CD
                 , A.PLAN_YM
                 , A.ORG_LEVEL
                 , A.DEPT_CD
                 , D.ORG_SHORT_NAME                 AS DEPT_ORG_NAME
                 , A.TEAM_CD
                 , E.ORG_SHORT_NAME                 AS TEAM_ORG_NAME
                 , A.PC_CD
                 , B.ORG_SHORT_NAME                 AS PC_ORG_NAME
                 , A.PUMBUN_CD
                 , G.PUMBUN_NAME
                 , A.BFYY_NORM_SAMT
                 , A.BFYY_EVT_SAMT
                 , A.BFYY_SALE_TAMT
                 , A.BFYY_PROF_TAMT
                 , A.BFYY_SALE_CRATE
                 , A.BFYY_PROF_CRATE
                 , A.ORIGIN_NORM_SAMT
                 , A.ORIGIN_EVT_SAMT
                 , A.ORIGIN_SALE_TAMT
                 , A.ORIGIN_PROF_TAMT
                 , A.ORIGIN_SALE_CRATE
                 , A.ORIGIN_PROF_CRATE
                 , A.ORIGIN_SALE_IRATE
                 , A.ORIGIN_PROF_IRATE
                 , ''                               AS ERROR_CHECK
                 , NVL(A.CONF_FLAG,'N')             AS CONF_FLAG
              FROM DPS.PS_PLANPUMBUN    A
                 , DPS.PC_ORGMST    B
                 , DPS.PC_STRMST    C
                 , ( SELECT STR_CD
                          , DEPT_CD
                          , ORG_SHORT_NAME
                       FROM DPS.PC_ORGMST
                      WHERE STR_CD    = ?
                        AND DEPT_CD   = ?
                        AND ORG_CD    = ? || ? || '000000') D
                 , ( SELECT STR_CD
                          , DEPT_CD
                          , TEAM_CD
                          , ORG_SHORT_NAME
                       FROM DPS.PC_ORGMST
                      WHERE STR_CD    = ?
                        AND DEPT_CD   = ?
                        AND TEAM_CD   = ?
                        AND ORG_CD    = ? || ? || ? || '0000') E
                 , DPS.PC_STRPBN   G
             WHERE A.STR_CD    = B.STR_CD
               AND A.STR_CD    = C.STR_CD
               AND A.ORG_CD    = B.ORG_CD
               AND A.STR_CD    = D.STR_CD
               AND A.DEPT_CD   = D.DEPT_CD
               AND A.STR_CD    = E.STR_CD
               AND A.DEPT_CD   = E.DEPT_CD
               AND A.TEAM_CD   = E.TEAM_CD
               AND A.STR_CD    = G.STR_CD
               AND A.PUMBUN_CD = G.PUMBUN_CD
               AND A.ORG_CD    = SUBSTR(G.SALE_ORG_CD,1,8) || '00'
               AND G.BIZ_TYPE  != '5'
               AND A.STR_CD    = ?
               AND A.DEPT_CD   = ?
               AND A.TEAM_CD   = ?
               AND A.PC_CD     = ?
               AND A.PLAN_YM   LIKE ? || '%'
               AND A.ORG_LEVEL = '4'
               AND B.ORG_FLAG  = ?
               AND EXISTS ( SELECT MYORG.USER_ID
                              FROM COM.V_MYORG MYORG
                             WHERE MYORG.STR_CD      = A.STR_CD
                               AND MYORG.DEPT_CD     = A.DEPT_CD
                               AND MYORG.TEAM_CD     = A.TEAM_CD
                               AND MYORG.PC_CD       = A.PC_CD
                               AND MYORG.PUMBUN_CD   = A.PUMBUN_CD
                               AND MYORG.USER_ID     = ?
