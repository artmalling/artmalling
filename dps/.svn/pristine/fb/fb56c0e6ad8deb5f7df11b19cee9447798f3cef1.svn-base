                                    , PUMMOK_CD      , SKU_TYPE        , STYLE_TYPE      , PLU_FLAG        , INPUT_PLU_CD  
                                    , MODEL_NO       , SALE_UNIT_CD    , CMP_SPEC_CD     , CMP_SPEC_UNIT   , BAS_SPEC_CD   
                                    , BAS_SPEC_UNIT  , ORIGIN_AREA_CD  , MAKER_CD        , SET_YN          , GIFT_FLAG    
                                    , GIFT_TYPE_CD   , GIFT_AMT_TYPE   , STYLE_CD        , COLOR_CD        , SIZE_CD
                                    , BOTTLE_CD      , REMARK          , USE_YN          , REG_DATE        , REG_ID
                                    , MOD_DATE       , MOD_ID        
                                     )
                               VALUES(?              , ?               , ?               , ?               , ?
                                    , ?              , ?               , ?               , ?               , ?  
                                    , ?              , ?               , ?               , ?               , ?  
                                    , ?              , ?               , ?               , ?               , ?    
                                    , ?              , ?               , ?               , ?               , ?
                                    , ?              , ?               , ?               , SYSDATE         , ?
                                    , SYSDATE        , ? 
                                     ) 
        ]]>
    </query>
    <!-- 스타일  마스터 신규 입력  -->
    <query name="INS_STYLEMST">
        <![CDATA[
            INSERT INTO DPS.PC_STYLEMST(PUMBUN_CD      , STYLE_CD       , STYLE_NAME     , RECP_NAME      , PUMMOK_CD
                                      , BRAND_CD       , SUB_BRD_CD     , PLAN_YEAR      , SEASON_CD      , ITEM_CD
                                      , SEQ_NO         , MNG_CD1        , MNG_CD2        , MNG_CD3        , MNG_CD4 
                                      , MNG_CD5        , SIZE_TYPE      , MAKER_CD       , SEX_CD         , CURRENCY_CD
                                      , ORIGIN_AREA_CD , VEN_STYLE_CD   , STYLE_TYPE     , SALE_UNIT_CD   , BAS_SPEC_CD
                                      , BAS_SPEC_UNIT  , CMP_SPEC_CD    , CMP_SPEC_UNIT  , REMARK         , USE_YN
                                      , REG_DATE       , REG_ID         , MOD_DATE       , MOD_ID
                                       )
                                 VALUES(?              , ?              , ?              , ?              , ?                            
                                      , ?              , ?              , ?              , ?              , ?              
                                      , ?              , ?              , ?              , ?              , ?               
                                      , ?              , ?              , ?              , ?              , ?              
                                      , ?              , ?              , ?              , ?              , ?              
                                      , ?              , ?              , ?              , ?              , ?              
                                      , SYSDATE        , ?              , SYSDATE        , ?
                                       )              
                                       
        ]]>
    </query>
    
