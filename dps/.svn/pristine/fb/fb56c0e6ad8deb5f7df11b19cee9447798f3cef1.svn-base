        <column colName="ORIGIN_AREA_CD"           colType="TB_STRING"  size="100" dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
   </gauce> 
   
    <!-- 규격단품 검증 SQL -->
    <query name="SEL_STN_SKU">
        <![CDATA[                                        
            SELECT 'F'                              AS SEL
                 , DU.SKU_NAME
                 , DU.RECP_NAME
                 , DU.PUMMOK_CD
                 , PMK.PUMMOK_NAME                  AS PUMMOK_NAME
                 , DU.INPUT_PLU_CD
                 , DU.MODEL_CD
                 , DU.SALE_UNIT_CD
                 , UNT_SALE.CODE_NAME               AS SALE_UNIT_NAME
                 , DU.BAS_SPEC_CD
                 , DU.BAS_SPEC_UNIT_CD
                 , UNT_BAS.CODE_NAME                AS BAS_SPEC_UNIT_NAME
                 , DU.CMP_SPEC_CD
                 , DU.CMP_SPEC_UNIT_CD
                 , UNT_CMP.CODE_NAME                AS CMP_SPEC_UNIT_NAME
                 , DU.ORIGIN_AREA_CD
                 , ORGN.CODE_NAME                   AS ORIGIN_AREA_NAME
                 , DU.SKU_CD
                 , DU.CHECK_SUM
                 /* 값에 대한 검증 */
                 , CASE WHEN DU.PUMMOK_CD_LEN        IS NOT NULL THEN DU.PUMMOK_CD_LEN        /*자리수체크 */
                        WHEN DU.INPUT_PLU_CD_LEN     IS NOT NULL THEN DU.INPUT_PLU_CD_LEN     /*자리수체크*/
                        WHEN DU.MODEL_CD_LEN         IS NOT NULL THEN DU.MODEL_CD_LEN         /*자리수체크*/
                        WHEN DU.SALE_UNIT_CD_LEN     IS NOT NULL THEN DU.SALE_UNIT_CD_LEN     /*자리수체크*/
                        WHEN DU.BAS_SPEC_CD_LEN      IS NOT NULL THEN DU.BAS_SPEC_CD_LEN      /*자리수체크*/
                        WHEN DU.BAS_SPEC_UNIT_CD_LEN IS NOT NULL THEN DU.BAS_SPEC_UNIT_CD_LEN /*자리수체크*/
                        WHEN DU.CMP_SPEC_CD_LEN      IS NOT NULL THEN DU.CMP_SPEC_CD_LEN      /*자리수체크*/
                        WHEN DU.CMP_SPEC_UNIT_CD_LEN IS NOT NULL THEN DU.CMP_SPEC_UNIT_CD_LEN /*자리수체크*/
                        WHEN DU.ORIGIN_AREA_CD_LEN   IS NOT NULL THEN DU.ORIGIN_AREA_CD_LEN   /*자리수체크*/
                        WHEN DU.SKU_NAME             IS NULL     THEN '024'                   /*단품명*/
                        WHEN DU.RECP_NAME            IS NULL     THEN '025'                   /*영수증명*/
                        WHEN DU.PUMMOK_CD            IS NULL     THEN '026'                   /*품목코드*/
                        WHEN DU.SALE_UNIT_CD         IS NULL     THEN '027'                   /*판매단위코드*/
                        WHEN PMK.PUMMOK_NAME         IS NULL     THEN '001'                   /*품목 검증*/
                        WHEN UNT_SALE.CODE_NAME      IS NULL     THEN '002'                   /*판매단위 검증*/
                        WHEN VSIZE(DU.SKU_NAME)      > 40        THEN '028'                   /*단품명 길이 검증*/
                        WHEN VSIZE(DU.RECP_NAME)     > 20        THEN '029'                   /*영수증명 길이 검증*/
                        WHEN SUBSTR(DU.SKU_CD,0,1)   = '2'       THEN '020'                   /*유효한 소스마킹코드  검증*/
                        WHEN NULLIF(
                                    SUBSTR(DU.SKU_CD,13)
                                  , DU.CHECK_SUM
                                   )                 IS NOT NULL THEN '020'                   /*유효한 소스마킹코드  검증*/
                        WHEN CASE WHEN DU.BAS_SPEC_UNIT_CD IS NOT NULL
                                  THEN UNT_BAS.CODE_NAME
                                  ELSE 'OK'
                             END                     IS NULL     THEN '003'                   /*기본규격단위 검증*/
                        WHEN CASE WHEN DU.CMP_SPEC_UNIT_CD IS NOT NULL
                                  THEN UNT_CMP.CODE_NAME
                                  ELSE 'OK'
                             END                     IS NULL     THEN '004'                   /*구성규격단위 검증*/
                        WHEN CASE WHEN DU.ORIGIN_AREA_CD IS NOT NULL
                                  THEN ORGN.CODE_NAME
                                  ELSE 'OK'
                             END                     IS NULL     THEN '005'                   /*원산지 검증*/
                        WHEN (SELECT COUNT(*) 
                                FROM DPS.PC_SKUMST 
                               WHERE SKU_CD  = DU.SKU_CD
                                 AND ROWNUM  = 1
                             ) > 0                               THEN '021'                   /*단품코드 중복 검증*/
                        WHEN (SELECT COUNT(*) 
                                FROM DPS.PC_BARCDMST 
                               WHERE BARCODE = DU.SKU_CD
                                 AND SKU_CD != DU.SKU_CD
                                 AND ROWNUM  = 1
                             ) > 0                               THEN '054'                   /*소스마킹코드(바코드) 중복 검증*/
                        WHEN (SELECT COUNT(*) 
                                FROM UPLOAD_EXCEL_DATA
                               WHERE SKU_CD  = DU.SKU_CD
                                 AND ROWNUM  = 1
                             ) > 1                               THEN '045'                   /*로드한 파일내 단품코드 중복 검증*/
			            WHEN (SELECT COUNT(*) 
			                    FROM DPS.PC_SKUMST 
			                   WHERE MODEL_NO  = DU.MODEL_CD
			                     AND ROWNUM  = 1
			                 ) > 0                               THEN '201'                   /*브랜드 상품코드 중복*/
                        ELSE ''
                   END                              AS ERROR_CODE
              FROM UPLOAD_EXCEL_DATA DU
                 , (SELECT S_PMK.PUMMOK_CD
                         , S_PMK.PUMMOK_NAME
                      FROM DPS.PC_PBNPMK S_PPK
                         , DPS.PC_PMKMST S_PMK
                     WHERE S_PPK.PUMMOK_CD = S_PMK.PUMMOK_CD
                       AND S_PPK.PUMBUN_CD = ?
                   ) PMK
                 , (SELECT COMM_CODE
                         , COMM_NAME1  AS CODE_NAME
                      FROM COM.TC_COMMCODE
                     WHERE SYS_PART  = 'D'
                       AND COMM_PART = 'P013'
                       AND USE_YN    = 'Y'
                   ) UNT_SALE
                 , (SELECT COMM_CODE
                         , COMM_NAME1  AS CODE_NAME
                      FROM COM.TC_COMMCODE
                     WHERE SYS_PART  = 'D'
                       AND COMM_PART = 'P013'
                       AND USE_YN    = 'Y'
                   ) UNT_BAS
                 , (SELECT COMM_CODE
                         , COMM_NAME1  AS CODE_NAME
                      FROM COM.TC_COMMCODE
                     WHERE SYS_PART  = 'D'
                       AND COMM_PART = 'P013'
                       AND USE_YN    = 'Y'
                   ) UNT_CMP
                 , (SELECT COMM_CODE
                         , COMM_NAME1  AS CODE_NAME
                      FROM COM.TC_COMMCODE
                     WHERE SYS_PART  = 'D'
                       AND COMM_PART = 'P040'
                       AND USE_YN    = 'Y'
                   ) ORGN
             WHERE DU.PUMMOK_CD        = PMK.PUMMOK_CD(+)
               AND DU.SALE_UNIT_CD     = UNT_SALE.COMM_CODE(+)
               AND DU.BAS_SPEC_UNIT_CD = UNT_BAS.COMM_CODE(+)
               AND DU.CMP_SPEC_UNIT_CD = UNT_CMP.COMM_CODE(+)
               AND DU.ORIGIN_AREA_CD   = ORGN.COMM_CODE(+)
        ]]>
    </query>
    <query name="SEL_STN_SKU_ORDER">
        <![CDATA[
             ORDER BY ERROR_CODE, SKU_NAME
        ]]>
    </query>
    
    <!-- 신선단품 검증 SQL -->
    <query name="SEL_FRESH">
        <![CDATA[                                        
            SELECT 'F'                              AS SEL
                 , DU.SKU_NAME
                 , DU.RECP_NAME
                 , DU.PUMMOK_CD
                 , PMK.PUMMOK_NAME                  AS PUMMOK_NAME
                 , DU.SALE_UNIT_CD
                 , UNT_SALE.CODE_NAME               AS SALE_UNIT_NAME
                 , DU.BAS_SPEC_CD
                 , DU.BAS_SPEC_UNIT_CD
                 , UNT_BAS.CODE_NAME                AS BAS_SPEC_UNIT_NAME
                 , DU.CMP_SPEC_CD
                 , DU.CMP_SPEC_UNIT_CD
                 , UNT_CMP.CODE_NAME                AS CMP_SPEC_UNIT_NAME
                 , DU.ORIGIN_AREA_CD
                 , ORGN.CODE_NAME                   AS ORIGIN_AREA_NAME
                 /* 값에 대한 검증 */
                 , CASE WHEN DU.PUMMOK_CD_LEN        IS NOT NULL THEN DU.PUMMOK_CD_LEN        /*자리수체크*/
                        WHEN DU.SALE_UNIT_CD_LEN     IS NOT NULL THEN DU.SALE_UNIT_CD_LEN     /*자리수체크*/
                        WHEN DU.BAS_SPEC_CD_LEN      IS NOT NULL THEN DU.BAS_SPEC_CD_LEN      /*자리수체크*/
                        WHEN DU.BAS_SPEC_UNIT_CD_LEN IS NOT NULL THEN DU.BAS_SPEC_UNIT_CD_LEN /*자리수체크*/
                        WHEN DU.CMP_SPEC_CD_LEN      IS NOT NULL THEN DU.CMP_SPEC_CD_LEN      /*자리수체크*/
                        WHEN DU.CMP_SPEC_UNIT_CD_LEN IS NOT NULL THEN DU.CMP_SPEC_UNIT_CD_LEN /*자리수체크*/
                        WHEN DU.ORIGIN_AREA_CD_LEN   IS NOT NULL THEN DU.ORIGIN_AREA_CD_LEN   /*자리수체크*/
                        WHEN DU.SKU_NAME             IS NULL     THEN '024'                   /*단품명*/
                        WHEN DU.RECP_NAME            IS NULL     THEN '025'                   /*영수증명*/
                        WHEN DU.PUMMOK_CD            IS NULL     THEN '026'                   /*품목코드*/
                        WHEN DU.SALE_UNIT_CD         IS NULL     THEN '027'                   /*판매단위코드*/
                        WHEN DU.ORIGIN_AREA_CD       IS NULL     THEN '031'                   /*원산지코드*/
                        WHEN PMK.PUMMOK_NAME         IS NULL     THEN '001'                   /*품목 검증*/
                        WHEN UNT_SALE.CODE_NAME      IS NULL     THEN '002'                   /*판매단위 검증*/
                        WHEN VSIZE(DU.SKU_NAME)      > 40        THEN '028'                   /*단품명 길이 검증*/
                        WHEN VSIZE(DU.RECP_NAME)     > 20        THEN '029'                   /*영수증명 길이 검증*/
                        WHEN ORGN.CODE_NAME          IS NULL     THEN '005'                   /*원산지 검증*/
                        WHEN CASE WHEN DU.BAS_SPEC_UNIT_CD IS NOT NULL
                                  THEN UNT_BAS.CODE_NAME
                                  ELSE 'OK'
                             END                     IS NULL     THEN '003'                   /*기본규격단위 검증*/
                        WHEN CASE WHEN DU.CMP_SPEC_UNIT_CD IS NOT NULL
                                  THEN UNT_CMP.CODE_NAME
                                  ELSE 'OK'
                             END                     IS NULL     THEN '004'                   /*구성규격단위 검증*/
                        ELSE ''
                   END                              AS ERROR_CODE
              FROM UPLOAD_EXCEL_DATA DU
                 , (SELECT S_PMK.PUMMOK_CD
                         , S_PMK.PUMMOK_NAME
                      FROM DPS.PC_PBNPMK S_PPK
                         , DPS.PC_PMKMST S_PMK
                     WHERE S_PPK.PUMMOK_CD = S_PMK.PUMMOK_CD
                       AND S_PPK.PUMBUN_CD = ?
                   ) PMK
                 , (SELECT COMM_CODE
                         , COMM_NAME1  AS CODE_NAME
                      FROM COM.TC_COMMCODE
                     WHERE SYS_PART  = 'D'
                       AND COMM_PART = 'P013'
                       AND USE_YN    = 'Y'
                   ) UNT_SALE
                 , (SELECT COMM_CODE
                         , COMM_NAME1  AS CODE_NAME
                      FROM COM.TC_COMMCODE
                     WHERE SYS_PART  = 'D'
                       AND COMM_PART = 'P013'
                       AND USE_YN    = 'Y'
                   ) UNT_BAS
                 , (SELECT COMM_CODE
                         , COMM_NAME1  AS CODE_NAME
                      FROM COM.TC_COMMCODE
                     WHERE SYS_PART  = 'D'
                       AND COMM_PART = 'P013'
                       AND USE_YN    = 'Y'
                   ) UNT_CMP
                 , (SELECT COMM_CODE
                         , COMM_NAME1  AS CODE_NAME
                      FROM COM.TC_COMMCODE
                     WHERE SYS_PART  = 'D'
                       AND COMM_PART = 'P040'
                       AND USE_YN    = 'Y'
                   ) ORGN
             WHERE DU.PUMMOK_CD        = PMK.PUMMOK_CD(+)
               AND DU.SALE_UNIT_CD     = UNT_SALE.COMM_CODE(+)
               AND DU.BAS_SPEC_UNIT_CD = UNT_BAS.COMM_CODE(+)
               AND DU.CMP_SPEC_UNIT_CD = UNT_CMP.COMM_CODE(+)
               AND DU.ORIGIN_AREA_CD   = ORGN.COMM_CODE(+)
        ]]>
    </query>
    <query name="SEL_FRESH_ORDER">
        <![CDATA[
             ORDER BY ERROR_CODE, SKU_NAME
        ]]>
    </query>    
    
    <!-- 의류단품(A) 검증 SQL -->
    <query name="SEL_ASTYLE">
        <![CDATA[                                        
            SELECT 'F'                              AS SEL
                 , DU.STYLE_NAME
                 , DU.RECP_NAME
                 , DU.STYLE_CD
                 , DU.COLOR_CD
                 , COLOR.CODE_NAME                  AS COLOR_NAME
                 , DU.SIZE_CD
                 , SIZ.CODE_NAME                    AS SIZE_NAME
                 , DU.SIZE_TYPE_CD
                 , SIZE_TYPE.CODE_NAME              AS SIZE_TYPE_NAME
                 , DU.SEX_CD
                 , SEX.CODE_NAME                    AS SEX_NAME
                 , DU.PUMMOK_CD
                 , PMK.PUMMOK_NAME                  AS PUMMOK_NAME
                 , DU.INPUT_PLU_CD
                 , DU.VEN_STYLE_CD
                 , DU.SALE_UNIT_CD
                 , UNT_SALE.CODE_NAME               AS SALE_UNIT_NAME
                 , DU.BAS_SPEC_CD
                 , DU.BAS_SPEC_UNIT_CD
                 , UNT_BAS.CODE_NAME                AS BAS_SPEC_UNIT_NAME
                 , DU.CMP_SPEC_CD
                 , DU.CMP_SPEC_UNIT_CD
                 , UNT_CMP.CODE_NAME                AS CMP_SPEC_UNIT_NAME
                 , DU.ORIGIN_AREA_CD
                 , ORGN.CODE_NAME                   AS ORIGIN_AREA_NAME
                 , DU.BRAND_CD
                 , DU.SUB_BRD_CD 
                 , DU.PLAN_YEAR_CD
                 , DU.SEASON_CD
                 , DU.ITEM_CD
                 , DU.SEQ_NO
                 , DU.SKU_CD
                 , SUBSTRB(DU.STYLE_NAME,0,30)||' '||DU.COLOR_CD||'/'||SUBSTRB(SIZ.CODE_NAME,0,4) AS SKU_NAME
                 , DU.CHECK_SUM
                 /* 값에 대한 검증 */
                 , CASE WHEN DU.STYLE_CD_LEN         IS NOT NULL THEN DU.STYLE_CD_LEN         /*자리수체크*/
                        WHEN DU.COLOR_CD_LEN         IS NOT NULL THEN DU.COLOR_CD_LEN         /*자리수체크*/
                        WHEN DU.SIZE_CD_LEN          IS NOT NULL THEN DU.SIZE_CD_LEN          /*자리수체크*/
                        WHEN DU.SIZE_TYPE_CD_LEN     IS NOT NULL THEN DU.SIZE_TYPE_CD_LEN     /*자리수체크*/
                        WHEN DU.SEX_CD_LEN           IS NOT NULL THEN DU.SEX_CD_LEN           /*자리수체크*/
                        WHEN DU.PUMMOK_CD_LEN        IS NOT NULL THEN DU.PUMMOK_CD_LEN        /*자리수체크*/
                        WHEN DU.INPUT_PLU_CD_LEN     IS NOT NULL THEN DU.INPUT_PLU_CD_LEN     /*자리수체크*/
                        WHEN DU.VEN_STYLE_CD_LEN     IS NOT NULL THEN DU.VEN_STYLE_CD_LEN     /*자리수체크*/
                        WHEN DU.SALE_UNIT_CD_LEN     IS NOT NULL THEN DU.SALE_UNIT_CD_LEN     /*자리수체크*/
                        WHEN DU.BAS_SPEC_CD_LEN      IS NOT NULL THEN DU.BAS_SPEC_CD_LEN      /*자리수체크*/
                        WHEN DU.BAS_SPEC_UNIT_CD_LEN IS NOT NULL THEN DU.BAS_SPEC_UNIT_CD_LEN /*자리수체크*/
                        WHEN DU.CMP_SPEC_CD_LEN      IS NOT NULL THEN DU.CMP_SPEC_CD_LEN      /*자리수체크*/
                        WHEN DU.CMP_SPEC_UNIT_CD_LEN IS NOT NULL THEN DU.CMP_SPEC_UNIT_CD_LEN /*자리수체크*/
                        WHEN DU.ORIGIN_AREA_CD_LEN   IS NOT NULL THEN DU.ORIGIN_AREA_CD_LEN   /*자리수체크*/
                        WHEN DU.STYLE_NAME           IS NULL     THEN '032'                   /*스타일명*/
                        WHEN DU.RECP_NAME            IS NULL     THEN '025'                   /*영수증명*/
                        WHEN DU.STYLE_CD             IS NULL     THEN '033'                   /*스타일코드*/
                        WHEN DU.STYLE_CD_CHK         = 'N'       THEN '046'                   /*스타일코드 검증*/
                        WHEN BRAND.BRAND_NM          IS NULL     THEN '010'                   /*브랜드 검증*/
                        WHEN SUB_BRD.CODE_NAME       IS NULL     THEN '011'                   /*서브브랜드 검증*/
                        WHEN PLAN_YEAR.CODE_NAME     IS NULL     THEN '012'                   /*기획년도 검증*/
                        WHEN SEASON.CODE_NAME        IS NULL     THEN '013'                   /*시즌 검증*/
                        WHEN ITEM.CODE_NAME          IS NULL     THEN '014'                   /*아이템 검증*/
                        WHEN DU.COLOR_CD             IS NULL     THEN '034'                   /*칼라코드*/
                        WHEN DU.SIZE_CD              IS NULL     THEN '035'                   /*사이즈코드*/
                        WHEN DU.SIZE_TYPE_CD         IS NULL     THEN '036'                   /*사이즈타입코드*/
                        WHEN DU.SEX_CD               IS NULL     THEN '037'                   /*성별코드*/
                        WHEN DU.PUMMOK_CD            IS NULL     THEN '026'                   /*품목코드*/
                        WHEN DU.SALE_UNIT_CD         IS NULL     THEN '027'                   /*판매단위코드*/
                        WHEN COLOR.CODE_NAME         IS NULL     THEN '006'                   /*칼라 검증*/
                        WHEN SIZ.CODE_NAME           IS NULL     THEN '007'                   /*사이즈 검증*/
                        WHEN SIZE_TYPE.CODE_NAME     IS NULL     THEN '008'                   /*사이즈타입 검증*/
                        WHEN SEX.CODE_NAME           IS NULL     THEN '009'                   /*성별 검증*/
                        WHEN PMK.PUMMOK_NAME         IS NULL     THEN '001'                   /*품목 검증*/
                        WHEN UNT_SALE.CODE_NAME      IS NULL     THEN '002'                   /*판매단위 검증*/
                        WHEN VSIZE(DU.STYLE_NAME)    > 40        THEN '028'                   /*스타일명 길이 검증*/
                        WHEN VSIZE(DU.RECP_NAME)     > 20        THEN '029'                   /*영수증명 길이 검증*/
                        WHEN SUBSTR(DU.SKU_CD,0,1)   = '2'       THEN '020'                   /*유효한 소스마킹코드  검증*/
                        WHEN NULLIF(
                                    SUBSTR(DU.SKU_CD,13)
                                  , DU.CHECK_SUM
                                   )                 IS NOT NULL THEN '020'                   /*유효한 단품코드  검증*/
                        WHEN CASE WHEN DU.BAS_SPEC_UNIT_CD IS NOT NULL
                                  THEN UNT_BAS.CODE_NAME
                                  ELSE 'OK'
                             END                     IS NULL     THEN '003'                   /*기본규격단위 검증*/
                        WHEN CASE WHEN DU.CMP_SPEC_UNIT_CD IS NOT NULL
                                  THEN UNT_CMP.CODE_NAME
                                  ELSE 'OK'
                             END                     IS NULL     THEN '004'                   /*구성규격단위 검증*/
                        WHEN CASE WHEN DU.ORIGIN_AREA_CD IS NOT NULL
                                  THEN ORGN.CODE_NAME
                                  ELSE 'OK'
                             END                     IS NULL     THEN '005'                   /*원산지 검증*/
                        WHEN (SELECT COUNT(*) 
                                FROM DPS.PC_SKUMST
                               WHERE SKU_CD = DU.SKU_CD
                             ) > 0                               THEN '021'                   /*단품코드 중복 검증*/
                        WHEN (SELECT COUNT(*) 
                                FROM DPS.PC_BARCDMST 
                               WHERE BARCODE = DU.SKU_CD
                                 AND SKU_CD != DU.SKU_CD
                                 AND ROWNUM  = 1
                             ) > 0                               THEN '054'                   /*소스마킹코드(바코드) 중복 검증*/
                        WHEN (SELECT COUNT(*) 
                                FROM DPS.PC_STYLEMST 
                               WHERE PUMBUN_CD  = ?
                                 AND STYLE_CD   = DU.STYLE_CD
                             ) > 0                               THEN '022'                   /*스타일코드 중복 검증*/
                        WHEN (SELECT COUNT(*) 
                                FROM UPLOAD_EXCEL_DATA
                               WHERE SKU_CD = DU.SKU_CD
                             ) > 1                               THEN '045'                   /*로드한 데이터 단품코드 중복 검증*/
                        WHEN (SELECT COUNT(*) 
                                FROM UPLOAD_EXCEL_DATA
                               WHERE STYLE_CD  = DU.STYLE_CD
                                 AND COLOR_CD  = DU.COLOR_CD
                                 AND SIZE_CD   = DU.SIZE_CD
                             ) > 1                               THEN '045'                   /*로드한 데이터 스타일 중복 검증*/
                        WHEN (SELECT COUNT(*) 
                                FROM UPLOAD_EXCEL_DATA
                               WHERE STYLE_CD         =  DU.STYLE_CD
                                 AND (STYLE_NAME       <> DU.STYLE_NAME
                                      OR RECP_NAME        <> DU.RECP_NAME
                                      OR SIZE_TYPE_CD     <> DU.SIZE_TYPE_CD
                                      OR SEX_CD           <> DU.SEX_CD
                                      OR PUMMOK_CD        <> DU.PUMMOK_CD
                                      OR VEN_STYLE_CD     <> DU.VEN_STYLE_CD
                                      OR SALE_UNIT_CD     <> DU.SALE_UNIT_CD
                                      OR BAS_SPEC_CD      <> DU.BAS_SPEC_CD
                                      OR BAS_SPEC_UNIT_CD <> DU.BAS_SPEC_UNIT_CD
                                      OR CMP_SPEC_CD      <> DU.CMP_SPEC_CD
                                      OR CMP_SPEC_UNIT_CD <> DU.CMP_SPEC_UNIT_CD
                                      OR ORIGIN_AREA_CD   <> DU.ORIGIN_AREA_CD
                                     )
                             ) > 0                            THEN '023'    /*로드한 데이터 상세정보 검증*/
                        ELSE ''
                   END                              AS ERROR_CODE
              FROM UPLOAD_EXCEL_DATA DU
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P027' AND USE_YN='Y' ) SIZE_TYPE
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P032' AND USE_YN='Y' ) SEX
                 , DPS.PC_BRDMST BRAND
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P031' AND USE_YN='Y' ) SUB_BRD
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P012' AND USE_YN='Y' ) PLAN_YEAR
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P035' AND USE_YN='Y' ) SEASON
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P036' AND USE_YN='Y' ) ITEM
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P013' AND USE_YN='Y' ) UNT_SALE
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P013' AND USE_YN='Y' ) UNT_BAS
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P013' AND USE_YN='Y' ) UNT_CMP
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P040' AND USE_YN='Y' ) ORGN
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P062' AND USE_YN='Y' ) COLOR
                 , (SELECT COMM_CODE
                         , COMM_NAME1 AS CODE_NAME
                         , REFER_CODE
                      FROM COM.TC_COMMCODE
                     WHERE SYS_PART='D'
                       AND COMM_PART='P026'
                       AND USE_YN='Y'
                   ) SIZ
                 , (SELECT S_PMK.PUMMOK_CD
                         , S_PMK.PUMMOK_NAME
                      FROM DPS.PC_PBNPMK S_PPK
                         , DPS.PC_PMKMST S_PMK
                     WHERE S_PPK.PUMMOK_CD = S_PMK.PUMMOK_CD
                       AND S_PPK.PUMBUN_CD = ?
                   ) PMK
             WHERE DU.PUMMOK_CD        = PMK.PUMMOK_CD(+)
               AND DU.COLOR_CD         = COLOR.COMM_CODE(+)
               AND DU.SIZE_CD          = SIZ.COMM_CODE(+)
               AND DU.SIZE_TYPE_CD     = SIZ.REFER_CODE(+)
               AND DU.SIZE_TYPE_CD     = SIZE_TYPE.COMM_CODE(+)
               AND DU.SEX_CD           = SEX.COMM_CODE(+)
               AND DU.BRAND_CD         = BRAND.BRAND_CD(+)
               AND DU.SUB_BRD_CD       = SUB_BRD.COMM_CODE(+)
               AND DU.PLAN_YEAR_CD     = PLAN_YEAR.COMM_CODE(+)
               AND DU.SEASON_CD        = SEASON.COMM_CODE(+)
               AND DU.ITEM_CD          = ITEM.COMM_CODE(+)
               AND DU.SALE_UNIT_CD     = UNT_SALE.COMM_CODE(+)
               AND DU.BAS_SPEC_UNIT_CD = UNT_BAS.COMM_CODE(+)
               AND DU.CMP_SPEC_UNIT_CD = UNT_CMP.COMM_CODE(+)
               AND DU.ORIGIN_AREA_CD   = ORGN.COMM_CODE(+)
        ]]>
    </query>
    <query name="SEL_ASTYLE_ORDER">
        <![CDATA[
             ORDER BY ERROR_CODE, STYLE_NAME
        ]]>
    </query>    
    
    <!-- 의류단품(B) 검증 SQL -->
    <query name="SEL_BSTYLE">
        <![CDATA[                                        
            SELECT 'F'                              AS SEL
                 , DU.STYLE_NAME
                 , DU.RECP_NAME
                 , DU.STYLE_CD
                 , DU.BRAND_CD
                 , BRAND.BRAND_NM                 AS BRAND_NAME
                 , DU.SUB_BRD_CD
                 , SUB_BRD.CODE_NAME                AS SUB_BRD_NAME
                 , DU.MNG_CD1
                 , MNG1.CODE_NAME                   AS MNG_NAME1
                 , DU.MNG_CD2
                 , MNG2.CODE_NAME                   AS MNG_NAME2
                 , DU.MNG_CD3
                 , MNG3.CODE_NAME                   AS MNG_NAME3
                 , DU.MNG_CD4
                 , MNG4.CODE_NAME                   AS MNG_NAME4
                 , DU.MNG_CD5
                 , MNG5.CODE_NAME                   AS MNG_NAME5
                 , DU.SEX_CD
                 , SEX.CODE_NAME                    AS SEX_NAME
                 , DU.PUMMOK_CD
                 , PMK.PUMMOK_NAME                  AS PUMMOK_NAME
                 , DU.INPUT_PLU_CD
                 , DU.VEN_STYLE_CD
                 , DU.SALE_UNIT_CD
                 , UNT_SALE.CODE_NAME               AS SALE_UNIT_NAME
                 , DU.BAS_SPEC_CD
                 , DU.BAS_SPEC_UNIT_CD
                 , UNT_BAS.CODE_NAME                AS BAS_SPEC_UNIT_NAME
                 , DU.CMP_SPEC_CD
                 , DU.CMP_SPEC_UNIT_CD
                 , UNT_CMP.CODE_NAME                AS CMP_SPEC_UNIT_NAME
                 , DU.ORIGIN_AREA_CD
                 , ORGN.CODE_NAME                   AS ORIGIN_AREA_NAME
                 , DU.SKU_CD
                 , DU.CHECK_SUM
                 /* 값에 대한 검증 */
                 , CASE WHEN DU.BRAND_CD_LEN         IS NOT NULL THEN DU.BRAND_CD_LEN         /*자리수체크*/ 
                        WHEN DU.SUB_BRD_CD_LEN       IS NOT NULL THEN DU.SUB_BRD_CD_LEN       /*자리수체크*/ 
                        WHEN DU.MNG_CD1_LEN          IS NOT NULL THEN DU.MNG_CD1_LEN          /*자리수체크*/ 
                        WHEN DU.MNG_CD2_LEN          IS NOT NULL THEN DU.MNG_CD2_LEN          /*자리수체크*/ 
                        WHEN DU.MNG_CD3_LEN          IS NOT NULL THEN DU.MNG_CD3_LEN          /*자리수체크*/ 
                        WHEN DU.MNG_CD4_LEN          IS NOT NULL THEN DU.MNG_CD4_LEN          /*자리수체크*/ 
                        WHEN DU.MNG_CD5_LEN          IS NOT NULL THEN DU.MNG_CD5_LEN          /*자리수체크*/ 
                        WHEN DU.SEX_CD_LEN           IS NOT NULL THEN DU.SEX_CD_LEN           /*자리수체크*/ 
                        WHEN DU.PUMMOK_CD_LEN        IS NOT NULL THEN DU.PUMMOK_CD_LEN        /*자리수체크*/ 
                        WHEN DU.INPUT_PLU_CD_LEN     IS NOT NULL THEN DU.INPUT_PLU_CD_LEN     /*자리수체크*/ 
                        WHEN DU.VEN_STYLE_CD_LEN     IS NOT NULL THEN DU.VEN_STYLE_CD_LEN     /*자리수체크*/ 
                        WHEN DU.SALE_UNIT_CD_LEN     IS NOT NULL THEN DU.SALE_UNIT_CD_LEN     /*자리수체크*/ 
                        WHEN DU.BAS_SPEC_CD_LEN      IS NOT NULL THEN DU.BAS_SPEC_CD_LEN      /*자리수체크*/ 
                        WHEN DU.BAS_SPEC_UNIT_CD_LEN IS NOT NULL THEN DU.BAS_SPEC_UNIT_CD_LEN /*자리수체크*/ 
                        WHEN DU.CMP_SPEC_CD_LEN      IS NOT NULL THEN DU.CMP_SPEC_CD_LEN      /*자리수체크*/ 
                        WHEN DU.CMP_SPEC_UNIT_CD_LEN IS NOT NULL THEN DU.CMP_SPEC_UNIT_CD_LEN /*자리수체크*/ 
                        WHEN DU.ORIGIN_AREA_CD_LEN   IS NOT NULL THEN DU.ORIGIN_AREA_CD_LEN   /*자리수체크*/ 
                        WHEN DU.STYLE_NAME           IS NULL     THEN '032'                   /*스타일명*/
                        WHEN DU.RECP_NAME            IS NULL     THEN '025'                   /*영수증명*/
                        WHEN DU.BRAND_CD             IS NULL     THEN '036'                   /*브랜드코드*/
                        WHEN DU.SUB_BRD_CD           IS NULL     THEN '034'                   /*서브브랜드코드*/
                        WHEN DU.MNG_CD1              IS NULL                                  /*관리항목1코드*/
                             AND DU.MNG_CD2          IS NULL                                  /*관리항목2코드*/
                             AND DU.MNG_CD3          IS NULL                                  /*관리항목3코드*/
                             AND DU.MNG_CD4          IS NULL                                  /*관리항목4코드*/
                             AND DU.MNG_CD5          IS NULL     THEN '047'                   /*관리항목5코드*/
                                                                                              /*관리항목은 1개 이상 입력되어야함20100610적용*/
                        WHEN DU.SEX_CD               IS NULL     THEN '037'                   /*성별코드*/
                        WHEN DU.PUMMOK_CD            IS NULL     THEN '026'                   /*품목코드*/
                        WHEN DU.SALE_UNIT_CD         IS NULL     THEN '027'                   /*판매단위코드*/
                        WHEN BRAND.BRAND_NM         IS NULL     THEN '010'                   /*브랜드 검증*/
                        WHEN SUB_BRD.CODE_NAME       IS NULL     THEN '011'                   /*서브브랜드 검증*/
                        WHEN DU.MNG_CD1 IS NOT NULL 
                             AND MNG1.CODE_NAME      IS NULL     THEN '015'                   /*관리항목1 검증*/
                        WHEN DU.MNG_CD2 IS NOT NULL 
                             AND MNG2.CODE_NAME      IS NULL     THEN '016'                   /*관리항목2 검증*/
                        WHEN DU.MNG_CD3 IS NOT NULL 
                             AND MNG3.CODE_NAME      IS NULL     THEN '017'                   /*관리항목3 검증*/
                        WHEN DU.MNG_CD4 IS NOT NULL 
                             AND MNG4.CODE_NAME      IS NULL     THEN '018'                   /*관리항목4 검증*/
                        WHEN DU.MNG_CD5 IS NOT NULL 
                             AND MNG5.CODE_NAME      IS NULL     THEN '019'                   /*관리항목5 검증*/
                        WHEN SEX.CODE_NAME           IS NULL     THEN '009'                   /*성별 검증*/
                        WHEN PMK.PUMMOK_NAME         IS NULL     THEN '001'                   /*품목 검증*/
                        WHEN UNT_SALE.CODE_NAME      IS NULL     THEN '002'                   /*판매단위 검증*/
                        WHEN VSIZE(DU.STYLE_NAME)    > 40        THEN '028'                   /*스타일명 길이 검증*/
                        WHEN VSIZE(DU.RECP_NAME)     > 20        THEN '029'                   /*영수증명 길이 검증*/
                        WHEN SUBSTR(DU.SKU_CD,0,1)   = '2'       THEN '020'                   /*유효한 소스마킹코드  검증*/
                        WHEN NULLIF(
                                    SUBSTR(DU.SKU_CD,13)
                                  , DU.CHECK_SUM
                                   )                 IS NOT NULL THEN '020'                   /*유효한 단품코드  검증*/
                        WHEN CASE WHEN DU.BAS_SPEC_UNIT_CD IS NOT NULL
                                  THEN UNT_BAS.CODE_NAME
                                  ELSE 'OK'
                             END                     IS NULL     THEN '003'                   /*기본규격단위 검증*/
                        WHEN CASE WHEN DU.CMP_SPEC_UNIT_CD IS NOT NULL
                                  THEN UNT_CMP.CODE_NAME
                                  ELSE 'OK'
                             END                     IS NULL     THEN '004'                   /*구성규격단위 검증*/
                        WHEN CASE WHEN DU.ORIGIN_AREA_CD IS NOT NULL
                                  THEN ORGN.CODE_NAME
                                  ELSE 'OK'
                             END                     IS NULL     THEN '005'                   /*원산지 검증*/
                        WHEN (SELECT COUNT(*) 
                                FROM DPS.PC_SKUMST
                               WHERE SKU_CD = DU.SKU_CD
                             ) > 0                               THEN '021'                   /*단품코드 중복 검증*/
                        WHEN (SELECT COUNT(*) 
                                FROM DPS.PC_BARCDMST 
                               WHERE BARCODE = DU.SKU_CD
                                 AND SKU_CD != DU.SKU_CD
                                 AND ROWNUM  = 1
                             ) > 0                               THEN '054'                   /*소스마킹코드(바코드) 중복 검증*/
                        WHEN (SELECT COUNT(*) 
                                FROM DPS.PC_STYLEMST 
                               WHERE PUMBUN_CD  = ?
                                 AND STYLE_CD   = DU.STYLE_CD
                             ) > 0                               THEN '022'                   /*스타일코드 중복 검증*/
                        WHEN (SELECT COUNT(*) 
                                FROM UPLOAD_EXCEL_DATA
                               WHERE SKU_CD = DU.SKU_CD
                             ) > 1                               THEN '045'                   /*로드한 데이터 단품코드 중복 검증*/
                        WHEN (SELECT COUNT(*) 
                                FROM UPLOAD_EXCEL_DATA
                               WHERE STYLE_CD  = DU.STYLE_CD
                             ) > 1                               THEN '045'                   /*로드한 데이터 스타일 중복 검증*/
                        WHEN (SELECT COUNT(*) 
                                FROM UPLOAD_EXCEL_DATA
                               WHERE STYLE_CD             =  DU.STYLE_CD
                                 AND (STYLE_NAME          <> DU.STYLE_NAME
                                      OR RECP_NAME        <> DU.RECP_NAME
                                      OR SEX_CD           <> DU.SEX_CD
                                      OR PUMMOK_CD        <> DU.PUMMOK_CD
                                      OR VEN_STYLE_CD     <> DU.VEN_STYLE_CD
                                      OR SALE_UNIT_CD     <> DU.SALE_UNIT_CD
                                      OR BAS_SPEC_CD      <> DU.BAS_SPEC_CD
                                      OR BAS_SPEC_UNIT_CD <> DU.BAS_SPEC_UNIT_CD
                                      OR CMP_SPEC_CD      <> DU.CMP_SPEC_CD
                                      OR CMP_SPEC_UNIT_CD <> DU.CMP_SPEC_UNIT_CD
                                      OR ORIGIN_AREA_CD   <> DU.ORIGIN_AREA_CD
                                     )
                             ) > 0                               THEN '023'                    /*로드한 데이터 상세정보 검증*/
                        ELSE ''
                   END                              AS ERROR_CODE
              FROM UPLOAD_EXCEL_DATA DU
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P032' AND USE_YN='Y' ) SEX
                 , DPS.PC_BRDMST BRAND
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P031' AND USE_YN='Y' ) SUB_BRD
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P013' AND USE_YN='Y' ) UNT_SALE
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P013' AND USE_YN='Y' ) UNT_BAS
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P013' AND USE_YN='Y' ) UNT_CMP
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P040' AND USE_YN='Y' ) ORGN
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME, REFER_CODE FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P005' AND USE_YN='Y' ) MNG1
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME, REFER_CODE FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P006' AND USE_YN='Y' ) MNG2
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME, REFER_CODE FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P007' AND USE_YN='Y' ) MNG3
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME, REFER_CODE FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P008' AND USE_YN='Y' ) MNG4
                 , (SELECT COMM_CODE, COMM_NAME1 AS CODE_NAME, REFER_CODE FROM COM.TC_COMMCODE WHERE SYS_PART='D' AND COMM_PART='P009' AND USE_YN='Y' ) MNG5
                 , (SELECT S_PMK.PUMMOK_CD
                         , S_PMK.PUMMOK_NAME
                      FROM DPS.PC_PBNPMK S_PPK
                         , DPS.PC_PMKMST S_PMK
                     WHERE S_PPK.PUMMOK_CD = S_PMK.PUMMOK_CD
                       AND S_PPK.PUMBUN_CD = ?
                   ) PMK
             WHERE DU.PUMMOK_CD        = PMK.PUMMOK_CD(+)
               AND DU.SEX_CD           = SEX.COMM_CODE(+)
               AND DU.BRAND_CD         = BRAND.BRAND_CD(+)
               AND DU.SUB_BRD_CD       = SUB_BRD.COMM_CODE(+)
               AND DU.MNG_CD1          = MNG1.COMM_CODE(+)
               AND DU.MNG_CD2          = MNG2.COMM_CODE(+)
               AND DU.MNG_CD3          = MNG3.COMM_CODE(+)
               AND DU.MNG_CD4          = MNG4.COMM_CODE(+)
               AND DU.MNG_CD5          = MNG5.COMM_CODE(+)
               AND DU.SALE_UNIT_CD     = UNT_SALE.COMM_CODE(+)
               AND DU.BAS_SPEC_UNIT_CD = UNT_BAS.COMM_CODE(+)
               AND DU.CMP_SPEC_UNIT_CD = UNT_CMP.COMM_CODE(+)
               AND DU.ORIGIN_AREA_CD   = ORGN.COMM_CODE(+)
        ]]>
    </query>
    <query name="SEL_BSTYLE_ORDER">
        <![CDATA[
             ORDER BY ERROR_CODE, STYLE_NAME
        ]]>
    </query>    
    <!-- 단품  마스터 중복체크  -->
    <query name="SEL_SKUMST_SKU_CD_CNT">
        <![CDATA[
            SELECT COUNT(*)  AS CNT
              FROM DPS.PC_SKUMST
             WHERE SKU_CD = ?
               AND ROWNUM = 1
        ]]>
    </query>
    
    <!-- 규격 단품  마스터 신규 코드 생성  -->
    <query name="SEL_SKUMST_STN_NEW_CD">
        <![CDATA[
            SELECT '24'||LPAD(DPS.SQ_PCSKU24.NEXTVAL,10,'0') AS NEW_SKU_CD
              FROM DUAL
        ]]>
    </query>
    <!-- 신선 단품  마스터 신규 코드 생성  -->
    <query name="SEL_SKUMST_FRESH_NEW_CD">
        <![CDATA[
            SELECT '20'||LPAD(DPS.SQ_PCSKU20.NEXTVAL,5,'0')||'00000' AS NEW_SKU_CD
              FROM DUAL
        ]]>
    </query>
    <!-- 의류단품  마스터 신규 코드 생성  -->
    <query name="SEL_SKUMST_STYLE_NEW_CD">
