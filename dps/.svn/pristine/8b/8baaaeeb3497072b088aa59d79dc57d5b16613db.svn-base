				 , PRICE            --단가                         
				 , QTY              --수량                         
				 , AMT               --금액                        
				 , PRODUCT_NM      --상품 명                       
				 , ETC                 --비고                      
				 , ERR_YN                                          
				 , ERR_MSG 
				 , PROC_FLAG   
	    		 , SEQ_NO     
	    		 , 'Y' AS SAVECHEK
	    		 , 'F'  AS SEL                                    
	      FROM DPS.PS_DAYONLINE
	       WHERE   STR_CD= ?
	         AND   SALE_DT=?
        	 AND   ERR_YN LIKE ?||'%'
        	 AND   PROC_FLAG LIKE ? ||'%'
        ]]>
    </query>
    
        
</service>

