    <!--  -->
    <query name="SEL_STN_SKU">
        <![CDATA[                                        
            SELECT CASE  WHEN (SELECT COUNT(1) 
                                      FROM COM.TC_COMMCODE 
                                    WHERE COMM_PART='PS35'
                                         AND COMM_NAME2 = TRIM(?) 
                             ) = 0                               THEN '001'                   /*주문처  검증*/
                        WHEN (SELECT COUNT(1)
                                     FROM COM.TC_COMMCODE
                                   WHERE COMM_PART= 'P614'
                                       AND COMM_NAME2 = TRIM(?)
                             ) = 0                               THEN '002'                   /*브랜드 검증*/
                       WHEN (SELECT NVL2(TRIM(TRANSLATE(?,'1234567890-_',' ')),0,1)  FROM DUAL
                             ) = 0                               THEN '004'                   /*주문번호검증*/
                       WHEN (SELECT NVL(LENGTH(?),0)  FROM DUAL
                             ) = 0                               THEN '004'                   /*주문번호검증*/
                        ELSE '000'
                   END                               AS ERR_CD
             FROM DUAL
        ]]>
    </query>
    <query name="SEL_STN_SKU_ORDER">
        <![CDATA[
             ORDER BY ERR_YN, BARCODE
        ]]>
    </query>
    <query name="INS_ONLINE">
        <![CDATA[
			INSERT INTO DPS.PS_DAYONLINE  (         SALE_DT      --매출일자
                                                    , STR_CD      --점코드
                                                    , SEQ_NO       
                                                    , BARCODE          --바코드
                                                    , RECIEVER         --수취인
                                                    , PUCHASER         --구매자
                                                    , RECIEVER_HP  
                                                    , RECIEVER_HP1      --핸드폰
                                                    , RECIEVER_HP2      --핸드폰
                                                    , RECIEVER_HP3      --핸드폰
                                                    , ORDERER_CD  --주문처명으로 코드 가져오
                                                    , ORDERER_NM       --주문처명
                                                    , ORDER_NO         --주문번호
                                                    , FLOR             --층
                                                    , PUMBUN_CD        --품번코드
                                                    , PUMBUN_NAME      --품번명
                                                    , ORDER_DT         --발주일자
                                                    , STOCK_DT         --입고일
                                                    , CONF_DT          --확인일
                                                    , WAYBILL          --운송장
                                                    , RELEASE_DT       --출고일
                                                    , CS1              
                                                    , CS2              
                                                    , BOX_NO           --박스번호
                                                    , ORDER_SEQ        --주문번호
                                                    , PRODUCT_CD   --
                                                    , COLOR_NM         --색상
                                                    , SIZE_NM          --사이즈
                                                    , PRICE            --단가
                                                    , QTY              --수량
                                                    , AMT               --금액
                                                    , PRODUCT_NM      --상품 명
                                                    , ETC                 --비고
                                                    , ERR_YN
                                                    , ERR_MSG
                                                    , PROC_FLAG
                                                    , REG_DATE        --등록일시
                                                    , REG_ID             --등록자            
                                                    , MOD_DATE           --수정일시
                                                    , MOD_ID                --수정자           
                                                  ) VALUES       --38
                                                 (    TRIM(?)
                                                    , TRIM(?)
                                                    , ?
                                                    , TRIM(?)
                                                    , TRIM(?)
                                                    , TRIM(?)
                                                    , DCS.SC_Crypto_FUN('ENC', TRIM(?))
                                                    , TRIM(?)
                                                    , TRIM(?)
                                                    , TRIM(?)
                                                    , ( SELECT COMM_NAME1 FROM COM.TC_COMMCODE WHERE COMM_PART='PS35' AND SYS_PART = 'D'  AND COMM_NAME2= TRIM(?) AND USE_YN = 'Y' AND ROWNUM= 1 )
                                                    , (SELECT CASE TRIM(?) WHEN '이마트몰' THEN '이마트' WHEN '하프' THEN '하프클럽' ELSE TRIM(?) END FROM DUAL)   --20160411_온라인TF팀 정재명 부장 요청사항 (몰명 통합)
                                                    , TRIM(?)
                                                    , TRIM(?)
                                                    , (SELECT REFER_CODE   FROM COM.TC_COMMCODE 
                                                         WHERE COMM_PART='P614'  AND SYS_PART = 'D' AND COMM_NAME2=TRIM(?) AND USE_YN = 'Y'  AND ROWNUM= 1)
                                                    , TRIM(?)
                                                    , TRIM(?) 
                                                    , TRIM(?)
                                                    ,  (SELECT COMM_CODE FROM COM.TC_COMMCODE WHERE COMM_PART='P193'AND SYS_PART = 'D' AND COMM_NAME1= TRIM(?)  AND USE_YN = 'Y' AND ROWNUM= 1  )
                                                    , TRIM(?)
                                                    , TRIM(?)
                                                    , TRIM(?)
                                                    , TRIM(?)
                                                    , ( SELECT REPLACE(TRIM(?),'-','') FROM DUAL )
                                                    , NVL(?,1)
                                                    , TRIM(?)
                                                    , TRIM(?)
                                                    , TRIM(?)
                                                    , ( SELECT REPLACE(?,',','') FROM DUAL )
                                                    , ? 
                                                    , ( SELECT REPLACE(?,',','') FROM DUAL )
                                                    , TRIM(?)
                                                    , TRIM(?)
                                                    , TRIM(?)
                                                    , TRIM(?)
                                                    , TRIM(?)
                                                    , SYSDATE
                                                    , TRIM(?)
                                                    , SYSDATE
                                                    , TRIM(?)
                                                    )   
        ]]>
    </query>
       <!-- 단품  마스터 신규 입력  -->
    <query name="UPD_ONLINE">
        <![CDATA[
 			UPDATE DPS.PS_DAYONLINE  SET       BARCODE      = ?      --바코드                       
                                             , RECIEVER     = ?      --수취인                       
                                             , PUCHASER     = ?      --구매자                       
                                             , RECIEVER_HP  = DCS.SC_Crypto_FUN('ENC', ?)  
                                             , RECIEVER_HP1 = ?
                                             , RECIEVER_HP2 = ?
                                             , RECIEVER_HP3 = ?                                  
                                             , ORDERER_NM   = ?      --주문처명                     
                                             , ORDER_NO     = ?                                     
                                             , FLOR         = ?      --층                           
                                             , PUMBUN_NAME  = ?      --품번명                       
                                             , ORDER_DT     = ?      --발주일자                     
                                             , STOCK_DT     = ?      --입고일                       
                                             , CONF_DT      = ?      --확인일                       
                                             , WAYBILL      = ?      --운송장                       
                                             , RELEASE_DT   = ?      --출고일                       
                                             , CS1          = ?                                
                                             , CS2          = ?                              
                                             , BOX_NO       = ?      --박스번호                     
                                             , ORDER_SEQ    = ?      --주문번호                     
                                             , PRODUCT_CD   = ?      --품번코드                     
                                             , COLOR_NM     = ?      --색상                         
                                             , SIZE_NM      = ?      --사이즈                       
                                             , PRICE        = ?      --단가                         
                                             , QTY          = ?      --수량                         
                                             , AMT          = ?      --금액                        
                                             , PRODUCT_NM   = ?      --상품 명                       
                                             , ETC          = ?      --비고                      
                                             , ERR_YN       = ?                                          
                                             , ERR_MSG      = ?
                                             , MOD_DATE     = SYSDATE
                                             , MOD_ID       = ?
                                         WHERE STR_CD = ?
                                           AND SALE_DT= ?
                                           AND SEQ_NO = ?
                                           AND ORDERER_NM= ?
                                           AND PRODUCT_CD= ?
                                           AND PUMBUN_NAME= ?
                                           AND ORDER_SEQ = ?
                                           
        ]]>
    
    </query>
