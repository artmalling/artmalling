    <query name="SEL_SEQNO">
        <![CDATA[
	    SELECT NVL(MAX(SEQ_NO) + 1,1) AS SEQ_NO  
	      FROM DPS.PS_DAYONLINE
          WHERE SALE_DT = TO_CHAR(SYSDATE,'YYYYMMDD')
        ]]>
    </query>
    
