    <!-- 쿼리는 전부 대문자 -->
    <!-- POS 마스터 조회 쿼리  -->
    <query name="SEL_POSMST">
        <![CDATA[
            SELECT STR_CD                       , POS_NO                       , SHOP_NAME                    , EVENT_PLACE_CD               , POS_NAME                     
                 , POS_FLAG                     , FLOR_CD                       ,HALL_CD                      , MST_POS_NO                   , FNB_SHOP_CD                  
                 , ITEM_REG_TYPE                
                 , MIX_REG_YN                   , POS_IP_ADDR1                 , MAIN_SRVR_IP_ADDR            , MAIN_SRVR_PORT_NO            , BACK_SRVR_IP_ADDR            
                 , BACK_SRVR_PORT_NO            , AD_FILE_URL1                 , AD_FILE_URL2                 , UPER_MSG_ID                  , MIDL_MSG_ID                  
                 , LWER_MSG_ID                  , CASH_RECP_MSG_ID             , PHONE1_NO                    , PHONE2_NO                    , PHONE3_NO                    
                 , RSPNS_NAME                   , RSPNS_TEL_NO_1               , RSPNS_TEL_NO_2               , RSPNS_TEL_NO_3               , OPTN_01                      
                 , OPTN_02                      , OPTN_03                      , OPTN_04                      , OPTN_05                      , OPTN_06                      
                 , OPTN_07                      , OPTN_08                      , OPTN_09                      , OPTN_10                      , OPTN_11                      
                 , OPTN_12                      , OPTN_13                      , OPTN_14                      , OPTN_15                      , OPTN_16                      
                 , OPTN_17                      , OPTN_18                      , OPTN_19                      , OPTN_20                      
                 , OPTN_22                      , OPTN_23                      , OPTN_24                      , OPTN_25                      , USE_YN
                 , OPTN_26                      , OPTN_27                      , OPTN_28                      , OPTN_29                      , OPTN_30
                 , OPTN_31                      , OPTN_32                      , OPTN_33                      , OPTN_34                      , OPTN_35
                 , OPTN_36                      , OPTN_37                      , OPTN_38                      , OPTN_39                      , OPTN_40
                 , OPTN_41                      , OPTN_42                      , OPTN_43                      , OPTN_44                      , OPTN_45
                 , OPTN_46                      , OPTN_47                      , OPTN_48                      , OPTN_49                      , OPTN_50
                 , DPS.FN_GET_POS_NAME(STR_CD,MST_POS_NO)              AS MST_POS_NAME 
                 , DPS.FN_GET_FNB_SHOP_NAME(STR_CD,FNB_SHOP_CD)        AS FNB_SHOP_NAME
                 , DPS.FN_GET_EVENT_PLACE_NAME(STR_CD,EVENT_PLACE_CD)  AS EVENT_PLACE_NAME
                 , DPS.FN_GET_POS_RCP_MSG_EXPL(UPER_MSG_ID)            AS UPER_MSG_NAME
                 , DPS.FN_GET_POS_RCP_MSG_EXPL(MIDL_MSG_ID)            AS MIDL_MSG_NAME
                 , DPS.FN_GET_POS_RCP_MSG_EXPL(LWER_MSG_ID)            AS LWER_MSG_NAME
                 , DPS.FN_GET_POS_RCP_MSG_EXPL(CASH_RECP_MSG_ID)       AS CASH_RECP_MSG_NAME
                 , SUBSTR(OPTN_21,0,INSTR(OPTN_21,'-')-1)              AS OPTN_21_FROM 
                 , SUBSTR(OPTN_21,INSTR(OPTN_21,'-')+1)                AS OPTN_21_TO     
                 , REMARK                       
                 , POS_IP_ADDR2                 
                 , VEN_CD                       
                 , DPS.FN_GET_VENDOR_NAME(VEN_CD)                      AS VEN_NAME
                 
              FROM DPS.PC_POSMST 
             WHERE STR_CD    LIKE ?||'%'
        ]]>
    </query>
    <query name="SEL_POSMST_WHERE_POS_NO">
        <![CDATA[
               AND POS_NO    LIKE ?||'%'
        ]]>
    </query>
    <query name="SEL_POSMST_WHERE_POS_NAME">
        <![CDATA[
               AND POS_NAME  LIKE '%'||?||'%'
        ]]>
    </query>
    <query name="SEL_POSMST_WHERE_SHOP_NAME">
        <![CDATA[
               AND SHOP_NAME LIKE '%'||?||'%'
        ]]>
    </query>
    <query name="SEL_POSMST_ORDER">
        <![CDATA[
             ORDER BY POS_NO
        ]]>
    </query>
    <!-- POS 번호 중복검사 -->
    <query name="SEL_POSMST_POS_NO_CNT">
        <![CDATA[
            SELECT COUNT(*) AS CNT
              FROM DPS.PC_POSMST
             WHERE STR_CD  = ?
               AND POS_NO  = ?
        ]]>
    </query>
    <!-- POS 마스터 등록 -->
    <query name="INS_POSMST">
        <![CDATA[
            INSERT INTO DPS.PC_POSMST(STR_CD                       , POS_NO                       , SHOP_NAME                    , EVENT_PLACE_CD               , POS_NAME                     
                                    , POS_FLAG                     , FLOR_CD                      , HALL_CD        
                                    , MST_POS_NO                   , FNB_SHOP_CD                  , ITEM_REG_TYPE                
                                    , MIX_REG_YN                   , POS_IP_ADDR1                  , MAIN_SRVR_IP_ADDR            , MAIN_SRVR_PORT_NO            , BACK_SRVR_IP_ADDR            
                                    , BACK_SRVR_PORT_NO            , AD_FILE_URL1                 , AD_FILE_URL2                 , UPER_MSG_ID                  , MIDL_MSG_ID                  
                                    , LWER_MSG_ID                  , CASH_RECP_MSG_ID             , PHONE1_NO                    , PHONE2_NO                    , PHONE3_NO                    
                                    , RSPNS_NAME                   , RSPNS_TEL_NO_1               , RSPNS_TEL_NO_2               , RSPNS_TEL_NO_3               , OPTN_01                      
                                    , OPTN_02                      , OPTN_03                      , OPTN_04                      , OPTN_05                      , OPTN_06                      
                                    , OPTN_07                      , OPTN_08                      , OPTN_09                      , OPTN_10                      , OPTN_11                      
                                    , OPTN_12                      , OPTN_13                      , OPTN_14                      , OPTN_15                      , OPTN_16                      
                                    , OPTN_17                      , OPTN_18                      , OPTN_19                      , OPTN_20                      , OPTN_21                      
                                    , OPTN_22                      , OPTN_23                      , OPTN_24                      , OPTN_25                      , USE_YN
                                    , OPTN_26                      , OPTN_27                      , OPTN_28                      , OPTN_29                      , OPTN_30
					                , OPTN_31                      , OPTN_32                      , OPTN_33                      , OPTN_34                      , OPTN_35
					                , OPTN_36                      , OPTN_37                      , OPTN_38                      , OPTN_39                      , OPTN_40
					                , OPTN_41                      , OPTN_42                      , OPTN_43                      , OPTN_44                      , OPTN_45
					                , OPTN_46                      , OPTN_47                      , OPTN_48                      , OPTN_49                      , OPTN_50 
                                    , REG_DATE                     , REG_ID                       , MOD_DATE                     , MOD_ID                       , REMARK
                                    , POS_IP_ADDR2                 , VEN_CD
                                     )
                               VALUES(?                            , ?                            , ?                            , ?                            , ?                                                 
                                    , ?                            , ?                            , ? 
                                    , ?                            , ?                            , ?                                            
                                    , ?                            , ?                            , ?                            , ?                            , ?                                        
                                    , ?                            , ?                            , ?                            , ?                            , ?                                              
                                    , ?                            , ?                            , ?                            , ?                            , ?                                                
                                    , ?                            , ?                            , ?                            , ?                            , ?                                                  
                                    , ?                            , ?                            , ?                            , ?                            , ?                                                  
                                    , ?                            , ?                            , ?                            , ?                            , ?                                                  
                                    , ?                            , ?                            , ?                            , ?                            , ?                                                  
                                    , ?                            , ?                            , ?                            , ?                            , LPAD(?,4,'0')||'-'||LPAD(?,4,'0')                                                  
                                    , ?                            , ?                            , ?                            , ?                            , ?                             
                                    , ?                            , ?                            , ?                            , ?                            , ?
                                    , ?                            , ?                            , ?                            , ?                            , ?
                                    , ?                            , ?                            , ?                            , ?                            , ?
                                    , ?                            , ?                            , ?                            , ?                            , ?
                                    , ?                            , ?                            , ?                            , ?                            , ?
                                    , SYSDATE                      , ?                            , SYSDATE                      , ?                            , ?
                                    , ?                            , ?
                                     )
        ]]>   
    </query>
    <!-- POS 마스터 수정 -->
    <query name="UPD_POSMST">
        <![CDATA[
            UPDATE DPS.PC_POSMST
               SET SHOP_NAME         = ?        , EVENT_PLACE_CD    = ?        , POS_NAME          = ?          
                 , POS_FLAG          = ?        , FLOR_CD           = ?        , HALL_CD           = ?
                 , MST_POS_NO        = ?        , FNB_SHOP_CD       = ?        , ITEM_REG_TYPE     = ?          
                 , MIX_REG_YN        = ?        , POS_IP_ADDR1       = ?       , MAIN_SRVR_IP_ADDR = ?        , MAIN_SRVR_PORT_NO = ?        , BACK_SRVR_IP_ADDR = ?          
                 , BACK_SRVR_PORT_NO = ?        , AD_FILE_URL1      = ?        , AD_FILE_URL2      = ?        , UPER_MSG_ID       = ?        , MIDL_MSG_ID       = ?          
                 , LWER_MSG_ID       = ?        , CASH_RECP_MSG_ID  = ?        , PHONE1_NO         = ?        , PHONE2_NO         = ?        , PHONE3_NO         = ?          
                 , RSPNS_NAME        = ?        , RSPNS_TEL_NO_1    = ?        , RSPNS_TEL_NO_2    = ?        , RSPNS_TEL_NO_3    = ?        , OPTN_01           = ?          
                 , OPTN_02           = ?        , OPTN_03           = ?        , OPTN_04           = ?        , OPTN_05           = ?        , OPTN_06           = ?          
                 , OPTN_07           = ?        , OPTN_08           = ?        , OPTN_09           = ?        , OPTN_10           = ?        , OPTN_11           = ?          
                 , OPTN_12           = ?        , OPTN_13           = ?        , OPTN_14           = ?        , OPTN_15           = ?        , OPTN_16           = ?          
                 , OPTN_17           = ?        , OPTN_18           = ?        , OPTN_19           = ?        , OPTN_20           = ?        , OPTN_21           = LPAD(?,4,'0')||'-'||LPAD(?,4,'0')          
                 , OPTN_22           = ?        , OPTN_23           = ?        , OPTN_24           = ?        , OPTN_25           = ?        , USE_YN            = ?
                 , OPTN_26           = ?        , OPTN_27           = ?        , OPTN_28           = ?        , OPTN_29           = ?        , OPTN_30           = ?
                 , OPTN_31           = ?        , OPTN_32           = ?        , OPTN_33           = ?        , OPTN_34           = ?        , OPTN_35           = ?
                 , OPTN_36           = ?        , OPTN_37           = ?        , OPTN_38           = ?        , OPTN_39           = ?        , OPTN_40           = ?
                 , OPTN_41           = ?        , OPTN_42           = ?        , OPTN_43           = ?        , OPTN_44           = ?        , OPTN_45           = ?
                 , OPTN_46           = ?        , OPTN_47           = ?        , OPTN_48           = ?        , OPTN_49           = ?        , OPTN_50           = ?
                 , MOD_DATE          = SYSDATE  , MOD_ID            = ?        , REMARK            = ?        , POS_IP_ADDR2      = ?        , VEN_CD            = ?
             WHERE STR_CD            = ?
               AND POS_NO            = ?
        ]]>   
    </query>
</service>