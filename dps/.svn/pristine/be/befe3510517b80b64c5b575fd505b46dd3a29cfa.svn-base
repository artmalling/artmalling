<service name="/pstk312.pt">

    <gauce name="H_SEL_MASTER"> 
        <column colName="BIZ_TAX_FLAG" colType="TB_STRING"      size="10"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="FLAG"         colType="TB_STRING"      size="40"       dec="0" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="AMT"          colType="TB_DECIMAL"     size="13"       dec="2" keyType="TB_NORMAL" roundMode="ROUND" />
    </gauce>
   	
    <!-- 쿼리는 전부 대문자 -->
    <!-- 당초팀별월매출계획 조회 쿼리  -->
   
    <query name="SEL_MASTER">        
        <![CDATA[
 SELECT BIZ_TAX_FLAG
      , FLAG
      , AMT
  FROM (
		 SELECT '전체'				AS BIZ_TAX_FLAG
		      , '기초재고 점출액'		AS FLAG
		      , 1                   AS SEQ_NO
		      , SUM(A.CAO_SALE_AMT)	AS AMT
		   FROM DPS.PT_PBNMONPROFIT A
		      , DPS.PC_ORGMST       B
		      , DPS.PC_STRPBN       C
		  WHERE A.STR_CD    = ?
		    AND A.YYYYMM    = ?
		    AND C.STR_CD    = A.STR_CD
		    AND C.PUMBUN_CD = A.PUMBUN_CD
		    AND (CASE WHEN ? = '1' THEN C.SALE_ORG_CD ELSE C.BUY_ORG_CD        END)      = B.ORG_CD
		    AND B.DEPT_CD  LIKE ?||'%'     
		    AND B.TEAM_CD  LIKE ?||'%'     
		    AND B.PC_CD    LIKE ?||'%'     
		 UNION ALL
		SELECT '전체'					AS BIZ_TAX_FLAG
		      , '기초재고 점출차액'		AS FLAG
		      , 2						AS SEQ_NO
		      , SUM(A.CAO_PROFIT_AMT)	AS AMT
		   FROM DPS.PT_PBNMONPROFIT A
		      , DPS.PC_ORGMST       B
		      , DPS.PC_STRPBN       C
		  WHERE A.STR_CD    = ?
		    AND A.YYYYMM    = ?
		    AND C.STR_CD    = A.STR_CD
		    AND C.PUMBUN_CD = A.PUMBUN_CD
		    AND (CASE WHEN ? = '1' THEN C.SALE_ORG_CD ELSE C.BUY_ORG_CD        END)      = B.ORG_CD
		    AND B.DEPT_CD  LIKE ?||'%'     
		    AND B.TEAM_CD  LIKE ?||'%'     
		    AND B.PC_CD    LIKE ?||'%'     
		 UNION ALL
		SELECT '전체'					AS BIZ_TAX_FLAG
		      , '기초재고 점출부가세'		AS FLAG
		      , 3						AS SEQ_NO
		      , SUM(A.CAO_TAX_AMT)		AS AMT
		   FROM DPS.PT_PBNMONPROFIT A
		      , DPS.PC_ORGMST       B
		      , DPS.PC_STRPBN       C
		  WHERE A.STR_CD    = ?
		    AND A.YYYYMM    = ?
		    AND C.STR_CD    = A.STR_CD
		    AND C.PUMBUN_CD = A.PUMBUN_CD
		    AND (CASE WHEN ? = '1' THEN C.SALE_ORG_CD ELSE C.BUY_ORG_CD        END)      = B.ORG_CD
		    AND B.DEPT_CD  LIKE ?||'%'     
		    AND B.TEAM_CD  LIKE ?||'%'     
		    AND B.PC_CD    LIKE ?||'%' 
		 UNION ALL
		 SELECT '전체'						AS BIZ_TAX_FLAG
		      , '기초재고 점출액(세제외)'		AS FLAG
		      , 4							AS SEQ_NO
		      , SUM(A.CAO_EX_VAT_SALE_AMT)	AS AMT
		   FROM DPS.PT_PBNMONPROFIT A
		      , DPS.PC_ORGMST       B
		      , DPS.PC_STRPBN       C
		  WHERE A.STR_CD    = ?
		    AND A.YYYYMM    = ?
		    AND C.STR_CD    = A.STR_CD
		    AND C.PUMBUN_CD = A.PUMBUN_CD
		    AND (CASE WHEN ? = '1' THEN C.SALE_ORG_CD ELSE C.BUY_ORG_CD        END)      = B.ORG_CD
		    AND B.DEPT_CD  LIKE ?||'%'     
		    AND B.TEAM_CD  LIKE ?||'%'     
		    AND B.PC_CD    LIKE ?||'%'     
		 UNION ALL
		 SELECT '전체'							AS BIZ_TAX_FLAG
		      , '기초재고 점출차액(세제외)'		AS FLAG
		      , 5								AS SEQ_NO
		      , SUM(A.CAO_EX_VAT_PROFIT_AMT)	AS AMT
		   FROM DPS.PT_PBNMONPROFIT A
		      , DPS.PC_ORGMST       B
		      , DPS.PC_STRPBN       C
		  WHERE A.STR_CD    = ?
		    AND A.YYYYMM    = ?
		    AND C.STR_CD    = A.STR_CD
		    AND C.PUMBUN_CD = A.PUMBUN_CD
		    AND (CASE WHEN ? = '1' THEN C.SALE_ORG_CD ELSE C.BUY_ORG_CD        END)      = B.ORG_CD
		    AND B.DEPT_CD  LIKE ?||'%'     
		    AND B.TEAM_CD  LIKE ?||'%'     
		    AND B.PC_CD    LIKE ?||'%'     
		 UNION ALL
		 SELECT '전체'							AS BIZ_TAX_FLAG
		      , '매입 점출액'						AS FLAG
		      , 6								AS SEQ_NO
		      , SUM(A.BUY_SALE_AMT)				AS AMT
		   FROM DPS.PT_PBNMONPROFIT A
		      , DPS.PC_ORGMST       B
		      , DPS.PC_STRPBN       C
		  WHERE A.STR_CD    = ?
		    AND A.YYYYMM    = ?
		    AND C.STR_CD    = A.STR_CD
		    AND C.PUMBUN_CD = A.PUMBUN_CD
		    AND (CASE WHEN ? = '1' THEN C.SALE_ORG_CD ELSE C.BUY_ORG_CD        END)      = B.ORG_CD
		    AND B.DEPT_CD  LIKE ?||'%'     
		    AND B.TEAM_CD  LIKE ?||'%'     
		    AND B.PC_CD    LIKE ?||'%'     
		 UNION ALL
		 SELECT '전체'								AS BIZ_TAX_FLAG
		      , '매입 점출차액'						AS FLAG
		      , 7									AS SEQ_NO
		      , SUM(A.BUY_PROFIT_AMT)				AS AMT
		   FROM DPS.PT_PBNMONPROFIT A
		      , DPS.PC_ORGMST       B
		      , DPS.PC_STRPBN       C
		  WHERE A.STR_CD    = ?
		    AND A.YYYYMM    = ?
		    AND C.STR_CD    = A.STR_CD
		    AND C.PUMBUN_CD = A.PUMBUN_CD
		    AND (CASE WHEN ? = '1' THEN C.SALE_ORG_CD ELSE C.BUY_ORG_CD        END)      = B.ORG_CD
		    AND B.DEPT_CD  LIKE ?||'%'     
		    AND B.TEAM_CD  LIKE ?||'%'     
		    AND B.PC_CD    LIKE ?||'%'
		 UNION ALL
		 SELECT '전체'							AS BIZ_TAX_FLAG
		      , '반품 점출액'						AS FLAG
		      , 8								AS SEQ_NO
		      , SUM(A.RFD_SALE_AMT)				AS AMT
		   FROM DPS.PT_PBNMONPROFIT A
