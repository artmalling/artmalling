    <!-- OFFER SHEET 리스트 조회 -->
    <query name="SEL_LIST">
        <![CDATA[
            SELECT 
                   A.STR_CD
                 , DPS.FN_GET_STORE_NAME(A.STR_CD)                      AS STR_NM
                 , A.OFFER_YM
                 , A.OFFER_SEQ_NO
                 , A.OFFER_SHEET_NO
                 , A.PUMBUN_CD
                 , DPS.FN_GET_PUMBUN_NAME(A.PUMBUN_CD)                  AS PUMBUN_NM
                 , A.VEN_CD
                 , DPS.FN_GET_VENDOR_NAME(A.VEN_CD)                     AS VEN_NM
                 , A.OFFER_DT
                 , B.SKU_TYPE                                    /* 단품 구분 */
                 , B.STYLE_TYPE                                  /* 의류단품 구분*/
                 , B.BIZ_TYPE                                    /* 거래형태 */
                 , TO_CHAR(A.REG_DATE, 'YYYYMMDD')                      AS REG_DATE
                 , C.OFFER_SEQ_NO                                       AS CHECKVALUE
              FROM DPS.PO_OFFMST A
                 , DPS.PC_STRPBN B
                 , (
                    SELECT A.STR_CD
                         , A.OFFER_YM
                         , A.OFFER_SEQ_NO
                         , A.OFFER_SHEET_NO
                      FROM DPS.PO_OFFMST A
                         , DPS.PO_INVMST B
                     WHERE A.STR_CD = ?
                       AND A.OFFER_DT      BETWEEN ? AND ?
                       AND A.OFFER_SHEET_NO LIKE ? || '%'
                       AND A.PUMBUN_CD      LIKE ? || '%'
                       AND A.VEN_CD         LIKE ? || '%'
                       AND A.STR_CD = B.STR_CD
                       AND A.OFFER_YM = B.OFFER_YM
                       AND A.OFFER_SEQ_NO = B.OFFER_SEQ_NO
                   ) C
             WHERE A.STR_CD        = B.STR_CD
               AND A.STR_CD        = ?
               AND A.STR_CD        = C.STR_CD(+)
               AND A.OFFER_YM      = C.OFFER_YM(+)
               AND A.OFFER_SEQ_NO  = C.OFFER_SEQ_NO(+)
               AND A.OFFER_DT      BETWEEN ? AND ?
               AND A.PUMBUN_CD     = B.PUMBUN_CD
               AND A.OFFER_SHEET_NO  LIKE ? || '%'
               AND A.PUMBUN_CD       LIKE ? || '%'
               AND A.VEN_CD          LIKE ? || '%'
               AND EXISTS ( SELECT MYORG.USER_ID
                                FROM COM.V_MYORG MYORG
                               WHERE MYORG.STR_CD      = A.STR_CD
                                 AND MYORG.PUMBUN_CD   = A.PUMBUN_CD
                                 AND MYORG.USER_ID     = ?
                                 AND ROWNUM            = 1
                                 AND MYORG.ORG_FLAG    = ?
                          )
             ORDER BY A.STR_CD DESC, A.OFFER_DT DESC, A.OFFER_SEQ_NO DESC
        ]]>
    </query> 

    <!-- OFFER SHEET 리스트 조회 -->
    <query name="SEL_LIST_ODD_OFFER_NO">
        <![CDATA[
            SELECT 
                   A.STR_CD
                 , DPS.FN_GET_STORE_NAME(A.STR_CD)                      AS STR_NM
                 , A.OFFER_YM
                 , A.OFFER_SEQ_NO
                 , A.OFFER_SHEET_NO
                 , A.PUMBUN_CD
                 , DPS.FN_GET_PUMBUN_NAME(A.PUMBUN_CD)                  AS PUMBUN_NM
                 , A.VEN_CD
                 , DPS.FN_GET_VENDOR_NAME(A.VEN_CD)                     AS VEN_NM
                 , A.OFFER_DT
                 , B.SKU_TYPE                                    /* 단품 구분 */
                 , B.STYLE_TYPE                                  /* 의류단품 구분*/
                 , B.BIZ_TYPE                                    /* 거래형태 */
                 , TO_CHAR(A.REG_DATE, 'YYYYMMDD')                      AS REG_DATE
                 , C.OFFER_SEQ_NO                                       AS CHECKVALUE
              FROM DPS.PO_OFFMST A
                 , DPS.PC_STRPBN B
                 , (
                    SELECT A.STR_CD
                         , A.OFFER_YM
                         , A.OFFER_SEQ_NO
                         , A.OFFER_SHEET_NO
                      FROM DPS.PO_OFFMST A
                         , DPS.PO_INVMST B
                     WHERE A.STR_CD = ?
                       AND A.OFFER_DT      BETWEEN ? AND ?
                       AND A.PUMBUN_CD      LIKE ? || '%'
                       AND A.VEN_CD         LIKE ? || '%'
                       AND A.STR_CD = B.STR_CD
                       AND A.OFFER_YM = B.OFFER_YM
                       AND A.OFFER_SEQ_NO = B.OFFER_SEQ_NO
                   ) C
             WHERE A.STR_CD        = B.STR_CD
               AND A.STR_CD        = ?
               AND A.STR_CD        = C.STR_CD(+)
               AND A.OFFER_YM      = C.OFFER_YM(+)
               AND A.OFFER_SEQ_NO  = C.OFFER_SEQ_NO(+)
               AND A.OFFER_DT      BETWEEN ? AND ?
               AND A.PUMBUN_CD     = B.PUMBUN_CD
               AND A.PUMBUN_CD     LIKE ? || '%'
               AND A.VEN_CD        LIKE ? || '%'
               AND EXISTS ( SELECT MYORG.USER_ID
                                FROM COM.V_MYORG MYORG
                               WHERE MYORG.STR_CD      = A.STR_CD
                                 AND MYORG.PUMBUN_CD   = A.PUMBUN_CD
                                 AND MYORG.USER_ID     = ?
                                 AND ROWNUM            = 1
                                 AND MYORG.ORG_FLAG    = ?
                          )
             ORDER BY A.STR_CD DESC, A.OFFER_DT DESC, A.OFFER_SEQ_NO DESC
        ]]>
    </query> 

    <!-- OFFER SHEET 마스터 조회 -->
    <query name="SEL_MASTER">
        <![CDATA[
            SELECT 
                   A.STR_CD
                 , DPS.FN_GET_STORE_NAME(A.STR_CD)                      AS STR_NM
                 , A.OFFER_YM
                 , A.OFFER_SEQ_NO
                 , A.OFFER_SHEET_NO
                 , A.PUMBUN_CD
                 , DPS.FN_GET_PUMBUN_NAME(A.PUMBUN_CD) AS PUMBUN_NM
                 , A.VEN_CD
                 , DPS.FN_GET_VENDOR_NAME(A.VEN_CD) AS VEN_NM
                 , A.PRC_COND
                 , A.SHIPPMENT_CD
                 , A.CURRENCY_CD
                 , A.OFFER_TOT_QTY
                 , A.OFFER_TOT_AMT
                 , A.PAYMENT_COND
                 , A.PAYMENT_DTL_COND
                 , A.SHIP_PORT
                 , A.ARRI_PORT
                 , A.LC_DATE
                 , A.LC_NO
                 , A.LC_EFFECTIVE_DT
                 , A.LC_OPEN_BANK
                 , A.VENDOR_INFO
                 , A.OFFER_DT
                 , A.IMPORT_COUNTRY
                 , A.ATTN
                 , A.MESSRS
                 , A.VALIDITY
                 , A.SHIPPMENT
                 , A.PACKING
                 , A.INSURANCE
                 , A.PRICE
                 , A.OFFER_TOT_AMT                               AS PRICE2
                 , A.BUYER_CD
                 , DPS.FN_GET_BUYER_NAME(A.BUYER_CD)             AS BUYER_NM
                 , A.ORIGIN
                 , A.INSPECTION
                 , A.DELIVERY
                 , A.BL_NO
                 , A.BL_DT
                 , A.COMMODITY
                 , A.PACKING_CHARGE
                 , A.NCV
                 , A.SHIPPER
                 , A.EXC_APP_DT
                 , A.EXC_RATE
                 , A.CLOSE_FLAG                 
                 , TO_CHAR(A.REG_DATE, 'YYYYMMDD')               AS REG_DATE
                 , A.CLOSE_DT
                 , A.EXPNC_CLOSE_FLAG
                 , A.COST_CLOSE_FLAG
                 , A.PAYMENT
                 , B.SKU_TYPE                                    /* 단품 구분 */
                 , B.STYLE_TYPE                                  /* 의류단품 구분*/
                 , B.BIZ_TYPE                                    /* 거래형태 */
              FROM DPS.PO_OFFMST A
                 , DPS.PC_STRPBN B
             WHERE A.STR_CD = B.STR_CD
               AND A.PUMBUN_CD = B.PUMBUN_CD
               AND A.STR_CD       = ?
               AND A.OFFER_YM     = ?
               AND A.OFFER_SEQ_NO = ?
        ]]>
    </query> 

    <!-- OFFER SHEET 디테일 조회 -->
    <query name="SEL_DETAIL">
        <![CDATA[           
            SELECT ''                          AS CHECK1  
                 , G.STR_CD                    AS STR_CD
                 , G.OFFER_YM                  AS OFFER_YM
                 , G.OFFER_SEQ_NO              AS OFFER_SEQ_NO
                 , G.OFFER_DTL_SEQ_NO
                 , G.SKU_CD                    AS SKU_CD
                 , H.SKU_NAME                  AS SKU_NM  
                 , G.ORD_UNIT_CD        
                 , I.MODEL_NO           
                 , I.STYLE_CD
                 , I.COLOR_CD
                 , I.SIZE_CD
                 , G.ORDER_SHEET_NO            AS ORDER_NO
                 , G.OFFER_QTY                 AS OFFER_QTY
                 , F.INVOICE_QTY               AS INVOICE_QTY
                 , F.CHK_QTY                   AS CHK_QTY
                 , G.OFFER_QTY - F.CHK_QTY     AS NOT_IN_QTY
                 , G.OFFER_UNIT_AMT      
                 , G.OFFER_AMT           
                 , G.ORDER_YM       
                 , G.ORDER_SEQ_NO     
              FROM DPS.PO_OFFDTL G
                 , DPS.PC_STRSKUMST H
                 , DPS.PC_SKUMST I
                 , (SELECT D.STR_CD                     AS STR_CD
                         , D.OFFER_YM                   AS OFFER_YM
                         , D.OFFER_SEQ_NO               AS OFFER_SEQ_NO
                         , E.SKU_CD                     AS SKU_CD
                         , SUM(NVL(E.INVOICE_QTY,0))    AS INVOICE_QTY
                         , SUM(NVL(C.CHK_QTY,0))        AS CHK_QTY
                      FROM DPS.PO_INVMST D
                         , DPS.PO_INVDTL E
                         , (SELECT A.STR_CD               AS STR_CD
                                 , A.INVOICE_YM           AS INVOICE_YM
                                 , A.INVOICE_SEQ_NO       AS INVOICE_SEQ_NO
                                 , B.SKU_CD               AS SKU_CD
                                 , SUM(NVL(B.CHK_QTY,0))  AS CHK_QTY
                              FROM DPS.PO_SLPMST A
                                 , DPS.PO_SLPDTL B
                             WHERE A.STR_CD         = ?
                               AND A.SLIP_PROC_STAT = '09'
                               AND B.STR_CD         = A.STR_CD
                               AND B.SLIP_NO        = A.SLIP_NO
                             GROUP BY A.STR_CD
                                    , A.INVOICE_YM
                                    , A.INVOICE_SEQ_NO
                                    , B.SKU_CD
                           ) C 
                     WHERE D.STR_CD             = ?
                       AND E.STR_CD             = D.STR_CD
                       AND E.INVOICE_YM         = D.INVOICE_YM
                       AND E.INVOICE_SEQ_NO     = D.INVOICE_SEQ_NO
                       AND C.STR_CD         (+) = E.STR_CD
