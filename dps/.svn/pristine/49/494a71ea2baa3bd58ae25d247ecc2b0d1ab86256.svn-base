        <column colName="OFFER_UNIT_AMT"      colType="TB_DECIMAL" size="11"   dec="2" keyType="TB_NORMAL" roundMode="ROUND" />
        <column colName="OFFER_AMT"           colType="TB_DECIMAL" size="15"   dec="2" keyType="TB_NORMAL" roundMode="ROUND" />
