<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
   

    <display-name>MARIO OUTLET</display-name>
  
  <servlet>
		<servlet-name>WebController</servlet-name>
		<servlet-class>
			com.shift.framework.servlet.WebController
		</servlet-class>
		<init-param>
			<param-name>config</param-name>
			<param-value>action-config.xml</param-value>
		</init-param>
		<init-param>
			<param-name>config/common</param-name>
			<param-value>common/action-config.xml</param-value>
		</init-param>
	</servlet>
 <servlet>
  	<servlet-name>FilterDataLoader</servlet-name>
		<servlet-class>
			com.gauce.servlet.FilterDataLoader
		</servlet-class>
		<init-param>
			<param-name>charset</param-name>
			<param-value>euc-kr</param-value>
		</init-param>
  </servlet>
 <servlet>
		<servlet-name>ServiceManager</servlet-name>
		<servlet-class>
			com.shift.utils.servlet.ServiceManager
		</servlet-class>
		<init-param>
			<param-name>charset</param-name>
			<param-value>euc-kr</param-value>
		</init-param>
	</servlet>
  <servlet>
  	<servlet-name>FilterChecker</servlet-name>
		<servlet-class>
			com.gauce.servlet.FilterChecker
		</servlet-class>
  </servlet>
  <servlet>
        <servlet-name>StartUpServlet</servlet-name>
        <servlet-class>kr.fujitsu.ffw.base.StartUpServlet</servlet-class>
        <init-param>
            <param-name>relative-path</param-name>
            <param-value>true</param-value>
        </init-param>
        <init-param>
            <param-name>global.config</param-name>
            <param-value>WEB-INF/global.config.xml</param-value>
        </init-param>
        <load-on-startup>1</load-on-startup>
    </servlet>

   <servlet>
        <servlet-name>PCServlet</servlet-name>
        <servlet-class>common.web.ExtendedActionServlet</servlet-class>
        <init-param>
            <param-name>relative-path</param-name>
            <param-value>true</param-value>
        </init-param>
        <init-param>
            <param-name>config</param-name>
            <param-value>WEB-INF/xml/pcod_config.xml</param-value>
        </init-param>
        <init-param>
            <param-name>container-name</param-name>
            <param-value>engine1</param-value>
        </init-param>
        <init-param>
            <param-name>encoding</param-name>
            <param-value>UTF-8</param-value>
        </init-param>
        <init-param>
            <param-name>process-debug</param-name>
            <param-value>true</param-value>
        </init-param>
    </servlet>

   <servlet>
        <servlet-name>POServlet</servlet-name>
        <servlet-class>common.web.ExtendedActionServlet</servlet-class>
        <init-param>
            <param-name>relative-path</param-name>
            <param-value>true</param-value>
        </init-param>
        <init-param>
            <param-name>config</param-name>
            <param-value>WEB-INF/xml/pord_config.xml</param-value>
        </init-param>
        <init-param>
            <param-name>container-name</param-name>
            <param-value>engine1</param-value>
        </init-param>
        <init-param>
            <param-name>encoding</param-name>
            <param-value>UTF-8</param-value>
        </init-param>
        <init-param>
            <param-name>process-debug</param-name>
            <param-value>true</param-value>
        </init-param>
    </servlet>

   <servlet>
        <servlet-name>PPServlet</servlet-name>
        <servlet-class>common.web.ExtendedActionServlet</servlet-class>
        <init-param>
            <param-name>relative-path</param-name>
            <param-value>true</param-value>
        </init-param>
        <init-param>
            <param-name>config</param-name>
            <param-value>WEB-INF/xml/ppay_config.xml</param-value>
        </init-param>
        <init-param>
            <param-name>container-name</param-name>
            <param-value>engine1</param-value>
        </init-param>
        <init-param>
            <param-name>encoding</param-name>
            <param-value>UTF-8</param-value>
        </init-param>
        <init-param>
            <param-name>process-debug</param-name>
            <param-value>true</param-value>
        </init-param>
    </servlet>

   <servlet>
        <servlet-name>PSServlet</servlet-name>
        <servlet-class>common.web.ExtendedActionServlet</servlet-class>
        <init-param>
            <param-name>relative-path</param-name>
            <param-value>true</param-value>
        </init-param>
        <init-param>
            <param-name>config</param-name>
            <param-value>WEB-INF/xml/psal_config.xml</param-value>
        </init-param>
        <init-param>
            <param-name>container-name</param-name>
            <param-value>engine1</param-value>
        </init-param>
        <init-param>
            <param-name>encoding</param-name>
            <param-value>UTF-8</param-value>
        </init-param>
        <init-param>
            <param-name>process-debug</param-name>
            <param-value>true</param-value>
        </init-param>
    </servlet>

   <servlet>
        <servlet-name>PTServlet</servlet-name>
        <servlet-class>common.web.ExtendedActionServlet</servlet-class>
        <init-param>
            <param-name>relative-path</param-name>
            <param-value>true</param-value>
        </init-param>
        <init-param>
            <param-name>config</param-name>
            <param-value>WEB-INF/xml/pstk_config.xml</param-value>
        </init-param>
        <init-param>
            <param-name>container-name</param-name>
            <param-value>engine1</param-value>
        </init-param>
        <init-param>
            <param-name>encoding</param-name>
            <param-value>UTF-8</param-value>
        </init-param>
        <init-param>
            <param-name>process-debug</param-name>
            <param-value>true</param-value>
        </init-param>
    </servlet>
  
    <servlet>
        <description>
        </description>
        <display-name>SfxWebResourceServlet</display-name>
        <servlet-name>ChartWebResourceServlet</servlet-name>
        <servlet-class>com.softwarefx.chartfx.server.servlets.ChartWebResourceServlet</servlet-class>
    </servlet>
    <servlet>
        <description>
        </description>
        <display-name>ChartCallbackServlet</display-name>
        <servlet-name>ChartCallbackServlet</servlet-name>
        <servlet-class>com.softwarefx.chartfx.server.servlets.ChartCallbackServlet</servlet-class>
    </servlet>
  
  <servlet-mapping>
		<servlet-name>WebController</servlet-name>
		<url-pattern>*.bf</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
		<servlet-name>FilterDataLoader</servlet-name>
		<url-pattern>/monitor/FilterDataLoader</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
		<servlet-name>ServiceManager</servlet-name>
		<url-pattern>/ServiceManager</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
		<servlet-name>FilterChecker</servlet-name>
		<url-pattern>/monitor/FilterChecker</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
        <servlet-name>ChartWebResourceServlet</servlet-name>
        <url-pattern>/SfxWebResourceServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChartCallbackServlet</servlet-name>
        <url-pattern>/ChartCallbackServlet</url-pattern>
    </servlet-mapping>
  <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
  <servlet-mapping>
        <servlet-name>StartUpServlet</servlet-name>
        <url-pattern>*.start</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>PCServlet</servlet-name>
        <url-pattern>*.pc</url-pattern>
    </servlet-mapping>
    

    <servlet-mapping>
        <servlet-name>POServlet</servlet-name>
        <url-pattern>*.po</url-pattern>
    </servlet-mapping>
    

    <servlet-mapping>
        <servlet-name>PPServlet</servlet-name>
        <url-pattern>*.pp</url-pattern>
    </servlet-mapping>
    

    <servlet-mapping>
        <servlet-name>PSServlet</servlet-name>
        <url-pattern>*.ps</url-pattern>
    </servlet-mapping>
    

    <servlet-mapping>
        <servlet-name>PTServlet</servlet-name>
        <url-pattern>*.pt</url-pattern>
    </servlet-mapping>
  
  <filter>
		<filter-name>GauceFilter</filter-name>
		<filter-class>com.gauce.filter.GauceFilter</filter-class>
		<init-param>
			<param-name>conf-reload</param-name>
			<param-value>5000</param-value>
		</init-param>
	</filter>
  
  <filter-mapping>
		<filter-name>GauceFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
  
  <mime-mapping>
		<extension>gfm</extension>
		<mime-type>application/x-gforms-xml</mime-type>
	</mime-mapping>
  </web-app>