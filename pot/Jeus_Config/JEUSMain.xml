<?xml version="1.0" encoding="UTF-8"?><jeus-system xmlns="http://www.tmaxsoft.com/xml/ns/jeus" version="6.0">
   <node>
      <name>worldstarh</name>
      <engine-container>
         <name>container1</name>
         <id>104</id>
         <base-port>10791</base-port>
         <command-option>-Xms256m -Xmx512m -XX:MaxPermSize=128m</command-option>
         <engine-command>
            <type>servlet</type>
            <name>engine1</name>
         </engine-command>
         <sequential-start>true</sequential-start>
      </engine-container>
      <class-ftp>true</class-ftp>
      <sequential-start>true</sequential-start>
      <enable-webadmin>true</enable-webadmin>
      <session-server>
		  <type>primary</type>
		  <resolution>60000</resolution>
		  <connect-timeout>5000</connect-timeout>
		  <read-timeout>20000</read-timeout>
		  <passivation-to>-1</passivation-to>
		  <removal-to>1800000</removal-to>
		  <check-to>30000</check-to>
	  </session-server>
      <system-logging>
         <level>INFO</level>
         <handler>
            <console-handler>
               <name>
               </name>
            </console-handler>
            <file-handler>
               <name>fileHandler</name>
               <enable-rotation>true</enable-rotation>
               <valid-day>1</valid-day>
               <append>false</append>
            </file-handler>
         </handler>
      </system-logging>
   </node>
   <resource>
      <data-source>
         <database>
            <vendor>oracle</vendor>
            <data-source-id>pot</data-source-id>
            <export-name>pot</export-name>
            <data-source-class-name>oracle.jdbc.pool.OracleConnectionPoolDataSource</data-source-class-name>
            <data-source-type>ConnectionPoolDataSource</data-source-type>
            <database-name>HGP</database-name>
            <port-number>1521</port-number>
            <server-name>222.106.70.40</server-name>
            <user>dis</user>
            <password>dis</password>
            <auto-commit>false</auto-commit>
            <property>
               <name>driverType</name>
               <type>java.lang.String</type>
               <value>thin</value>
            </property>
            <connection-pool>
               <pooling>
                  <min>2</min>
                  <max>5</max>
                  <step>1</step>
                  <period>3600000</period>
               </pooling>
               <check-query>select * from dual</check-query>
            </connection-pool>
         </database>
      </data-source>
   </resource>
   
   
